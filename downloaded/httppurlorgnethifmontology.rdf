  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/hifm/ontology#refPriceWithVAT">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty" />
    <rdfs:domain rdf:resource="http://purl.org/net/hifm/ontology#Drug" />
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float" />
    <rdfs:comment>This property represents the referent value of the medicine in macedonian denars (MKD), with VAT calculated</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/hifm/ontology#strength">
