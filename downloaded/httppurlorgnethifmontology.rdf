    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty" />
    <rdfs:domain rdf:resource="http://purl.org/net/hifm/ontology#Drug" />
    <rdfs:range rdf:resource="http://purl.org/net/hifm/ontology#Drug" />
    <rdfs:comment>This property represents a similarity that is known between two drugs.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/hifm/ontology#Drug">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
    <rdfs:label>Drug</rdfs:label>
    <rdfs:comment>This class represents a brand-name drug.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/hifm/ontology#dosageForm">
