&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;tl:duration "P10Y"^^xsd:duration ;
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;dcterms:modified "2010-01-10T03:15:00+08:00"^^xsd:dateTime
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;] ;
&nbsp;&nbsp;&nbsp;cco:longest_duration [ 
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;a tl:UTInterval ;
