							<a href="http://purl.org/vocommons/voaf#reliesOn">voaf:reliesOn</a>
						</td>
					</tr>
					<tr>
						<td>Associated Query:</td>
						<td>
							<div style="font-family:courier new; margin-top:10px; margin-bottom:5px; padding:5px; border:1px solid #B5B5B5;">
								PREFIX rdf:&lt;http://www.w3.org/1999/02/22-rdf-syntax-ns#&gt;<br>
								PREFIX rdfs:&lt;http://www.w3.org/2000/01/rdf-schema#&gt;<br>
								PREFIX owl:&lt;http://www.w3.org/2002/07/owl#&gt;<br>
								PREFIX voaf:&lt;http://purl.org/vocommons/voaf#&gt;<br>
								PREFIX skos:&lt;http://www.w3.org/2004/02/skos/core#&gt;<br>
								CONSTRUCT{<br>
								&emsp;?vocab1 voaf:generalizes ?vocab2<br>
								}<br>
								WHERE{<br>
								&emsp;{?elem1 skos:narrowMatch ?elem2. FILTER(!isBlank(?elem2))}<br>
								&emsp;UNION{?elem2 rdfs:subPropertyOf ?elem1.}<br>
								&emsp;UNION{?elem1 a owl:Class. ?elem1 owl:unionOf ?union. ?union rdf:first ?elem2.}<br>
								&emsp;UNION{?elem1 a owl:Class. ?elem1 owl:unionOf ?union. ?union rdf:rest ?union2. ?union2 rdf:first ?elem2.}<br>
								&emsp;UNION{?elem1 a owl:Class. ?elem1 owl:unionOf ?union. ?union rdf:rest ?union2. ?union2 rdf:rest ?union3. ?union3 rdf:first ?elem2.}<br>
								&emsp;UNION{?elem1 a owl:Class. ?elem1 owl:unionOf ?union. ?union rdf:rest ?union2. ?union2 rdf:rest ?union3. ?union3 rdf:rest ?union4. ?union4 rdf:first ?elem2.}<br>
								&emsp;UNION{?elem1 a owl:Class. ?elem1 owl:unionOf ?union. ?union rdf:rest ?union2. ?union2 rdf:rest ?union3. ?union3 rdf:rest ?union4. ?union4 rdf:rest ?union5. ?union5 rdf:first ?elem2.}<br>
								&emsp;FILTER(!isBlank(?elem2))<br>
								&emsp;?elem1 rdfs:isDefinedBy ?vocab1.<br>
								&emsp;?vocab1 a voaf:Vocabulary.<br>
								&emsp;?elem2 rdfs:isDefinedBy ?vocab2.<br>
								&emsp;?vocab2 a voaf:Vocabulary.<br>
								&emsp;FILTER(?vocab1!=?vocab2)<br>
								}
