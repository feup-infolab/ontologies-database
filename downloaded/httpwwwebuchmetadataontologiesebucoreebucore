    <rdfs:comment xml:lang="en">To describe a Person.</rdfs:comment>
    <owl:equivalentClass rdf:resource="http://www.bbc.co.uk/ontologies/coreconcepts/Person"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Service">
