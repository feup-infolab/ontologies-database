  <rdf:Property rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#hasRightsContact">
    <rdfs:domain rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Rights"/>
