  <rdf:Property rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#hasDubbedLanguage">
    <rdfs:label xml:lang="en">Dubbed language</rdfs:label>
    <rdfs:subPropertyOf>
      <rdf:Property rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#hasLanguage">
        <rdfs:domain>
          <owl:Class rdf:nodeID="Nd2e2f24775ac4bafb69bc8a0ac2cbf43">
            <owl:unionOf rdf:parseType="Collection">
              <rdf:Description rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#BusinessObject"/>
              <rdf:Description rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Resource"/>
            </owl:unionOf>
          </owl:Class>
        </rdfs:domain>
        <rdfs:comment xml:lang="en">To associate a Language to a Resource or
            BusinessObject. A controlled vocabulary based on BCP 47 is recommended. This
            property can also be used to identify the presence of sign language (RFC 5646). By
            inheritance, the hasLanguage property applies indifferently at the MediaResource /
            Fragment / Track levels at which the usage is being defined. Best practice recommends to
            use to best possible level of granularity fo describe the usage of language within a
            MediaResource including at Fragment and Track levels.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/language"/>
        <owl:equivalentProperty rdf:resource="http://www.w3.org/ns/ma-ont#hasLanguage"/>
        <rdfs:label xml:lang="en">Language</rdfs:label>
      </rdf:Property>
    </rdfs:subPropertyOf>
  </rdf:Property>
