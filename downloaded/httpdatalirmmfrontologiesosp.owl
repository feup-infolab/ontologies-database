    <foaf:homepage rdf:resource="http://www.scharffe.fr"/>
  </foaf:Person>

  <dcterms:Agent rdf:about="http://data.lirmm.fr/ontologies/osp#LIRMM">
    <foaf:member rdf:resource="http://data.lirmm.fr/ontologies/osp#fran%C3%A7ois"/>
    <foaf:name>LIRMM</foaf:name>
    <foaf:homepage rdf:resource="http://www.lirmm.fr"/>
  </dcterms:Agent>

  <foaf:Person rdf:about="http://data.lirmm.fr/ontologies/osp#felix">
    <foaf:name>Felix Averlant</foaf:name>
    <foaf:homepage></foaf:homepage>
  </foaf:Person>

  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/osp#Tribunal">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>tribunal</rdfs:label>
    <rdfs:comment>Le tribunal est un bâtiment public où la justice y est rendu par des juges.</rdfs:comment>
    <owl:disjointWith rdf:resource="http://data.lirmm.fr/ontologies/osp#Ambassade"/>
    <owl:disjointWith rdf:resource="http://data.lirmm.fr/ontologies/osp#Mairie"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/osp#OrganisationEtatique">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Organisation Etatique</rdfs:label>
    <rdfs:comment>La classe stateservice a pour but de différencier un Etat ou une organisation d'Etat d'une entreprise privée.</rdfs:comment>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/osp#SecretairedEtat">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Secretaire d'Etat</rdfs:label>
    <rdfs:comment>Il existe bien un secretary of state dans les autres pays mais n'a rien à voir avec la qualification de secretaire d'Etat en France</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/osp#OrganisationPublique">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Organisation publique</rdfs:label>
    <rdfs:comment>Regroupe les structure qui ne correspondent pas à un niveau étatique mais inter étatique ou extra étatique (ex : les ONG)</rdfs:comment>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/osp#Ministre">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Ministre</rdfs:label>
    <rdfs:comment>Classe décrivant une personne ayant un rôle de ministre dans un gouvernement.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/osp#PouvoirLegislatif">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Pouvoir législatif</rdfs:label>
    <rdfs:comment>Définie la fonction législative d'un pays</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://data.lirmm.fr/ontologies/osp#OrganisationEtatique"/>
    <owl:disjointWith rdf:resource="http://data.lirmm.fr/ontologies/osp#PouvoirExecutif"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/osp#PouvoirJudiciaire">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Pouvoir judiciaire</rdfs:label>
    <rdfs:comment>Branche judiciaire d'une organisation étatique</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://data.lirmm.fr/ontologies/osp#OrganisationEtatique"/>
    <owl:disjointWith rdf:resource="http://data.lirmm.fr/ontologies/osp#PouvoirExecutif"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/osp#Juge">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Juge</rdfs:label>
    <rdfs:comment>La classe Juge représente la fonction de juge au sein d'un tribunal</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/osp#ChefDEtat">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>chef d'Etat</rdfs:label>
    <rdfs:comment>Classe décrivant le chef d'Etat. La réalité de son pouvoir n'est pas le critère retenu mais ce qui apparait dans les textes. Ex : La reine d'Angleterre.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/osp#ChefDuGouvernement">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Chef du gouvernement</rdfs:label>
    <rdfs:comment>Classe décrivant un chef de gouvernement</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/osp#Gouvernement">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Gouvernment</rdfs:label>
    <rdfs:comment>Le gouvernement est un des organes issus de la séparation des pouvoirs</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://data.lirmm.fr/ontologies/osp#PouvoirExecutif"/>
    <owl:disjointWith rdf:resource="http://data.lirmm.fr/ontologies/osp#Administration"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/osp#PouvoirExecutif">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>pouvoir exécutif</rdfs:label>
    <rdfs:comment>Le pouvoir exécutif a pour charge d'exécuter les décisions de justice issues du pouvoir judiciaire et d'appliquer les lois votées par le pouvoir législatif.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://data.lirmm.fr/ontologies/osp#OrganisationEtatique"/>
    <owl:disjointWith rdf:resource="http://data.lirmm.fr/ontologies/osp#PouvoirJudiciaire"/>
    <owl:disjointWith rdf:resource="http://data.lirmm.fr/ontologies/osp#PouvoirLegislatif"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/osp#Mairie">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Mairie</rdfs:label>
    <rdfs:comment>Bâtiment officiel de l'Etat français où réside le maire et le conseil municipal. Il fait partit de l'administration décentralisée.</rdfs:comment>
    <owl:disjointWith rdf:resource="http://data.lirmm.fr/ontologies/osp#Tribunal"/>
    <owl:disjointWith rdf:resource="http://data.lirmm.fr/ontologies/osp#Ambassade"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/osp#Ambassade">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Ambassade</rdfs:label>
    <owl:disjointWith rdf:resource="http://data.lirmm.fr/ontologies/osp#Mairie"/>
    <owl:disjointWith rdf:resource="http://data.lirmm.fr/ontologies/osp#Tribunal"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/osp#Administration">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Administration</rdfs:label>
    <rdfs:comment>L'administration est dans le prolongement de l'administration. Elle est chargée de régler les affaires courantes</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://data.lirmm.fr/ontologies/osp#PouvoirExecutif"/>
    <owl:disjointWith rdf:resource="http://data.lirmm.fr/ontologies/osp#Gouvernement"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/osp#Adresse">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Classe décrivant une adresse postale</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </rdfs:Class>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#sousLAutoriteDe">
    <rdfs:label>Sous l'autorite de</rdfs:label>
    <rdfs:comment>propriété instaurant la hiérarchie entre un organisme et un service. Ne pas confondre avec des sous ensemble d'un même service</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#regionDe">
    <rdfs:label>regionDe</rdfs:label>
    <rdfs:comment>Propriété liant une région à quelque chose</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#competenceJuge">
    <rdfs:label>compétence d'un juge</rdfs:label>
    <rdfs:comment>Propriété liant un juge à sa fonction au sein du tribunal.</rdfs:comment>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#initialesDe">
    <rdfs:label>Initiales de</rdfs:label>
    <rdfs:comment>Propriété liant quelque chose à ses initiales.</rdfs:comment>
    <rdfs:range rdf:resource="http://data.lirmm.fr/ontologies/osp#OrganisationEtatique"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#chefDEtat">
    <rdfs:label>est Chef d'Etat</rdfs:label>
    <rdfs:comment>Propriété liant une personne au rôle de chef de l'Etat.</rdfs:comment>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#chefDuGouvernement">
    <rdfs:label>chef du gouvernement</rdfs:label>
    <rdfs:comment>Une personne à la tête du gouvernement</rdfs:comment>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#initials">
    <rdfs:label>initials</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#typeTribunal">
    <rdfs:label>type du tribunal</rdfs:label>
    <rdfs:comment>Propriété définissant quel type de tribunal il s'agit. Ex : tribunal d'instance, TGI ...</rdfs:comment>
    <rdfs:domain rdf:resource="http://data.lirmm.fr/ontologies/osp#Tribunal"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#aSection">
    <rdfs:label>a section</rdfs:label>
    <rdfs:comment>Les chambres d'un tribunal sont décomposées en section</rdfs:comment>
    <rdfs:domain rdf:resource="http://data.lirmm.fr/ontologies/osp#Tribunal"/>
    <rdfs:subPropertyOf rdf:resource="http://data.lirmm.fr/ontologies/osp#aChambre"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#hasPresident">
    <rdfs:label>has president of tribunal</rdfs:label>
    <rdfs:comment>Propriété définissant qu'un tribunal a un président</rdfs:comment>
    <rdfs:domain rdf:resource="http://data.lirmm.fr/ontologies/osp#Tribunal"/>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#hasMatter">
    <rdfs:label>has matter</rdfs:label>
    <rdfs:comment>Propriété désignant la matière civile ou pénale que le tribunal revêt.</rdfs:comment>
    <rdfs:domain rdf:resource="http://data.lirmm.fr/ontologies/osp#Tribunal"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#aOrdreDeJuridiction">
    <rdfs:label>A ordre de juridiction</rdfs:label>
    <rdfs:comment>Propriété apportant la distinction entre ordre administratif ou judiciaire pour un tribunal</rdfs:comment>
    <rdfs:domain rdf:resource="http://data.lirmm.fr/ontologies/osp#Tribunal"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#hasHeadOfState">
    <rdfs:label>has head of state</rdfs:label>
    <rdfs:comment>Propriété définissant le chef d'Etat d'un pays</rdfs:comment>
    <rdfs:range rdf:resource="http://data.lirmm.fr/ontologies/osp#ChefDEtat"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#hasHeadOfGov">
    <rdfs:label>has head of gov</rdfs:label>
    <rdfs:comment>Un service ou un pays à, dans un gouvernement démocratique, un chef de gouvernement</rdfs:comment>
    <rdfs:range rdf:resource="http://data.lirmm.fr/ontologies/osp#ChefDuGouvernement"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#aEvenement">
    <rdfs:label>A evenement</rdfs:label>
    <rdfs:comment>Propriété permettant de lier quelque chose à un évènement tel qu'il est définie dans l'ontologie ical.</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#aDegre">
    <rdfs:label>a degre</rdfs:label>
    <rdfs:comment>Le système juridictionnel français est composé de 2 degrés et d'une haute juridiction. La haute juridiction n'est pas un degre. Nous l'a comptons néanmoins ainsi pour simplifier l'utilisation pour d'autres systèmes. 1er degré, 2nd degré ou haute juridiction</rdfs:comment>
    <rdfs:domain rdf:resource="http://data.lirmm.fr/ontologies/osp#Tribunal"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#aCompetenceTerritoriale">
    <rdfs:label>A Competence Territoriale</rdfs:label>
    <rdfs:comment>Propriété permettant la hiérarchisation du terrain couvert par un organisme. Généralement : régional, national, international.</rdfs:comment>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#hasCompetence">
    <rdfs:label>has competence</rdfs:label>
    <rdfs:comment>Compétence d'un juge dans un domaine.
Ex : propriété intellectuelle, mer ...</rdfs:comment>
    <rdfs:domain rdf:resource="http://data.lirmm.fr/ontologies/osp#Juge"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#aChambre">
    <rdfs:label>a chambre</rdfs:label>
    <rdfs:comment>Les chambres divisent un tribunal, souvent de façon thématique (droit de la construction, droit de la co-propriété)</rdfs:comment>
    <rdfs:domain rdf:resource="http://data.lirmm.fr/ontologies/osp#Tribunal"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#aAutoriteSur">
    <rdfs:label>a autorité sur</rdfs:label>
    <rdfs:comment>Propriété définissant une autorité hiérarchique d'une organization sur une autre. Cette autorité est de préférence issue de textes légaux.

Ne pas confondre autorité et sous organisme. Ex :
Un ministère a un cabinet (on utilisera suborganization de @org)
Le ministère de l'éducation exerce une autorité sur les écoles.</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#fax">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label>fax</rdfs:label>
    <rdfs:comment>fax number</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#departementDe">
    <rdfs:label>departementDe</rdfs:label>
    <rdfs:comment>Propriété liant un département à quelque chose</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#communeDe">
    <rdfs:label>Commune de</rdfs:label>
    <rdfs:comment>Propriété établissant un lien entre une commune et quelque chose.</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#arrondissementDe">
    <rdfs:label>arrondissementDe</rdfs:label>
    <rdfs:comment>Propriété liant un arrondissement à quelque chose</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#aRegion">
    <rdfs:label>aRegion</rdfs:label>
    <rdfs:comment>Propriété liant quelque chose à une région</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#aCommune">
    <rdfs:label>a commune</rdfs:label>
    <rdfs:comment>Propriété établissant un lien entre quelque chose et une commune</rdfs:comment>
    <rdfs:domain rdf:resource="http://data.lirmm.fr/ontologies/osp#OrganisationEtatique"/>
    <rdfs:domain rdf:resource="http://data.lirmm.fr/ontologies/osp#OrganisationPublique"/>
    <rdfs:domain rdf:resource="http://data.lirmm.fr/ontologies/osp#Adresse"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#aArrondissement">
    <rdfs:label>aArrondissement</rdfs:label>
    <rdfs:comment>Propriété liant quelque chose à un département. Inverse de arrondissementDe.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#ordreDeJuridiction">
    <rdfs:label>ordre de juridiction</rdfs:label>
    <rdfs:domain rdf:resource="http://data.lirmm.fr/ontologies/osp#Tribunal"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#aInitiales">
    <rdfs:label>a initiales</rdfs:label>
    <rdfs:comment>propriété décrivant les initials d'une entreprise, d'un pays ... ex : RU, SNCF ...
sameAs hashtag oegov</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#numeroFax">
    <rdfs:label>Numéro de fax</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#hyperlienEvenement">
    <rdfs:label>hyperlien Evenement</rdfs:label>
    <rdfs:comment>Correspond au lien vers un évènement particulier</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#adresse">
    <rdfs:label>Adresse d'un établissement de service public</rdfs:label>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:range rdf:resource="http://data.lirmm.fr/ontologies/osp#Adresse"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/osp#ligneAdresse">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label>Ligne pour une adresse (ex: rue, numéro)</rdfs:label>
