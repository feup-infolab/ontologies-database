    <owl:DatatypeProperty rdf:about="&voidwh;#producesValue">
        <rdfs:label>produces Value</rdfs:label>
        <rdfs:isDefinedBy>http://www.ics.forth.gr/isl/VoIDWarehouse/VoID_Extension_Schema.owl</rdfs:isDefinedBy>
        <ns:term_status>Stable</ns:term_status>
        <rdfs:comment>A measurement is carried out by an agent using a specific metric according to one (or more) configurations over one (or more) datasets. Therefore, this property indicates the value produced by the measurement. </rdfs:comment>
        <rdfs:domain rdf:resource="&voidwh;#Measurement"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.ics.forth.gr/isl/VoIDWarehouse/VoID_Extension_Schema.owl#usesSPARQLquery -->

    <owl:DatatypeProperty rdf:about="&voidwh;#usesSPARQLquery">
        <rdfs:label>uses SPARQL query</rdfs:label>
        <ns:term_status>Stable</ns:term_status>
        <rdfs:comment>This property specifies the entities of interest involved in a configuration by providing a SPARQL query that returns them.</rdfs:comment>
        <rdfs:isDefinedBy>http://www.ics.forth.gr/isl/VoIDWarehouse/VoID_Extension_Schema.owl</rdfs:isDefinedBy>
        <rdfs:domain rdf:resource="&voidwh;#EntityConfiguration"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


