<rdf:Description rdf:about="http://purl.org/essglobal/vocab/SSEInitiative">
	<rdfs:label xml:lang="en">SSE Initiative</rdfs:label>
	<dcterms:description xml:lang="en">An organization, practice, network, or other initiative that is recognized as belonging within the social solidarity economy.  </dcterms:description>
	<rdfs:isDefinedBy>http://purl.org/essglobal/vocab/</rdfs:isDefinedBy>
	<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2014-02-06</dcterms:issued>
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-05-04</dcterms:modified>
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Class"/>
	<rdfs:subClassOf>http://www.w3.org/ns/org/OrganizationUnit</rdfs:subClassOf>
</rdf:Description>


<rdf:Description rdf:about="http://purl.org/essglobal/vocab/Network">
	<rdfs:label xml:lang="en">Network</rdfs:label>
	<dcterms:description xml:lang="en">A network of individuals and/or organizations that participate in the SSE. </dcterms:description>
	<rdfs:isDefinedBy>http://purl.org/essglobal/vocab/</rdfs:isDefinedBy>
	<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2014-02-06</dcterms:issued>
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-05-04</dcterms:modified>
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Class"/>
	<rdfs:subClassOf>http://www.w3.org/ns/org/OrganizationColaboration</rdfs:subClassOf>
</rdf:Description>


<rdf:Description rdf:about="http://purl.org/essglobal/vocab/ProductOrService">
	<rdfs:label xml:lang="en">Product or Service</rdfs:label>
	<dcterms:description xml:lang="en">The good offered by an SSE initiative. It may be material goods or provision of services. </dcterms:description>
	<rdfs:isDefinedBy>http://purl.org/essglobal/vocab/</rdfs:isDefinedBy>
	<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2014-02-06</dcterms:issued>
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-05-04</dcterms:modified>
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Class"/>
	<rdfs:subClassOf>http://purl.org/goodrelations/v1/ProductOrService</rdfs:subClassOf>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/essglobal/vocab/Address">
	<rdfs:label xml:lang="en">Address</rdfs:label>
	<dcterms:description xml:lang="en">A phisical address of a Location of sale, of a SSE Initiative or of a Network </dcterms:description>
	<rdfs:isDefinedBy>http://purl.org/essglobal/vocab/</rdfs:isDefinedBy>
	<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-05-04</dcterms:issued>
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-05-04</dcterms:modified>
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Class"/>
</rdf:Description>


<rdf:Description rdf:about="http://purl.org/essglobal/vocab/LocationOfSale">
	<rdfs:label xml:lang="en">Location of Sale</rdfs:label>
	<dcterms:description xml:lang="en">Place where the goods or services of an SSE initiative are provided. It can be self owned shops, but also partner places where the products or services are available among those from other initiatives. </dcterms:description>
	<rdfs:isDefinedBy>http://purl.org/essglobal/vocab/</rdfs:isDefinedBy>
	<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2014-02-06</dcterms:issued>
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-05-04</dcterms:modified>
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Class"/>
	<rdfs:subClassOf>http://purl.org/goodrelations/v1/Location</rdfs:subClassOf>
</rdf:Description>


<rdf:Description rdf:about="http://purl.org/essglobal/vocab/SaleOption">
	<rdfs:label xml:lang="en">Sale Option</rdfs:label>
	<dcterms:description xml:lang="en">A Product sold at a certain price, under specific properties, in a certain Location. </dcterms:description>
	<rdfs:isDefinedBy>http://purl.org/essglobal/vocab/</rdfs:isDefinedBy>
	<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2014-02-06</dcterms:issued>
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-05-04</dcterms:modified>
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Class"/>
</rdf:Description>


<rdf:Description rdf:about="http://purl.org/essglobal/vocab/Cost">
	<rdfs:label xml:lang="en">Cost</rdfs:label>
	<dcterms:description xml:lang="en">The cost for a particular product or service produced by an SSE initiative or network, including all costs. The price will be this cost added to delivery costs and sales margin as can be seen in the sale-option class.</dcterms:description>
	<rdfs:isDefinedBy>http://purl.org/essglobal/vocab/</rdfs:isDefinedBy>
	<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2014-02-06</dcterms:issued>
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-05-04</dcterms:modified>
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Class"/>
</rdf:Description>


<rdf:Description rdf:about="http://purl.org/essglobal/vocab/CostComposition">
	<rdfs:label xml:lang="en">Cost Composition</rdfs:label>
	<dcterms:description xml:lang="en">A breakdown of all inputs (as taxes and raw materials) and labour costs that make up the product or service's final cost. An open cost is a pre-requisite for solidarity fair trade systems. </dcterms:description>
	<rdfs:isDefinedBy>http://purl.org/essglobal/vocab/</rdfs:isDefinedBy>
	<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2014-02-06</dcterms:issued>
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-05-04</dcterms:modified>
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Class"/>
</rdf:Description>


<rdf:Description rdf:about="http://purl.org/essglobal/vocab/Input">
	<rdfs:label xml:lang="en">Input</rdfs:label>
	<dcterms:description xml:lang="en">A product, service, or activity that goes into making the final product or service. </dcterms:description>
	<rdfs:isDefinedBy>http://purl.org/essglobal/vocab/</rdfs:isDefinedBy>
	<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2014-02-06</dcterms:issued>
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-05-04</dcterms:modified>
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Class"/>
</rdf:Description>


<rdf:Description rdf:about="http://purl.org/essglobal/vocab/Labour">
	<rdfs:label xml:lang="en">Labour</rdfs:label>
	<dcterms:description xml:lang="en">Work done for specific tasks related to the provision of goods or services offered by the SSE initiative. Generally it can be human, animal or machine labour, but in ESSglobal it's human labour only.</dcterms:description>
	<rdfs:isDefinedBy>http://purl.org/essglobal/vocab/</rdfs:isDefinedBy>
	<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2014-02-06</dcterms:issued>
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-05-04</dcterms:modified>
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Class"/>
</rdf:Description>


<rdf:Description rdf:about="http://purl.org/essglobal/vocab/OtherCosts">
	<rdfs:label xml:lang="en">Other Costs</rdfs:label>
	<dcterms:description xml:lang="en">Other costs which impact on the final cost of a product or service provided by an SSE initiative, other than input or labour, like taxes, deprecation of machinery, funds, etc. The delivery costs are not included in these costs: they are included in the sale-options class.</dcterms:description>
	<rdfs:isDefinedBy>http://purl.org/essglobal/vocab/</rdfs:isDefinedBy>
	<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2014-02-06</dcterms:issued>
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-05-04</dcterms:modified>
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Class"/>
</rdf:Description>



<rdf:Description rdf:about="http://purl.org/essglobal/vocab/objective">
	<rdfs:label xml:lang="en">Objective</rdfs:label>
	<dcterms:description xml:lang="en">Actionable and measurable steps the SSE Initiative is taking to carry out its mission. </dcterms:description>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/essglobal/vocab/"/>
	<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2014-02-06</dcterms:issued>
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-05-04</dcterms:modified>
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/essglobal/vocab/SSEInitiative"/>
	<rdfs:domain rdf:resource="http://purl.org/essglobal/vocab/Network"/>
</rdf:Description>


<rdf:Description rdf:about="http://purl.org/essglobal/vocab/mission">
	<rdfs:label xml:lang="en">Mission</rdfs:label>
	<dcterms:description xml:lang="en">The initiative's vision, values, and principles.</dcterms:description>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/essglobal/vocab/"/>
