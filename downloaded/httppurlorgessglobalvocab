<rdf:Description rdf:about="http://purl.org/essglobal/vocab/hasLabourCost">
	<rdfs:label xml:lang="en">Has Labour</rdfs:label>
	<dcterms:description xml:lang="en">Work done for specific tasks related to the provision of goods or services offered by the SSE initiative. Generally it can be human, animal or machine labour, but in ESSglobal it's human labour only.</dcterms:description>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/essglobal/vocab/"/>
	<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2014-02-06</dcterms:issued>
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-05-04</dcterms:modified>
