<rdf:Description rdf:about="http://purl.org/essglobal/vocab/qualifier">
	<rdfs:label xml:lang="en">Qualifier</rdfs:label>
	<dcterms:description xml:lang="en">
		Aspect that differentiates the product or service of an SSE initiative which is related to the form of production, the actors involved, the use of only organic materials, etc. 
		Examples are organic, permacultural, made with recycled raw materials, indigenous, made by women, etc.
   </dcterms:description>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/essglobal/vocab/"/>
	<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2014-02-06</dcterms:issued>
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-05-04</dcterms:modified>
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:Datatype rdf:resource="http://purl.org/essglobal/standard/qualifiers/" />
	<rdfs:domain rdf:resource="http://purl.org/essglobal/vocab/ProductOrService"/>
	<rdfs:domain rdf:resource="http://purl.org/essglobal/vocab/Input" />
 </rdf:Description>
 
