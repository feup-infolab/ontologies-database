op:cas-number  a            owl:DatatypeProperty ;
        rdfs:domain         op:Substance ;
        rdfs:isDefinedBy    <http://environment.data.gov.au/def/op> ;
        rdfs:label          "CAS Number"^^xsd:string ;
        rdfs:range          op:CAS-number ;
        rdfs:subPropertyOf  skos:notation .

op:ScaledQuantityKind
        a                 owl:Class ;
        rdfs:comment      "Scaled quantity kind has one or more unit properties, which indicate valid units of measure for this quantity kind. \r\n\r\nIt is likely that this class is equivalent to qudt:QuantityKind, but has been declared independently for governance of the attached qudt:unit property"^^xsd:string ;
        rdfs:isDefinedBy  <http://environment.data.gov.au/def/op> ;
        rdfs:label        "Scaled Quantity Kind"@en ;
        rdfs:subClassOf   qudt:QuantityKind ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:minCardinality  1 ;
                            owl:onProperty      qudt:unit
                          ] ;
        rdfs:subClassOf   [ a                  owl:Restriction ;
                            owl:allValuesFrom  qudt:Unit ;
                            owl:onProperty     qudt:unit
                          ] .

op:substance  a                 owl:ObjectProperty ;
        rdfs:comment            "In an observable property definition, this is the substance whose concentration, presence, or similar is of interest. "^^xsd:string ;
        rdfs:domain             op:PropertyKind ;
        rdfs:isDefinedBy        <http://environment.data.gov.au/def/op> ;
        rdfs:label              "object of interest"@en ;
        rdfs:range              op:Substance ;
        rdfs:subPropertyOf      op:constraint ;
        owl:equivalentProperty  op:objectOfInterest .

op:Taxon  a               owl:Class ;
        rdfs:comment      "type of organism, any rank\r\n\r\nPreferably with a formally recognised designiation such as a latin binomial, or LSID, but may be a less formal grouping when needed."^^xsd:string ;
        rdfs:isDefinedBy  <http://environment.data.gov.au/def/op> ;
        rdfs:label        "taxon"^^xsd:string ;
        rdfs:subClassOf   op:SubstanceOrTaxon ;
        skos:prefLabel    "taxon"@en .

_:b0    xsd:pattern  "[0-9]+\\-[0-9]+\\-[0-9]+"^^xsd:string .

op:objectOfInterest  a          owl:ObjectProperty ;
        rdfs:comment            "In an observable property definition, this is the substance or taxon whose count, concentration, presence, or other similar property, is of interest. "^^xsd:string ;
        rdfs:domain             op:PropertyKind ;
        rdfs:isDefinedBy        <http://environment.data.gov.au/def/op> ;
        rdfs:label              "object of interest"@en ;
        rdfs:range              op:SubstanceOrTaxon ;
        rdfs:subPropertyOf      op:constraint ;
        owl:equivalentProperty  op:substance .

op:procedure  a             owl:ObjectProperty ;
        rdfs:comment        "The procedure is a description of the procedure, algorithm, protocol or sensor system used in making observation or estimate of property value"^^xsd:string ;
        rdfs:domain         op:PropertyKind ;
        rdfs:isDefinedBy    <http://environment.data.gov.au/def/op> ;
        rdfs:label          "procedure"@en ;
        rdfs:subPropertyOf  op:constraint .

qudt:generalization  rdfs:subPropertyOf
                skos:broader .

op:constraint  a          owl:ObjectProperty ;
        rdfs:comment      "Constraint that refines the definition of an observable property definition. This may include concerns such as realm of application, substance or taxon involved, host organism, matrix, applicable units or vocabulary. "^^xsd:string ;
        rdfs:domain       op:PropertyKind ;
        rdfs:isDefinedBy  <http://environment.data.gov.au/def/op> ;
        rdfs:label        "constraint"@en .

<http://environment.data.gov.au/def/_op>
        a                  reg:RegisterItem ;
        rdfs:label         "Observable-Properties ontology"^^xsd:string ;
        dct:dateAccepted   "2016-02-15T01:05:23.073Z"^^xsd:dateTime , "2016-02-16T04:16:54.215Z"^^xsd:dateTime ;
        dct:dateSubmitted  "2016-02-15T01:05:11.079Z"^^xsd:dateTime ;
        dct:description    "A general purpose ontology for observable properties. The ontology supports description of both qualitative and quantitative properties. The allowed scale or units of measure may be specified. A property may be linked to substances-or-taxa and to features or realms, if they play a role in the definition. "^^xsd:string ;
        dct:modified       "2016-02-28T06:26:54.954Z"^^xsd:dateTime ;
        reg:definition     [ reg:entity       <http://environment.data.gov.au/def/op> ;
                             reg:sourceGraph  <http://environment.data.gov.au/def/_op:4#graph>
                           ] ;
        reg:itemClass      owl:Ontology , <http://purl.org/vocommons/voaf#Vocabulary> ;
        reg:notation       "op" ;
        reg:register       <http://environment.data.gov.au/def> ;
        reg:status         reg:statusStable ;
        reg:submitter      [ foaf:accountName  "simon.cox@csiro.au" ;
                             foaf:name         "Simon Cox (email)"
                           ] ;
        owl:versionInfo    4 .

op:CAS-number  a             rdfs:Datatype ;
        rdfs:comment         "The identifier for a substance provided by the Chemical Abstracts Service"^^xsd:string ;
        rdfs:isDefinedBy     <http://environment.data.gov.au/def/op> ;
        rdfs:label           "CAS Number"^^xsd:string ;
        owl:equivalentClass  [ a                     rdfs:Datatype ;
                               owl:onDataType        xsd:string ;
                               owl:withRestrictions  ( _:b0 )
                             ] .

qudt:Unit  rdfs:subClassOf  skos:Concept .

qudt:specialization  rdfs:subPropertyOf
                skos:narrower .

qudt:QuantityKind  rdfs:subClassOf  op:PropertyKind .

op:SubstanceOrTaxon  a    owl:Class ;
        rdfs:comment      "Class of stuff and things, individuals of which identify a class of stuff and things that make observed properties concrete."^^xsd:string ;
        rdfs:isDefinedBy  <http://environment.data.gov.au/def/op> ;
        rdfs:label        "Substance or Taxon"@en ;
        rdfs:subClassOf   qudt:EnumeratedValue ;
        skos:altLabel     "Species"@en ;
        skos:prefLabel    "Substance or Taxon"@en .

qudt:EnumeratedValue  rdfs:subClassOf  skos:Concept .

op:propertyKind  a        owl:ObjectProperty ;
        rdfs:isDefinedBy  <http://environment.data.gov.au/def/op> ;
        rdfs:label        "property kind"@en ;
        rdfs:range        op:PropertyKind .

op:applicableVocabulary
        a                 owl:ObjectProperty ;
        rdfs:comment      "In an observable property definition, the applicable vocabulary is the set of terms or concepts from which a nominal value must be drawn. \r\n\r\nCompare with QUDT2 'applicable unit'"^^xsd:string ;
        rdfs:domain       op:QualityKind ;
        rdfs:isDefinedBy  <http://environment.data.gov.au/def/op> ;
        rdfs:label        "applicable vocabulary"@en .

op:Substance  a           owl:Class ;
        rdfs:comment      "type of material\r\n\r\nTypically a chemical substance, with a formal designator from CAS or ChEBI or similar, but may include less formal grouplings or union types when needed"^^xsd:string ;
        rdfs:isDefinedBy  <http://environment.data.gov.au/def/op> ;
        rdfs:label        "Substance"^^xsd:string ;
        rdfs:subClassOf   op:SubstanceOrTaxon ;
        skos:prefLabel    "Substance"@en .

op:QualityKind  a         owl:Class ;
        rdfs:comment      "non-numeric PropertyKind"^^xsd:string ;
        rdfs:isDefinedBy  <http://environment.data.gov.au/def/op> ;
        rdfs:label        "Quality Kind"@en ;
        rdfs:subClassOf   op:PropertyKind ;
        rdfs:subClassOf   [ a                  owl:Restriction ;
                            owl:allValuesFrom  op:QualityKind ;
                            owl:onProperty     qudt:generalization
                          ] ;
        owl:disjointWith  qudt:QuantityKind .

op:statisticalMeasure
        a                   owl:ObjectProperty ;
        rdfs:isDefinedBy    <http://environment.data.gov.au/def/op> ;
        rdfs:label          "Statistical Measure"^^xsd:string ;
        rdfs:subPropertyOf  op:constraint .

<http://environment.data.gov.au/def/op>
        a                              <http://purl.org/vocommons/voaf#Vocabulary> , owl:Ontology ;
        rdfs:comment                   "This ontology was developed to enable publication of a vocabulary of water quality properties for the Bioregional Assessments and eReefs projects.\r\n\r\nThis ontology extends the QUDT schema. \r\n\r\nSome key classes and properties are linked to SKOS, so that instances of an observable-property vocabulary are implicitly also SKOS concepts, and may be accessed through general purpose vocabulary APIs based on SKOS. "^^xsd:string ;
        rdfs:label                     "Observable-Properties ontology"^^xsd:string ;
        dct:created                    "2013"^^xsd:gYear ;
        dct:creator                    "Simon J D Cox"^^xsd:string , "Jonathan Yu"^^xsd:string , "Bruce A Simons"^^xsd:string ;
        dct:creator                    <http://people.csiro.au/Jonathan-Yu> , <http://people.csiro.au/Simon-Cox> ;
        dct:description                "A general purpose ontology for observable properties. The ontology supports description of both qualitative and quantitative properties. The allowed scale or units of measure may be specified. A property may be linked to substances-or-taxa and to features or realms, if they play a role in the definition. "^^xsd:string ;
        dct:license                    <http://creativecommons.org/licenses/by/3.0/> ;
        dct:modified                   "2016-02-27"^^xsd:date ;
        dct:publisher                  "CSIRO"^^xsd:string ;
        dct:rights                     "Copyright Â© 2013-2016 CSIRO"^^xsd:string ;
        dct:title                      "Observable properties"^^xsd:string ;
        vann:preferredNamespacePrefix  "op"^^xsd:string ;
        vann:preferredNamespaceUri     "http://registry.it.csiro.au/ont/op#"^^xsd:string ;
        <http://purl.org/vocommons/voaf#extends>
                <http://qudt.org/schema/qudt> ;
        owl:imports                    <http://qudt.org/1.1/schema/quantity> , vann: , <http://purl.org/vocommons/voaf> , dc: ;
        skos:editorialNote             "fixed range of op:substance to op:Substance 2016-02-27"^^xsd:string ;
        skos:historyNote               "Improved documentation and labels 2016-02-27"^^xsd:string .

op:hostOrganism  a          owl:ObjectProperty ;
        rdfs:comment        "In an observable property definition, the host-organism indicates the host taxon, optionally constrained by sex, age, size etc, for an observable property."^^xsd:string ;
        rdfs:domain         op:PropertyKind ;
        rdfs:isDefinedBy    <http://environment.data.gov.au/def/op> ;
        rdfs:label          "host organism"@en ;
        rdfs:range          op:Taxon ;
        rdfs:subPropertyOf  op:constraint .

op:matrix  a                owl:ObjectProperty ;
        rdfs:comment        "In an observable property definition, the matrix is the material context (container feature or medium) for an observable property."^^xsd:string ;
        rdfs:domain         op:PropertyKind ;
        rdfs:isDefinedBy    <http://environment.data.gov.au/def/op> ;
        rdfs:label          "matrix"@en ;
        rdfs:subPropertyOf  op:constraint .

op:featureOfInterest  a     owl:ObjectProperty ;
        rdfs:comment        "In an observable property definition, the feature-of-interest constrains the feature realm that the property is associated with."^^xsd:string ;
        rdfs:domain         op:PropertyKind ;
        rdfs:isDefinedBy    <http://environment.data.gov.au/def/op> ;
        rdfs:label          "feature of interest"@en ;
        rdfs:subPropertyOf  op:constraint .

op:PropertyKind  a        owl:Class ;
        rdfs:comment      "Superclass of qudt:QuantityKind\nThis class accommodates all kinds of properties, including those (qualities) that are not described by quantities (numeric values). \r\nMay carry constraints, such as substance, objectOfInterest, featureOfInterest, matrix, hostOrganism, to refine the definition of a generalization. property is associated\r\ne.g. tree-height, nitrogen-concentration, concentration-of-zinc-in-the-liver-of-a-specific-fish-taxon-only-males-larger-than-20cm"^^xsd:string ;
        rdfs:isDefinedBy  <http://environment.data.gov.au/def/op> ;
        rdfs:label        "Property kind"@en ;
        rdfs:subClassOf   qudt:EnumeratedValue ;
        rdfs:subClassOf   [ a                  owl:Restriction ;
                            owl:allValuesFrom  op:PropertyKind ;
                            owl:onProperty     qudt:generalization
                          ] .

op:qualityKind  a                 owl:ObjectProperty ;
        rdfs:isDefinedBy          <http://environment.data.gov.au/def/op> ;
        rdfs:label                "quality kind"@en ;
        rdfs:range                op:QualityKind ;
        rdfs:subPropertyOf        op:propertyKind ;
        owl:propertyDisjointWith  qudt:quantityKind .

qudt:quantityKind  rdfs:subPropertyOf  op:propertyKind .
