    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">Class for units of numerical values that are used to express certain property states.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#DataCleansingRule">
