    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">Reference Property 4</rdfs:label>
    <rdfs:domain rdf:resource="#FuncDepReferenceRule"/>
