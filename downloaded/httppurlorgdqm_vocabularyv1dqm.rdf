    <rdfs:label xml:lang="en">White Space Removal Rule</rdfs:label>
    <rdfs:subClassOf rdf:resource="#LiteralCleansingRule"/>
  </rdf:Description>
  <rdf:Description rdf:about="#Condition">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Condition</rdfs:label>
    <rdfs:comment xml:lang="en">A condition constrains instances of a class to a certain subset for which certain data quality rules apply.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#MissingProperty">
    <rdfs:label xml:lang="en">Missing Property</rdfs:label>
    <rdfs:subClassOf rdf:resource="#MissingElement"/>
