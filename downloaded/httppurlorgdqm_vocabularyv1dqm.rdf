  </rdf:Description>
  <rdf:Description rdf:about="#referenceProperty5">
    <rdfs:subPropertyOf rdf:resource="#referenceProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">The reference property points to the URI of a property that holds trusted values, i.e. values of high quality</rdfs:comment>
    <rdfs:domain rdf:resource="#FuncDepReferenceRule"/>
    <rdfs:label xml:lang="en">Reference Property 5</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="#unitOfConfidence">
