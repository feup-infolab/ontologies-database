<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:dqm="http://purl.org/dqm-vocabulary/v1/dqm#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://purl.org/dqm-vocabulary/v1/dqm" > 
  <rdf:Description rdf:about="#taskDependent">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#DataRequirement"/>
    <rdfs:comment xml:lang="en">Specifies whether the data quality rule is task dependent.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:label xml:lang="en">Task Dependent</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="#reqMetadata"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A0">
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    <rdf:first rdf:resource="#DataRequirement"/>
  </rdf:Description>
  <rdf:Description rdf:about="#subTaskOf">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="#Task"/>
    <rdfs:comment xml:lang="en">Property that can be used to represent hierachies of between tasks.</rdfs:comment>
    <rdfs:range rdf:resource="#Task"/>
    <rdfs:subPropertyOf rdf:resource="#dataRequirementsRelationship"/>
    <rdfs:label xml:lang="en">Sub Task Of</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="#reqName">
    <rdfs:subPropertyOf rdf:resource="#reqMetadata"/>
    <rdfs:comment xml:lang="en">Specifies the name of a data requirement in natural language.</rdfs:comment>
    <rdfs:label xml:lang="en">Requirement Name</rdfs:label>
    <rdfs:domain rdf:nodeID="A1"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Description>
  <rdf:Description rdf:about="#IllegalValue">
    <rdfs:label xml:lang="en">Illegal Value</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">An illegal value is a data value that must not be used for a property.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#DataRequirementViolation"/>
  </rdf:Description>
  <rdf:Description rdf:about="#reqDescription">
    <rdfs:subPropertyOf rdf:resource="#reqMetadata"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment xml:lang="en">Describes a data requirement in natural language.</rdfs:comment>
    <rdfs:domain rdf:resource="#DataRequirement"/>
    <rdfs:label xml:lang="en">Requirement Description</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="#requiredValue">
    <rdfs:comment xml:lang="en">Specifies whether a value for a property must exist.</rdfs:comment>
    <rdfs:domain rdf:nodeID="A2"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:label xml:lang="en">Required Value</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="#blacklistProperty">
    <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
    <rdfs:domain rdf:resource="#IllegalValueRule"/>
    <rdfs:label xml:lang="en">Blacklist Property</rdfs:label>
    <rdfs:comment xml:lang="en">A blacklist property holds the values that are disallowed for a certain data set.</rdfs:comment>
    <rdfs:range rdf:resource="#BlacklistProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="#AffectedProperty">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Affected Property</rdfs:label>
    <rdfs:subClassOf rdf:resource="#DataElement"/>
    <rdfs:comment xml:lang="en">An affected property specifies the property of a data instance that contains one or more data quality problems.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A3">
    <owl:unionOf rdf:nodeID="A4"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A1">
    <owl:unionOf rdf:nodeID="A5"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="#Completeness">
    <rdfs:comment xml:lang="en">Completeness is the extent to which data are of sufficient breadth, depth, and scope for the task at hand.</rdfs:comment>
    <rdfs:label xml:lang="en">Completeness</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="#DataQualityScore"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A6">
    <rdf:first rdf:resource="#FuncDepValueRule"/>
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
  </rdf:Description>
  <rdf:Description rdf:about="#referenceProperty">
    <rdfs:label xml:lang="en">Reference Property</rdfs:label>
    <rdfs:range rdf:resource="#TrustedProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">The reference property points to the URI of a property that holds trusted values, i.e. values of high quality</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/license">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A7">
    <rdf:first rdf:resource="#FuncDepReferenceRule"/>
    <rdf:rest rdf:nodeID="A8"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A2">
    <owl:unionOf rdf:nodeID="A9"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A10">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <owl:unionOf rdf:nodeID="A11"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A12">
    <rdf:first rdf:resource="#TestedProperty"/>
    <rdf:rest rdf:nodeID="A13"/>
  </rdf:Description>
  <rdf:Description rdf:about="#referenceClass">
    <rdfs:domain rdf:nodeID="A14"/>
    <rdfs:range rdf:resource="#TrustedClass"/>
    <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
    <rdfs:label xml:lang="en">Reference Class</rdfs:label>
    <rdfs:comment xml:lang="en">The reference class property points to the URI of a class that contains trusted instances, i.e. instances of high quality.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="#SemanticAccuracy">
    <rdfs:subClassOf rdf:resource="#Accuracy"/>
    <rdfs:comment xml:lang="en">Semantic accuracy is the degree to which the data values of an instance represent the correct state of an entity’s property.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Semantic Accuracy</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="#officialName">
    <rdfs:label xml:lang="en">Official Name</rdfs:label>
    <rdfs:comment xml:lang="en">Official name of a value.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#ScaleValue"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Description>
  <rdf:Description rdf:about="#IllegalValueRule">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Illegal Value Rule</rdfs:label>
    <rdfs:comment xml:lang="en">An illegal value rule is a property requirement that specifies the values that a certain property must not obtain. Illegal value rules, therefore, refer to reference properties that hold all disallowed values.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#PropertyRequirement"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A15">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <owl:unionOf rdf:nodeID="A16"/>
  </rdf:Description>
  <rdf:Description rdf:about="#testedProperty">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">The property with the values that shall be tested for data quality problems.</rdfs:comment>
    <rdfs:label xml:lang="en">Tested Property</rdfs:label>
    <rdfs:range rdf:resource="#TestedProperty"/>
    <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
  </rdf:Description>
  <rdf:Description rdf:about="#BlacklistProperty">
    <rdfs:comment xml:lang="en">A blacklist property holds the values that are disallowed for a certain data set.</rdfs:comment>
    <rdfs:label xml:lang="en">Blacklist Property</rdfs:label>
    <rdfs:subClassOf rdf:resource="#DataElement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="#UniqueValueRule">
    <rdfs:label xml:lang="en">Unique Value Rule</rdfs:label>
    <rdfs:comment xml:lang="en">A unique value rule is a property requirement that specifies that each value of a certain property must be unique in instances of a certain class.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#PropertyRequirement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="#ruleViolation">
    <rdfs:comment xml:lang="en">Connects data requirements with data quality problems that have been identified using the requirement.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#dataRequirementsRelationship"/>
    <rdfs:range rdf:resource="#DataRequirementViolation"/>
    <owl:inverseOf rdf:resource="#ruleOfIdentification"/>
    <rdfs:domain rdf:resource="#DataRequirement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">Rule Violation</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="#testedClass">
    <rdfs:comment xml:lang="en">The tested class property specifies the class that holds the instances that shall be tested for data quality problems.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
    <rdfs:domain rdf:resource="#DataRequirement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#TestedClass"/>
    <rdfs:label xml:lang="en">Tested Class</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="#timeOfIdentification">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#DataRequirementViolation"/>
    <rdfs:label xml:lang="en">Time Of Identification</rdfs:label>
    <rdfs:comment xml:lang="en">The time of identification property specifies the date and time in which the data quality problem was detected</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </rdf:Description>
  <rdf:Description rdf:about="#newValue">
    <rdfs:subPropertyOf rdf:resource="#cleansingProperty"/>
    <rdfs:label xml:lang="en">New Value</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#ValueSubstitution"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">The new value property specifies the new value which shall substitute the current value.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#testedProperty5">
    <rdfs:label xml:lang="en">Tested Property 5</rdfs:label>
    <rdfs:domain rdf:resource="#MatchingValueRule"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">The property with the values that shall be tested for data quality problems.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#testedProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="#PropertyUniqueness">
    <rdfs:subClassOf rdf:resource="#Uniqueness"/>
    <rdfs:comment xml:lang="en">Property uniqueness is the degree to which the values of a property (that must only contain unique values within instances of a certain class) are unique.</rdfs:comment>
    <rdfs:label xml:lang="en">Property Uniqueness</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="#validFrom">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="#reqMetadata"/>
    <rdfs:comment xml:lang="en">Specifies the date and time from which a rule is valid.</rdfs:comment>
    <rdfs:label xml:lang="en">Valid From</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:domain rdf:nodeID="A17"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A18">
    <rdf:first rdf:resource="#DataCleansingRule"/>
    <rdf:rest rdf:nodeID="A0"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A19">
    <rdf:first rdf:resource="#Task"/>
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
  </rdf:Description>
  <rdf:Description rdf:about="#notEqualTo">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en">Not Equal To</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="#operatedValue"/>
    <rdfs:domain rdf:nodeID="A20"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">Holds one or more consecutive characters that are used to identify values of another property that do not match with the value of this property.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#basedOn">
    <rdfs:label xml:lang="en">Based On</rdfs:label>
    <rdfs:range rdf:resource="#DataRequirement"/>
    <rdfs:domain rdf:resource="#DataQualityScore"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <owl:inverseOf rdf:resource="#hasScore"/>
    <rdfs:comment xml:lang="en">Connects a data quality score with the data requirements that were used to calculate the score.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#dataRequirementsRelationship"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A21">
    <owl:unionOf rdf:nodeID="A18"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="#hasURI">
    <rdfs:comment xml:lang="en">The hasURI property is used to map properties to instances of a class.</rdfs:comment>
    <rdfs:label xml:lang="en">Has URI</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <rdfs:domain rdf:resource="#DataElement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A22">
    <rdf:rest rdf:nodeID="A23"/>
    <rdf:first rdf:resource="#DataElement"/>
  </rdf:Description>
  <rdf:Description rdf:about="#assessedProperty">
    <rdfs:label xml:lang="en">Assessed Property</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="#DataQualityScore"/>
    <rdfs:comment xml:lang="en">The assessed property represents all properties that have been tested for rule violations during the computation of the assessment score.</rdfs:comment>
    <rdfs:range rdf:resource="#TestedProperty"/>
    <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
  </rdf:Description>
  <rdf:Description rdf:about="#TrustedProperty">
    <rdfs:subClassOf rdf:resource="#DataElement"/>
    <rdfs:comment xml:lang="en">A trusted property holds the values that serve as a trusted reference in legal value rules / functional dependency reference rules, e.g. to define legal values for a tested property.</rdfs:comment>
    <rdfs:label xml:lang="en">Trusted Property</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="#IllegalValueRangeRule">
    <rdfs:comment xml:lang="en">An illegal value range rule is a property requirement that specifies the upper and/or lower boundary of numeric values that a certain property must not obtain.</rdfs:comment>
    <rdfs:label xml:lang="en">Illegal Value Range Rule</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="#PropertyRequirement"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A5">
    <rdf:rest rdf:nodeID="A24"/>
    <rdf:first rdf:resource="#DataCleansingRule"/>
  </rdf:Description>
  <rdf:Description rdf:about="#AffectedInstance">
    <rdfs:label xml:lang="en">Affected Instance</rdfs:label>
    <rdfs:comment xml:lang="en">An affected instance is a data instance that contains one or more data quality problems.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="#DataElement"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/title">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A25">
    <rdf:first rdf:resource="#Task"/>
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
  </rdf:Description>
  <rdf:Description rdf:about="#technicalName">
    <rdfs:domain rdf:resource="#ScaleValue"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en">Technical Name</rdfs:label>
    <rdfs:comment xml:lang="en">Technical name of a value.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A26">
    <rdf:first rdf:resource="#MultiPropertyCleansingRule"/>
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
  </rdf:Description>
  <rdf:Description rdf:about="#requiredProperty">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">Specifies whether a property must exist.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:nodeID="A27"/>
    <rdfs:label xml:lang="en">Required Property</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A28">
    <rdf:first rdf:resource="#IllegalValueRangeRule"/>
    <rdf:rest rdf:nodeID="A29"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A30">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <owl:unionOf rdf:nodeID="A31"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A32">
    <rdf:first rdf:resource="#DataRequirement"/>
    <rdf:rest rdf:nodeID="A19"/>
  </rdf:Description>
  <rdf:Description rdf:about="#validUntil">
    <rdfs:domain rdf:nodeID="A33"/>
    <rdfs:label xml:lang="en">Valid Until</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="#reqMetadata"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">Specifies the date and time until a rule stays valid.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </rdf:Description>
  <rdf:Description rdf:about="#ClassRequirement">
    <rdfs:subClassOf rdf:resource="#DataRequirement"/>
    <rdfs:label xml:lang="en">Class Requirement</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A class requirement is a data requirement that refers to the instances of a class as a whole.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#TestedProperty">
    <rdfs:subClassOf rdf:resource="#DataElement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Tested Property</rdfs:label>
    <rdfs:comment xml:lang="en">A tested property holds the values to be analyzed for data quality problems.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#lastModified">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">Last Modified</rdfs:label>
    <rdfs:comment xml:lang="en">The property lastModified indicates the date and time when the rule was last modified.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:subPropertyOf rdf:resource="#reqMetadata"/>
    <rdfs:domain rdf:resource="#DataRequirement"/>
  </rdf:Description>
  <rdf:Description rdf:about="#blacklistClass">
    <rdfs:comment xml:lang="en">A blacklist class holds instances with values that are disallowed for a certain data set.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
    <rdfs:domain rdf:resource="#IllegalValueRule"/>
    <rdfs:range rdf:resource="#BlacklistClass"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">Blacklist Class</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A11">
    <rdf:rest rdf:nodeID="A34"/>
    <rdf:first rdf:resource="#Condition"/>
  </rdf:Description>
  <rdf:Description rdf:about="#dataCleansingRuleRelationship">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">Data Cleansing Rule Relationship</rdfs:label>
    <rdfs:comment xml:lang="en">Data cleansing rule relationships are object properties of the data cleansing rules.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#affectedInstance">
    <rdfs:comment xml:lang="de">An affected instance is the data instance that contains one or more data requirement violations.</rdfs:comment>
    <rdfs:domain rdf:resource="#DataRequirementViolation"/>
    <rdfs:range rdf:resource="#AffectedInstance"/>
    <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="de">Affected Instance</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A35">
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    <rdf:first rdf:resource="#FuncDepValueRule"/>
  </rdf:Description>
  <rdf:Description rdf:about="#ExpiryRule">
    <rdfs:subClassOf rdf:resource="#OutdatedInstanceRule"/>
    <rdfs:comment xml:lang="en">An expiry rule is an outdated instance rule that specifies the expiration date of an instance that must not exceed the current date and time. Therefore, expiry rules require date information about the validity period of an instance.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Expiry Rule</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A36">
    <owl:unionOf rdf:nodeID="A28"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="#lessThan">
    <rdfs:label xml:lang="en">Less Than</rdfs:label>
    <rdfs:comment xml:lang="en">Holds a numeric value that is used to identify values of a property that are less than the value of this property.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#operatedValue"/>
    <rdfs:domain rdf:nodeID="A10"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="#contains">
    <rdfs:domain rdf:nodeID="A37"/>
    <rdfs:comment xml:lang="en">Holds one or more consecutive characters that are supposed to be found in strings of another property.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#operatedValue"/>
    <rdfs:label xml:lang="en">Contains</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A38">
    <rdf:rest rdf:nodeID="A39"/>
    <rdf:first rdf:resource="#ConditionalRule"/>
  </rdf:Description>
  <rdf:Description rdf:about="#dataRequirementsRelationship">
    <rdfs:comment xml:lang="en">The data requirements relationship property is an abstract property that holds object properties that are used to define internal and external relationships of data requirements.</rdfs:comment>
    <rdfs:label xml:lang="en">Data Requirements Relationship</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="#ConditionalSyntaxRule">
    <rdfs:label xml:lang="en">Conditional Syntax Rule</rdfs:label>
    <rdfs:subClassOf rdf:resource="#ConditionalRule"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">Conditional syntax rules define that the literal values of a tested property must follow a certain syntax when the value of another property obtains a certain state. E.g. the property foo:state must have a value with two capital letters when the property foo:country obtains the value "USA".</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#upperLimit">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:comment xml:lang="en">Specifies the upper limit a numeric property value shall obtain.</rdfs:comment>
    <rdfs:domain rdf:nodeID="A40"/>
    <rdfs:label xml:lang="en">Upper Limit</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/rights">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="#rank">
    <rdfs:label xml:lang="en">Rank</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:comment xml:lang="en">Position of a value in an ordinal scale.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#ScaleValue"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A41">
    <owl:unionOf rdf:nodeID="A42"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="#MultiPropertyRequirement">
    <rdfs:label xml:lang="en">Multi Property Requirement</rdfs:label>
    <rdfs:comment xml:lang="en">Multi property requirements are data requirements that consider values from two or more properties.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="#DataRequirement"/>
  </rdf:Description>
  <rdf:Description rdf:about="#affectedClass">
    <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">An affected class holds one or more instances with one or more data requirement violations.</rdfs:comment>
    <rdfs:range rdf:resource="#AffectedClass"/>
    <rdfs:label xml:lang="en">Affected Class</rdfs:label>
    <rdfs:domain rdf:resource="#DataRequirementViolation"/>
  </rdf:Description>
  <rdf:Description rdf:about="#AffectedClass">
    <rdfs:comment xml:lang="en">An affected class holds one or more affected instances</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Affected Class</rdfs:label>
    <rdfs:subClassOf rdf:resource="#DataElement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A43">
    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ordinal</rdf:first>
    <rdf:rest rdf:nodeID="A44"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A45">
    <rdf:rest rdf:nodeID="A46"/>
    <rdf:first rdf:resource="#Condition"/>
  </rdf:Description>
  <rdf:Description rdf:about="#cleansingProperty">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">Cleansing Property</rdfs:label>
    <rdfs:comment xml:lang="en">Cleansing properties are properties of data cleansing rules.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#unitOfMeasurement">
    <rdfs:domain rdf:resource="#DataQualityScore"/>
    <rdfs:comment xml:lang="en">Indicates the unit of measurement, e.g. of data quality scores. It is recommended to use UN/CEFACT common codes to specify the unit of measurement, e.g. "P1" stands for percent.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#Unit"/>
    <rdfs:label xml:lang="en">Unit of Measurement</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="#hasCondition1">
    <rdfs:label xml:lang="en">Has Condition 1</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">Connects a conditional rule with a certain condition.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#hasCondition"/>
    <rdfs:domain rdf:nodeID="A47"/>
  </rdf:Description>
  <rdf:Description rdf:about="#ConditionalRule">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Conditional Rule</rdfs:label>
    <rdfs:comment xml:lang="en">A conditional rule is a multi property requirement that can be used to specify that values of a tested property A must always obtain a certain state under the condition that values of another property B obtain a certain state (condition).</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#MultiPropertyRequirement"/>
  </rdf:Description>
  <rdf:Description rdf:about="#SyntacticAccuracy">
    <rdfs:subClassOf rdf:resource="#Accuracy"/>
    <rdfs:label xml:lang="en">Syntactic Accuracy</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">Syntactic accuracy is the degree to which data values of a property represent legal values and are free from syntax violations.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A34">
    <rdf:first rdf:resource="#FuncDepValueRule"/>
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
  </rdf:Description>
  <rdf:Description rdf:about="#expectedUpdateInterval">
    <rdfs:comment xml:lang="en">The expected update interval specifies the time interval in which instances of a specific class must be updated in order to be current.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#duration"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#UpdateRule"/>
    <rdfs:label xml:lang="en">Expected Update Interval</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A48">
    <owl:unionOf rdf:nodeID="A49"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/subject">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="#FuncDepValueRule">
    <rdfs:subClassOf rdf:resource="#ConditionalRule"/>
    <rdfs:label xml:lang="en">Functionally Dependent Value Rule</rdfs:label>
    <rdfs:comment xml:lang="en">A functionally dependent value rule defines that the value of a tested property must have a certain value when the values of other properties obtain certain states (conditions). E.g. the property foo:inStock must have value "true" when the property foo:availableAmount is greater than zero.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="#MatchingValueRule">
    <rdfs:label xml:lang="en">Matching Value Rule</rdfs:label>
    <rdfs:comment xml:lang="en">Matching value rules are multi property requirements in which the property values of other instances are used to identify data requirements violations in an instance.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="#MultiPropertyRequirement"/>
  </rdf:Description>
  <rdf:Description rdf:about="#BlacklistClass">
    <rdfs:comment xml:lang="en">A blacklist class holds instances with values that are disallowed for a certain data set. Thus, illegal value rules refer to such classes over the blacklist class property.</rdfs:comment>
    <rdfs:label xml:lang="en">Blacklist Class</rdfs:label>
    <rdfs:subClassOf rdf:resource="#DataElement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/creator">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="#SyntaxViolation">
    <rdfs:label xml:lang="en">Syntax Violation</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A syntax violation is a data requirement violation that occurs when a data value contains disallowed characters or does not match a predefined pattern.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#DataRequirementViolation"/>
  </rdf:Description>
  <rdf:Description rdf:about="#OutOfRangeValue">
    <rdfs:subClassOf rdf:resource="#DataRequirementViolation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Out of Range Value</rdfs:label>
    <rdfs:comment xml:lang="en">A value is out of range when it is not part of the legal value range or when it is part of the illegal value range.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#hasCondition2">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">Connects a conditional rule with a certain condition.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#hasCondition"/>
    <rdfs:domain rdf:nodeID="A3"/>
    <rdfs:label xml:lang="en">Has Condition 2</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="#greaterThan">
    <rdfs:domain rdf:nodeID="A50"/>
    <rdfs:subPropertyOf rdf:resource="#operatedValue"/>
    <rdfs:comment xml:lang="en">Holds a numeric value that is used to identify values of a property that are greater than the value of this property.</rdfs:comment>
    <rdfs:label xml:lang="en">Greater Than</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="#timeOfAssessment">
    <rdfs:domain rdf:resource="#DataQualityScore"/>
    <rdfs:comment xml:lang="en">The time of assessment represents the data and time when the data quality score was computed.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">Time Of Assessment</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </rdf:Description>
  <rdf:Description rdf:about="#currentValue">
    <rdfs:subPropertyOf rdf:resource="#cleansingProperty"/>
    <rdfs:label xml:lang="en">Current Value</rdfs:label>
    <rdfs:domain rdf:resource="#ValueSubstitution"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">The current value propertiy specifies the value to be removed.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A51">
    <rdf:first rdf:resource="#LegalValueRangeRule"/>
    <rdf:rest rdf:nodeID="A52"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A16">
    <rdf:first rdf:resource="#AffectedClass"/>
    <rdf:rest rdf:nodeID="A53"/>
  </rdf:Description>
  <rdf:Description rdf:about="#referenceProperty1">
    <rdfs:domain rdf:nodeID="A54"/>
    <rdfs:label xml:lang="en">Reference Property 1</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="#referenceProperty"/>
    <rdfs:comment xml:lang="en">The reference property points to the URI of a property that holds trusted values, i.e. values of high quality</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#PropertyCompletenessRule">
    <rdfs:subClassOf rdf:resource="#PropertyRequirement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Property Completeness Rule</rdfs:label>
    <rdfs:comment xml:lang="en">A property completeness rule is a data requirement that specifies that a certain property and/or its value must exist in all instances of a certain class.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A55">
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    <rdf:first rdf:resource="#MultiPropertyCleansingRule"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A13">
    <rdf:first rdf:resource="#TrustedProperty"/>
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
  </rdf:Description>
  <rdf:Description rdf:about="#DuplicateInstanceRule">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="#MatchingValueRule"/>
    <rdfs:comment xml:lang="en">A duplicate instance rule is a multi property requirement which specifies the properties that (when seen together) uniquely identify an entity. I.e. if the properties of two or more different instances represent the same state, then the instances represent the same entity. Thus, the instances are considered to be duplicates.</rdfs:comment>
    <rdfs:label xml:lang="en">Duplicate Instance Rule</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="#MissingValue">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Missing Value</rdfs:label>
    <rdfs:comment xml:lang="en">A missing value occurs when a property of an instance does not hold a value.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#MissingElement"/>
  </rdf:Description>
  <rdf:Description rdf:about="#hasCondition3">
    <rdfs:label xml:lang="en">Has Condition 3</rdfs:label>
    <rdfs:domain rdf:nodeID="A56"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">Connects a conditional rule with a certain condition.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#hasCondition"/>
  </rdf:Description>
  <rdf:Description rdf:about="#TestedClass">
    <rdfs:label xml:lang="en">Tested Class</rdfs:label>
    <rdfs:comment xml:lang="en">A tested class is a class that holds the instances that shall be analyzed for data quality problems.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="#DataElement"/>
  </rdf:Description>
  <rdf:Description rdf:about="#ValueSubstitution">
    <rdfs:subClassOf rdf:resource="#LiteralCleansingRule"/>
    <rdfs:label xml:lang="en">Value Substitution Rule</rdfs:label>
    <rdfs:comment xml:lang="en">A value substitution rule specifies a value to be removed and a new value that shall substitute the removed value.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="#unitName">
    <rdfs:domain rdf:resource="#Unit"/>
    <rdfs:label xml:lang="en">Unit Name</rdfs:label>
    <rdfs:comment xml:lang="en">Name of a unit of measurement.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A57">
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    <rdf:first rdf:resource="#TrustedClass"/>
  </rdf:Description>
  <rdf:Description rdf:about="#hasProperties">
    <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:nodeID="A58"/>
    <rdfs:comment xml:lang="en">Reconnects mapped classes and properties with each other.</rdfs:comment>
    <rdfs:label xml:lang="en">Has Properties</rdfs:label>
    <rdfs:domain rdf:nodeID="A15"/>
  </rdf:Description>
  <rdf:Description rdf:about="#FunctionalDependencyViolation">
    <rdfs:comment xml:lang="en">A functional dependency violation is a combination of different property values within the same instance that must not occur together. E.g. an instance describing the man “Peter Miller” has the value “Mr.” as salutation property, but the value “female” as sex.</rdfs:comment>
    <rdfs:label xml:lang="en">Functional Dependency Violation</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="#DataRequirementViolation"/>
  </rdf:Description>
  <rdf:Description rdf:about="#next">
    <rdfs:domain rdf:resource="#ScaleValue"/>
    <rdfs:range rdf:resource="#ScaleValue"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">Links an instances to the next instance in an ordinal scale.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#referenceProperty2">
    <rdfs:subPropertyOf rdf:resource="#referenceProperty"/>
    <rdfs:label xml:lang="en">Reference Property 2</rdfs:label>
    <rdfs:comment xml:lang="en">The reference property points to the URI of a property that holds trusted values, i.e. values of high quality</rdfs:comment>
    <rdfs:domain rdf:resource="#FuncDepReferenceRule"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A40">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <owl:unionOf rdf:nodeID="A59"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A39">
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    <rdf:first rdf:resource="#MultiPropertyCleansingRule"/>
  </rdf:Description>
  <rdf:Description rdf:about="#Accuracy">
