    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A data cleansing rule is an unambiguous rule that precisely specifies the required state of a data value.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#referenceProperty4">
