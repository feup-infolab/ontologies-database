  <rdf:Description rdf:about="#confidence">
    <rdfs:subPropertyOf rdf:resource="#reqMetadata"/>
    <rdfs:domain rdf:resource="#DataRequirement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:label xml:lang="en">Confidence</rdfs:label>
    <rdfs:comment xml:lang="en">The confidence property indicates how confident a rule creators is regarding the correctness of the rule.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="#affectedProperty">
