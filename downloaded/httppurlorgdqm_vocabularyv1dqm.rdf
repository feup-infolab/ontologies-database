    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="#weightedScore">
    <rdfs:label xml:lang="en">Weighted Score</rdfs:label>
