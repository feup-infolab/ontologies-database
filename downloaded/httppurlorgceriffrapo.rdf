    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has quotation date</rdfs:label>
    <rdfs:comment xml:lang="en">A data property linking a quotation to its date.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Laboratory">
    <rdfs:comment xml:lang="en">A facility in which research can be conducted.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Facility"/>
    <rdfs:label xml:lang="en">laboratory</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasMonetaryValue">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property enabling one to specify the monetary value of a financial entity such as a grant, a gift or a budget category.  Use in conjuction with frapo:hasCurrency.</rdfs:comment>
    <rdfs:label xml:lang="en">has value</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/AdmissionApplication">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">admission application</rdfs:label>
    <rdfs:comment xml:lang="en">The activity of applying for admission to some group or institution, for example a learned society or a university.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Application"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/awards">
    <rdfs:label xml:lang="en">awards</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/isAwardedBy"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:comment xml:lang="en">An object property linking an agent to something the agent awards, for example linking a funding agency to a grant, a university to a degree, or an organization to a prize.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Person">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N9a23e7e5175c4025aef17375c7977113">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <owl:unionOf rdf:nodeID="N483a0ba2ef8547fbb6c89b58a6c61f9d"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/FundingAgency">
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
    <rdfs:comment xml:lang="en">An organization that provides funding for projects or resources, often in the form of grants awarded on the basis of competitive application.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">funding agency</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/owns">
    <rdfs:comment xml:lang="en">An object property linking an agent to an entity owned by that agent.</rdfs:comment>
    <rdfs:label xml:lang="en">owns</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/indirect-costs">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:label xml:lang="en">indirect costs</rdfs:label>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdfs:comment xml:lang="en">A budget category to cover the indirect costs of running a project, for example heating and lighting, and the employment of non-project staff including administrators, accountants and cleaners.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Status">
    <rdfs:comment xml:lang="en">The status of something such as an application, an investigation or a project.</rdfs:comment>
    <rdfs:label xml:lang="en">status</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/date">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Manufacturer">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">An agent who manufactures goods or equipment.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label xml:lang="en">manufacturer</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasGrantNumber">
