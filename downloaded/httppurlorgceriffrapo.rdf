<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
   xmlns:dc="http://purl.org/dc/elements/1.1/"
   xmlns:owl="http://www.w3.org/2002/07/owl#"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasCurrencyCode">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasCode"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property specifying the three-letter currency code used to identify a currency, as defined by ISO 4217 (e.g. EUR or GBP) (see http://en.wikipedia.org/wiki/ISO_4217).  

Not to be confused with the symbol used with numbers to denote an amount of money in a particular currency (e.g.€500; £3.40)</rdfs:comment>
    <rdfs:label xml:lang="en">has currency code</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isEnabledBy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">An object property relating an entity to another entity that enables it, e.g. linking an investigation to a project that enables it.</rdfs:comment>
    <rdfs:label xml:lang="en">is enabled by</rdfs:label>
    <dc:description xml:lang="en">Domain and range constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasLanguageCode">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property specifying the two-letter or three-letter lower-case code used to identify a language (e.g. fr/fra for French, de/deu for Germany) (see http://en.wikipedia.org/wiki/List_of_ISO_639-1_codes).</rdfs:comment>
    <rdfs:label xml:lang="en">has language code</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasCode"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/purchasesFrom">
    <rdfs:comment xml:lang="en">An object property relating a purchaser to a supplier from whom the purchase is made.</rdfs:comment>
    <dc:description xml:lang="en">Domain and range constraints on this object property have been relaxed to permit it to be used in unforeseen ways.</dc:description>
    <rdfs:label xml:lang="en">purchases from</rdfs:label>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/sellsTo"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasPostalAddressLine">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasAgentInformation"/>
    <rdfs:label xml:lang="en">has postal address line</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/PostalAddress"/>
    <rdfs:comment xml:lang="en">A data property permitting specification of a single line of a postal address.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/created">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">A data property linking something to its date of creation.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:label xml:lang="en">has creation date</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Service">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/InfrastructureEntity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A service of help to others, often requiring funding, employees, equipment and/or facilities for its provision, for example a library service, a transportation service, a DNA sequencing service.</rdfs:comment>
    <rdfs:label xml:lang="en">service</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/AccountStatement">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A statement of income and expenditure for an account, typically presented as dated income receipts and outgoings against categories in a budget for that account.</rdfs:comment>
    <rdfs:label xml:lang="en">account statement</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/funds">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">funds</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/supports"/>
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/isFundedBy"/>
    <rdfs:comment xml:lang="en">An object property that links a grant to something that it funds (i.e. that it finances or pays for), or that links an agent providing funding to something that it funds.  This flexibility is given because of alternative usages of the English word 'funds'.  [See also frapo:awards and frapo:supports.]</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/FundingApplication">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Application"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="en">For the documents supporting a funding application, use fabio:GrantApplicationDocument and fabio:CaseForSupportDocument.</dc:description>
    <rdfs:comment xml:lang="en">The activity of applying for funding.</rdfs:comment>
    <rdfs:label xml:lang="en">funding application</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasInvoiceDate">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdfs:comment xml:lang="en">A data property linking an invoice to its date.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
    <rdfs:label xml:lang="en">has invoice date</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/Invoice"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/EmploymentContract">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Contract"/>
    <rdfs:label xml:lang="en">employment contract</rdfs:label>
    <rdfs:comment xml:lang="en">A contract of employment between a person and an organization.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasProfessionalName">
    <rdfs:comment xml:lang="en">An alternative name that a person uses for professional purposes, for example the pen name of an author, or a married woman's maiden name used as her professional name. </rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has alternate name</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasMilestone">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property that permits one to define a significant event (a 'milestone') in the progress of a project.</rdfs:comment>
    <rdfs:label xml:lang="en">has milestone</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasPersonalIdentifier">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <dc:description xml:lang="en">owl:equivalentProperty &lt;http://purl.org/spar/scoro/hasPersonalIdentifier&gt; .</dc:description>
    <rdfs:comment xml:lang="en">A unique and unambiguous identifier for an individual person within a given context.</rdfs:comment>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <rdfs:label xml:lang="en">has personal identifier</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    <dc:description>owl:equivalentProperty &lt;http://purl.org/spar/datacite/hasPersonalIdentifier&gt; .</dc:description>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasNameSuffix">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
    <rdfs:comment xml:lang="en">The letters that should be displayed after a person's name.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has name suffix</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Repository">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Facility"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">repository</rdfs:label>
    <dc:description xml:lang="en">May also be described as an archive, or as a database.</dc:description>
    <rdfs:comment xml:lang="en">A facility in which entities can be deposited for long-term preservation.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasSerialNumber">
    <rdfs:comment xml:lang="en">A data property linking something, for example a piece of equipment, to its serial number.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has serial number</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasCustomerNumber">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    <rdfs:label xml:lang="en">has customer number</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/Purchaser"/>
    <rdfs:comment xml:lang="en">A data property linking an agent to an identifying number for that agent, typically used by the accounting system of the supplier when issuing quotations, fulfilling orders and submitting invoices.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/publishing-costs">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:label xml:lang="en">publishing costs</rdfs:label>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdfs:comment xml:lang="en">A budget category to cover the cost of publishing outpots from a project, for example author fees for journal articles in an open access journal.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasManufactureDate">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property linking something to its date of manufacture.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdfs:label xml:lang="en">has data of manufacture</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isOutputOf">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dc:description xml:lang="en">Domain and range constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
    <rdfs:label xml:lang="en">is output of</rdfs:label>
    <rdfs:comment xml:lang="en">An object property linking something that is created or produced to the activity that created or produced it.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasAgentInformation">
    <rdfs:label xml:lang="en">has information</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:comment xml:lang="en">A data property linking an agent to information about that agent.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasStartDate">
    <rdfs:comment xml:lang="en">A data property linking something such as a project to its start date.</rdfs:comment>
    <rdfs:label xml:lang="en">has start date</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Invoice">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">An invoice for payment of goods or services.</rdfs:comment>
    <rdfs:label xml:lang="en">invoice</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Investigation">
    <dc:description xml:lang="en">Equivalent class in ISA-RDF: 'Investigation'.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">investigation</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Endeavour"/>
    <rdfs:comment xml:lang="en">An investigation is an endeavour that involves research, enabled by a project.  

The investigation relate to the scholarly and practical aspects of conducting the research itself, including the hypotheses being investigated, the experiments being undertaken, the data being gathered and analysed, the interpretations being drawn from the data, and the results obtained.  

In contrast, the project that enables it encompasses the administrative aspects of undertaking the endeavour, including the funding obtained, the people employed, and the management of the project in terms of deadlines and deliverables.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2006/timezone#TimeZone">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">A data property that permits the time zone relating to an entity to be specified.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has time zone</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasCurrency">
    <rdfs:label xml:lang="en">has currency</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property defining the currency used to specify a financial entity.  Use in conjection with frapo:hasMonetaryValue.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Business_angel">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/University">
    <rdfs:label xml:lang="en">university</rdfs:label>
    <rdfs:comment xml:lang="en">A university at which higher education and research is conducted.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasExchangeRate">
    <rdfs:comment xml:lang="en">A data property specifying the currency exchange rate between two specified currencies.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has exchange rate</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasApplicationDeadline">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <dc:description xml:lang="en">owl:equivalentProperty fabio:hasApplicationDeadline .</dc:description>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/Application"/>
    <rdfs:comment xml:lang="en">The date by which an application, for example a job application, a fellowship application or a grant application, has to be submitted if it is to be considered for acceptance or funding.</rdfs:comment>
    <rdfs:label xml:lang="en">has application deadline</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ResearchGroup">
    <rdfs:label xml:lang="en">research group</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Group"/>
    <rdfs:comment xml:lang="en">A group of people, usually within a university department or a research institute, and typically under a single group leader, dedicated to working together to undertake research on one or more topics. May also be called a research unit.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Quotation">
    <rdfs:label xml:lang="en">quotation</rdfs:label>
    <rdfs:comment xml:lang="en">A formal statement of promise (usually provided in response to a request for quotation) by potential supplier to supply goods or services at specified prices and within a specified period. Acceptance of the quotation by the buyer constitutes an agreement binding on both parties.  (Shortened from http://www.businessdictionary.com/definition/quotation.html).
</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/FinancialControlSystem">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">financial control system</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/ComputationalService"/>
    <rdfs:comment xml:lang="en">A software system controlling the financial activities of an organization, for example determining full economic cost budgets for grant applications, and managing the funding of funded projects.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/full-economic-costs">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">A budget category to cover all the direct and indirect costs of a project. </rdfs:comment>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <dc:description xml:lang="en">Frequently abbreviated 'FEC'.</dc:description>
    <rdfs:label xml:lang="en">full economic costs</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ResearchInstitute">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">An organization primarily dedicated to undertaking research, that might be part of a university or of a commercial company, or might be independently funded by a research funding agency.  </rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
    <rdfs:label xml:lang="en">research institute</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/rejected">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:label xml:lang="en">rejected</rdfs:label>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/Status"/>
    <rdfs:comment xml:lang="en">The status of something such as an application as having been rejected.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasCompletionDate">
    <rdfs:comment xml:lang="en">A data property linking something such as a building or a contract to its date of completion.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has completion date</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/BudgetedAmount">
    <rdfs:comment xml:lang="en">The amount of money originally available to spend in a particular budget category.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/BudgetInformation"/>
    <rdfs:label xml:lang="en">budgeted amount</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/manufactures">
    <rdfs:comment xml:lang="en">An object property relating a manufacturer to a manufactured entity.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/Manufacturer"/>
    <rdfs:label xml:lang="en">manufactures</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isDonatedBy">
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label xml:lang="en">is donated by</rdfs:label>
    <rdfs:comment xml:lang="en">An object property linking a gift to the benefactor who provided it.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dc:description xml:lang="en">Domain constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/NotForProfitOrganization">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
    <rdfs:comment xml:lang="en">An organization that does not primarily exist to make profit, for example a charity or charitable foundation, a learned society, or a standards body such as the World Wide Web Consortium.</rdfs:comment>
    <rdfs:label xml:lang="en">not-for-profit organization</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Department">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
    <rdfs:comment xml:lang="en">A department or sub-section within a larger organization, for example a university academic department.</rdfs:comment>
    <rdfs:label xml:lang="en">department</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasORCID">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalIdentifier"/>
    <dc:description xml:lang="en">owl:equivalentProperty &lt;http://purl.org/spar/scoro/hasORCID&gt; .</dc:description>
    <rdfs:label xml:lang="en">has ORCID</rdfs:label>
    <rdfs:comment xml:lang="en">A data property relating an individual to his/her Open Researcher and Contributor ID (ORCID), an alphanumeric identifier for individual scholars described at http://about.orcid.org/.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ExpenditureToDate">
    <rdfs:comment xml:lang="en">Total money expended to date from an an account, or from a particular budget category.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">expenditure to date</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Expenditure"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/usesService">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="http://purl.org/cerif/frapo/Service"/>
    <rdfs:label xml:lang="en">uses service</rdfs:label>
    <dc:description xml:lang="en">Domain constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
    <rdfs:comment xml:lang="en">An object property linking an activity such as an expedition, a research project or an investigation, to a service employed in that activity.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ScholarshipApplication">
    <rdfs:comment xml:lang="en">The activity of applying for an educational scholarship.</rdfs:comment>
    <rdfs:label xml:lang="en">scholarship application</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Application"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/directly-incurred-costs">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdfs:comment xml:lang="en">A budget category to cover all costs directly incurred because of a legitimate expenditure on a project, for example the purchase of reagents or the employment of a post-doctoral researcher.</rdfs:comment>
    <dc:description xml:lang="en">Also known as the 'direct costs' of the project.</dc:description>
    <rdfs:label xml:lang="en">directly incurred costs</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/SME">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">sme</rdfs:label>
    <rdfs:comment xml:lang="en">A small to medium enterprise, having up to 250 employees.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Company"/>
    <rdfs:label xml:lang="en">small to medium enterprise</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Investment">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">An investment of money, generally in stocks and shares, or in property, for the purpose of safeguarding the capital and yielding an income.</rdfs:comment>
    <rdfs:label xml:lang="en">investment</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Grant">
    <rdfs:label xml:lang="en">grant</rdfs:label>
    <rdfs:comment xml:lang="en">A financial award by a funding agency for a particular project or purpose, usually awarded on the basis of a competitive application.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Funding"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasApplicationOutcome">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has application outcome</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:comment xml:lang="en">A data property that permits the outcome of an application to be described, for example that a job has been offered or that a grant has been awarded.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/purchases">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">An object property relating a purchaser to a purchased entity.</rdfs:comment>
    <dc:description xml:lang="en">Range constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label xml:lang="en">purchases</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasEndDate">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:label xml:lang="en">has end date</rdfs:label>
    <rdfs:comment xml:lang="en">A data property linking something such as a project to its end date.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/description">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasRegistrationDeadline">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdfs:label xml:lang="en">has registration deadline</rdfs:label>
    <rdfs:comment xml:lang="en">The date by which registration must be made, for example renewal of an internet domain name.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasProvenance">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property that permits the provenance of an entity to be described.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:label xml:lang="en">has provenance</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/specifiesExpediture">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dc:description xml:lang="en">Range constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
    <rdfs:comment xml:lang="en">An object property relating an agent or a budget to the expenditure specified by that agent or budget.</rdfs:comment>
    <rdfs:label xml:lang="en">specifies expenditure</rdfs:label>
    <rdfs:domain rdf:nodeID="Ndab0cd38bb1e47f282a8daf32582e022"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/familyNameInitial">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/initial"/>
    <rdfs:comment xml:lang="en">A data property permitting specification of the initial(s) of a person's family name.</rdfs:comment>
    <rdfs:label xml:lang="en">family name initial</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Library">
    <rdfs:label xml:lang="en">library</rdfs:label>
    <rdfs:comment xml:lang="en">A facility in which printed documents (for example books and journals) are deposited for long-term preservation and access.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Repository"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasPostalAddress">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasAgentInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property identifying a postal address for an agent.</rdfs:comment>
    <rdfs:label xml:lang="en">has postal address</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasFunderIdentifier">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    <rdfs:label xml:lang="en">has funder identifer</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/FundingAgency"/>
    <rdfs:comment xml:lang="en">A data property linking a funding agency to an identifier for that funding agency.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/accepted">
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/Status"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">The status of something such as an application as having been accepted.</rdfs:comment>
    <rdfs:label xml:lang="en">accepted</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/evaluates">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/isEvaluatedBy"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label xml:lang="en">evaluates</rdfs:label>
    <rdfs:comment xml:lang="en">An object property linking an agent to an entity which that agent evaluates, for example a funding agency that is evaluating a grant application to determine whether the proposed project is worthy of funding, or a publisher evaluating a manuscript to determine whether it is suitable for publication.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/givenNameInitial">
    <rdfs:comment xml:lang="en">A data property permitting specification of the initial(s) of a person's given name(s).</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">given name initial</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/initial"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/givenname">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/directly-allocated-costs">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdfs:comment xml:lang="en">A budget category to cover those costs normally borne by an institution, that are assigned to the budget of an externally funded project to cover the value of contributions to that project, for example to pay for the salary of a permanent member of staff seconded to work on the project, to compensate the employing organization for the loss of that person's efforts on other activities that he or she would otherwise be undertaking were it not for that secondment.</rdfs:comment>
    <rdfs:label xml:lang="en">directly allocated costs</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/title">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Income">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">income</rdfs:label>
    <rdfs:comment xml:lang="en">Money coming into an account.  May be associated with a particular budget category.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/BudgetInformation"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ownsIRPOf">
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/owns"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">owns IPR of</rdfs:label>
    <rdfs:comment xml:lang="en">An object property linking an agent who owns IPR arising from or relating to an entity to that entity.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasHonorificSuffix">
    <rdfs:comment xml:lang="en">The letters that should be displayed after a person's name to indicate qualification, membership or fellowship in a professional society, honours or elite status, e.g. MRCVS, FRCS, FRS, OBE, Kt.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasNameSuffix"/>
    <rdfs:label xml:lang="en">post-nominals</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isSubmittedBy">
    <rdfs:comment xml:lang="en">An object property linking an entity that is submitted (to some third party) to the agent who submits it.</rdfs:comment>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/submits"/>
    <rdfs:label xml:lang="en">is submitted by</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ResearchInformationSystem">
    <rdfs:comment xml:lang="en">A database containing administrative information about the research undertaken in an institution, and the publications and other outputs from such research.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">research information system</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/ComputationalService"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/facilities-costs">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">A budget category to cover the costs of maintaining facilities used by a project, for example an electron microscopy facility.</rdfs:comment>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdfs:label xml:lang="en">facilities costs</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasOutput">
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/isOutputOf"/>
    <rdfs:comment xml:lang="en">An object property linking an activity such as a project or endeavour to something that is created or produced as a result of that activity.  Examples include a building created as a result of a building project, and a journal article published to describe the results of a research investigation.  </rdfs:comment>
    <dc:description xml:lang="en">Domain and range constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">has output</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Document">
    <dc:description xml:lang="en">Ontology terms and definitions for many documents relevant to FRAPO are given as 'Expression' classes in FaBiO, the FRBR-aligned Bibliographic Ontology, e.g. fabio:CallForApplications, fabio:DataManagementPlan, fabio:GrantApplicationDocument, fabio:ProjectPlan.  Others are given as sub-classes of this class.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/EmploymentApplication">
    <rdfs:label xml:lang="en">employment application</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">The activity of applying for a job.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Application"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/providesFractionOfTotalFunding">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">An object property linking funding to a budget, specifying what fraction of the total budget is provided by that source of funding.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/Funding"/>
    <rdfs:range rdf:resource="http://purl.org/cerif/frapo/Budget"/>
    <rdfs:label xml:lang="en">percentage of total funding</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasCurriculumVitae">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">An object property linking a person to an on-line version of that person's curriculum vitae.</rdfs:comment>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <rdfs:label xml:lang="en">has curriculum vitae</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Project">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:comment xml:lang="en">In FRAPO, a project is an administrative entity that enables an endeavour such as a research investigation.  A project relates to the administrative aspects of undertaking an endeavour, including the funding obtained, the people employed, and the management of the project in terms of deadlines and deliverables.  In contrast, an endeavour such as a research investigation relate to the academic and practical aspects of undertaking that endeavour - thus for a research investigation: the research itself, including the hypotheses being investigated, the experiments being undertaken, the data being gathered and analysed, the interpretations being drawn from the data, and the results obtained.</rdfs:comment>
    <dc:description xml:lang="en">Super-class of cerif:Project.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">project</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/software-costs">
    <rdfs:comment xml:lang="en">A budget category to cover the costs of software licenses required by a project.</rdfs:comment>
    <rdfs:label xml:lang="en">software costs</rdfs:label>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/RegistrationAgency">
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
    <rdfs:comment xml:lang="en">An institution or organization officially appointed by a Registration Authority that acts to register things. For example, CrossRef is a registration agency for DOIs relating to documents, while DataCite is a registration agency for DOIs relating to datasets, acting for the Registration Authority for DOIs, namely the International DOI Foundation (IDF).</rdfs:comment>
    <rdfs:label xml:lang="en">registration agency</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasPersonalInformation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property linking a person to information about that person.</rdfs:comment>
    <rdfs:label xml:lang="en">has contact information</rdfs:label>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasAgentInformation"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Division">
    <rdfs:comment xml:lang="en">A grouping of similar departments within a larger organization.  In the university context, also known as a school or a faculty, for example the Humanities Division, the Mathematics Faculty, or the School of Medicine.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">division</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasPostCode">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property defining the postal code of an address.</rdfs:comment>
    <rdfs:label xml:lang="en">has post code</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasCode"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasDeliveryDate">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:label xml:lang="en">has delivery date</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">The date on which something is delivered, or is due to be delivered.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/service-costs">
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:label xml:lang="en">service costs</rdfs:label>
    <rdfs:comment xml:lang="en">A budget category to cover the costs of services used by a project, for example a DNA sequencing service, or an equipment maintenance service.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasDegreeSuffix">
    <rdfs:comment xml:lang="en">The letters that should be displayed after a person's name to indicate an academic degree, e.g. BA, BVSc, PhD, DSc.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasNameSuffix"/>
    <rdfs:label xml:lang="en">has degrees</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasDecisionDate">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:seeAlso rdf:resource="http://purl.org/spar/fabio/hasDecisionDate"/>
    <rdfs:comment xml:lang="en">The date on which a particular action is or will be decided upon, or when an application, such as a grant application, is or will be approved or rejected. [See also frapo:hasAwardDate.]</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:label xml:lang="en">has decision date</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <dc:description xml:lang="en">This same data property appears in FaBiO, the FRBR-aligned Bibliographic Ontology (http://purl.org/spar/fabio/).</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isFundingAgencyFor">
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/FundingAgency"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">is funding agency for</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/supports"/>
    <rdfs:comment xml:lang="en">An object property linking a funding agency to an entity for which it provides funding.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2000/01/rdf-schema#seeAlso">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/surname">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/AvailableFunds">
    <rdfs:comment xml:lang="en">The amount available to spend in a budget category, equalling the income minus the expenditure to date plus future expenditure commitments. </rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/BudgetInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">available funds</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isFundedBy">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/isSupportedBy"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">An object property linking something to the funding that funds it (i.e. that finances or pays for it), or to the funding agency providing that funding.  This flexibility is given because of alternative usages of the English word 'funds'.  [See also frapo:isAwardedBy and frapo:hasFundingAgency and frapo:isSupportedBy.]</rdfs:comment>
    <rdfs:label xml:lang="en">is funded by</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Equipment">
    <rdfs:label xml:lang="en">equipment</rdfs:label>
    <rdfs:comment xml:lang="en">An item of equipment or apparatus, or a laboratory instrument, for example a refrigerated centrifuge.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/InfrastructureEntity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasModelNumber">
    <rdfs:label xml:lang="en">has model number</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    <rdfs:comment xml:lang="en">A data property linking something, for example a piece of equipment, to its model number.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/consumables-costs">
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">A budget category to cover the costs of purchasing consumables.</rdfs:comment>
    <rdfs:label xml:lang="en">consumables costs</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:nodeID="Nf29784501921496ba49a95a4c449fa0e">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <owl:unionOf rdf:nodeID="Nc535475a3ab64c78b1ea2c2af0b44dab"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/donates">
    <rdfs:label xml:lang="en">donates</rdfs:label>
    <dc:description xml:lang="en">Range constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">An object property linking a benefactor to a gift.</rdfs:comment>
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/isDonatedBy"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/DocumentRepository">
    <dc:description xml:lang="en">May also be described as a document archive, or a digital library.</dc:description>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Repository"/>
    <rdfs:comment xml:lang="en">A facility in which digital documents are deposited for long-term preservation and (optional) publication.</rdfs:comment>
    <rdfs:label xml:lang="en">document repository</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="Ndab0cd38bb1e47f282a8daf32582e022">
    <owl:unionOf rdf:nodeID="Nc25fc3cab73c4167b069fe62c12fce9c"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isSuppliedBy">
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/supplies"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="http://purl.org/cerif/frapo/Supplier"/>
    <rdfs:comment xml:lang="en">An object property relating an entity that is supplied, or an agent to whom an entity is supplied, to the supplier.</rdfs:comment>
    <rdfs:label xml:lang="en">is supplied by</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Vendor">
    <rdfs:label xml:lang="en">vendor</rdfs:label>
    <rdfs:comment xml:lang="en">An agent who supplies goods or services in exchange for payment, in response to receipt of payment or of an official purchase order from a purchaser that is responded to by submission of an invoice for what is supplied.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Supplier"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasProjectIdentifier">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
    <rdfs:label xml:lang="en">has project identifier</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    <rdfs:comment xml:lang="en">A data property linking a project to an identifier for that project.</rdfs:comment>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Project"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasFundingAgency">
    <rdfs:range rdf:resource="http://purl.org/cerif/frapo/FundingAgency"/>
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/isFundingAgencyFor"/>
    <rdfs:label xml:lang="en">has funding agency</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/isSupportedBy"/>
    <rdfs:comment xml:lang="en">An object property linking something to a funding agency that provides funding for it.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/sellsTo">
    <dc:description xml:lang="en">Domain and range constraints on this object property have been relaxed to permit it to be used in unforeseen ways.</dc:description>
    <rdfs:label xml:lang="en">sells to</rdfs:label>
    <rdfs:comment xml:lang="en">An object property relating a supplier to a purchaser to whom an entity is sold.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/family_name">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/RegistrationAuthority">
    <rdfs:label xml:lang="en">registration authority</rdfs:label>
    <rdfs:comment xml:lang="en">An agent with responsibility for registering and maintaining international standards, internet domain names, patents, controlled vocabularies and other similar entities required for reliable international cooperatative activities. For example, the International DOI Foundation (IDF) is the Registration Authority for DOIs.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasCatalogNumber">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property linking something to its catalog number.</rdfs:comment>
    <rdfs:label xml:lang="en">has catalog number</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasExpertise">
    <rdfs:label xml:lang="en">has expertise</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasAgentInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property identifying the area(s) of expertise and skills posessed by an individual or an organization.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/HostInstitution">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
    <rdfs:comment xml:lang="en">An institution that acts as a host, for example of a project, a resource or a person, for example by providing accommodation, services, operational support and/or funding.</rdfs:comment>
    <rdfs:label xml:lang="en">host institution</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/estates-costs">
    <rdfs:comment xml:lang="en">A budget category to cover the 'overhead' costs of maintaining the premises in which work is undertaken.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdfs:label xml:lang="en">estates costs</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasAwardDate">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">The date on which notification is given or received that something, for example a scholarship or a grant, has been awarded.  [See also frapo:hasDecisionDate.]</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:label xml:lang="en">has award date</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasFaxNumber">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasAgentInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has fax number</rdfs:label>
    <rdfs:comment xml:lang="en">A data property identifying a Fax number for an agent.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/enables">
    <rdfs:comment xml:lang="en">An object property relating an entity to another entity that it enables, e.g. linking a project to an endeavour that the project enables.</rdfs:comment>
    <dc:description xml:lang="en">Domain and range constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
    <rdfs:label xml:lang="en">enables</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/isEnabledBy"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasExpiryDate">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdfs:label xml:lang="en">has expiry date</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">A data property linking something, for example a drug, to its expiry date, beyond which the manufacture recommends that it should not be used.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ConferenceFee">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Fee"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A fee paid to attend a conference.</rdfs:comment>
    <rdfs:label xml:lang="en">conference fee</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Funding">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
    <rdfs:comment xml:lang="en">An amount of money available to finance some project or activity. </rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">funding</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Budget">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">budget</rdfs:label>
    <rdfs:comment xml:lang="en">A structured statement of how funds are to be used for expenditure in defined budget categories.  A budget is created, for example, when applying for funding for a project, and is subsequently used to manage the expenditure from the awarded grant for that project.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/name">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isEmployedBy">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:comment xml:lang="en">An object property relating an employed agent, typically a person or an organization, to an employing agent.</rdfs:comment>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <dc:description xml:lang="en">This property should not be used for the other meaning of 'employ', meaning to use something, e.g. 'Great skill is employed by master craftsmen . . .'/</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label xml:lang="en">is employed by</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/contributor">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/feeOrFree">
    <rdfs:label xml:lang="en">fee or free</rdfs:label>
    <rdfs:comment xml:lang="en">A data property defining whether participation in an event is free, or requires payment of a fee.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/rights">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/MaterialOutput">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Output"/>
    <rdfs:label xml:lang="en">material output</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A material or physical output created or produced as a result of a project or an endeavour such as a research investigation. Examples include a building constructed as a result of a building project, and research outputs such as a cell line, a mouse mutant or a new form of microscope.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/supplies">
    <rdfs:comment xml:lang="en">An object property relating a supplier to a supplied entity, or to an agent to whom the entity is supplied.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <dc:description xml:lang="en">Domain constraints on this object property have been relaxed to permit it to be used in unforeseen ways.</dc:description>
    <rdfs:label xml:lang="en">supplies</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasPurchaseOrderNumber">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/PurchaseOrder"/>
    <rdfs:label xml:lang="en">has purchase order number</rdfs:label>
    <rdfs:comment xml:lang="en">A data property identifying an official purchase order issued by a purchasing agent to a supplier of goods or services, and used by the supplier to identify the order when delivering goods or submitting an invoice.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Tender">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">Sealed bid or offer document submitted in response to a request for tenders, containing detailed information on costs, requirements and terms associated with a potential contract.  (Adapted from http://www.businessdictionary.com/definition/tender.html).</rdfs:comment>
    <rdfs:label xml:lang="en">tender</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/non-staff-costs">
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:label xml:lang="en">non-staff costs</rdfs:label>
    <rdfs:comment xml:lang="en">A budget category to cover all the costs associated with a project that do not involve the employment of staff by the institution, thus, for example, including consumables purchase, travel costs and consultancy fees, but excluding the salary, estates and indirect costs associated with the allocation of the PI's time to the project and the direct employment of a project manager.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isAwardedBy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <dc:description xml:lang="en">Domain constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label xml:lang="en">is awarded by</rdfs:label>
    <rdfs:comment xml:lang="en">An object property linking something awarded to the agent making the award, for example linking a grant to a funding agency, a degree to a university, or a prize to the awarding organization.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/usesEquipment">
    <rdfs:comment xml:lang="en">An object property linking an activity such as an expedition, a research project or an investigation, to an item of equipment employed in that activity.</rdfs:comment>
    <rdfs:label xml:lang="en">uses equipment</rdfs:label>
    <dc:description xml:lang="en">Domain constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:range rdf:resource="http://purl.org/cerif/frapo/Equipment"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ConsultancyAgreement">
    <rdfs:comment xml:lang="en">A contract between an employer and a consultant, detailing the terms of the consultancy.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Contract"/>
    <rdfs:label xml:lang="en">consultancy agreement</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/birthday">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/nick">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasTemporalDuration">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:comment xml:lang="en">A data property enabling one to define the temporal duration of some entity such as an event or a project.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has temporal duration</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/supports">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:nodeID="Nf29784501921496ba49a95a4c449fa0e"/>
    <rdfs:label xml:lang="en">supports</rdfs:label>
    <rdfs:comment xml:lang="en">An object property linking an agent to something that the agent supports by financial or other means.</rdfs:comment>
    <dc:description xml:lang="en">This property is not intended for use to describe mechanical or physical support, as in 'The plinth supports the statue'. </dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasCallIdentifier">
    <rdfs:comment xml:lang="en">A data property specifying the identifying number of a call for applications issued by a funding agency.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has call identifier</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Bursary">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">bursary</rdfs:label>
    <rdfs:comment xml:lang="en">An amount of money given to a person by an organization, for example by a university to a university student in need to enable that student to continue his/her studies.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Funding"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/contributions-in-kind">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:label xml:lang="en">contributions in kind</rdfs:label>
    <rdfs:comment xml:lang="en">A budget category to categorize the financial worth of contributions in kind (e.g. staff effort, access to facilities, sharing of know-how) given to a project by an agent, for example by the host institution or by a commerical partner in an academic project. </rdfs:comment>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Contract">
    <rdfs:label xml:lang="en">contract</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
    <rdfs:comment xml:lang="en">A legally binding agreement between parties.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasAccountNumber">
    <rdfs:comment xml:lang="en">A data property linking a financial account to an identifying number for that account.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/Account"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has account number</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ServiceContract">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Contract"/>
    <rdfs:comment xml:lang="en">A contract for the provision of a service such as a cleaning service, or for the regular servicing, maintenance and repair of some item of equipment or infrastructure.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">service contract</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/business-angel">
    <rdfs:label xml:lang="en">business angel</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <rdfs:comment xml:lang="en">A person willing to invest in start-up companies.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Purchase">
    <rdfs:label xml:lang="en">purchase</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">The payment for obtaining something, for example property, goods or services.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Output">
    <rdfs:comment xml:lang="en">Something that is created or produced as a result of a project or an endeavour. Examples include a building resulting from a building project, educated students graduating from an educational programme, and a journal article published as a result of a research project.  Research outputs can also include reports, datasets, experimental protocols, workflows and mathematical models, and material outputs such as a cell line, a mouse mutant or a new form of microscope.</rdfs:comment>
    <rdfs:label xml:lang="en">output</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Equipment"/>
    <dc:description xml:lang="en">Ontology terms and definitions for most publishable project outputs are given as 'Expression' classes in FaBiO, the FRBR-aligned Bibliographic Ontology, e.g. fabio:JournalArticle, fabio:DataSet.  For 'deliverables', use frapo:Deliverable.  For material outputs, use frapo:MaterialOutput.</dc:description>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ArticleProcessingCharge">
    <rdfs:comment xml:lang="en">A fee levied by a publisher on an author or an institution to cover the cost of publishing an Open Access journal article.  Also knows as Article Processing Fee, Article Publication Charge, Author Processing Charge or Author Publishing Charge.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">article processing charge</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Fee"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Payment">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A financial payment made, for example in exchange for something purchased such as property, goods or services, or to refund expenses incurred.</rdfs:comment>
    <rdfs:label xml:lang="en">payment</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/unfunded">
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/Status"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">The status of something such as an investigation or a project as being unfunded.</rdfs:comment>
    <rdfs:label xml:lang="en">unfunded</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasFamilialSuffix">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasNameSuffix"/>
    <rdfs:comment xml:lang="en">The letters that may be displayed after a person's name to indicate familial status, e.g. Sn., Jn., III, 3rd.</rdfs:comment>
    <rdfs:label xml:lang="en">has familiar suffix</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/other-costs">
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:label xml:lang="en">other costs</rdfs:label>
    <rdfs:comment xml:lang="en">A budget category to cover costs not covered in other budget categories.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/SpinOffCompany">
    <rdfs:comment xml:lang="en">A company that has been established by a university or research institute to mature and market the results of research conducted at that research institution.</rdfs:comment>
    <rdfs:label xml:lang="en">spin-off company</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Company"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasCountry">
    <rdfs:label xml:lang="en">has country</rdfs:label>
    <rdfs:comment xml:lang="en">A data property specifying a country, for example as the location of an event.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/initial">
    <rdfs:comment xml:lang="en">A data property permitting specification of the initial(s) of a person's name.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">initials</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Commitments">
    <rdfs:label xml:lang="en">commitments</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/BudgetInformation"/>
    <rdfs:comment xml:lang="en">Money set aside to honour expenditures already made but for which no invoices have yet been received, or expenditures planned for the future, for example for salaries.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/BudgetCategory">
    <rdfs:label xml:lang="en">budget category</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
    <rdfs:comment xml:lang="en">A category of expenditure within a budget, determining what the allocated money can be spent on, for example salaries, equipment, travel.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/">
    <dc:rights xml:lang="en">This work is distributed under a Creative Commons Attribution License (http://creativecommons.org/licenses/by/3.0/).</dc:rights>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <dc:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">FRAPO, the Funding, Research Administration and Projects Ontology</dc:title>
    <dc:contributor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Silvio Peroni</dc:contributor>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.1.1</owl:versionInfo>
    <owl:imports rdf:resource="http://purl.org/swan/2.0/foaf-essential/"/>
    <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">FRAPO is an ontology for use to describe research project administrative information and to work with CRIS (Current Research Information Systems). It can be used for the characterization of grant applications, funding bodies, projects, project partners, etc.  It can also be used to describe other types of projects, for example building projects and educational projects.  

FRAPO imports FOAF and is designed to work with SCoRO, the Scholarly Contributions and Roles Ontology (http://purl.org/spar/scoro/), which is used to characterize the contributions and time-limited roles that may exist between agents such as people and organizations and entities such as projects, research investigations or research outputs.  

FaBiO, the FRBR-alightned bibliographic ontology (http://purl.org/spar/fabio/), provides terms for documents of relevance, such as grant applications, project plans, project reports, datasets and journal articles, which are therefore not included within FRAPO.

FRAPO is not intended to describe the details of research investigations enabled by funded research projects, nor their component studies and assays, which are the domain of a complementary ontology, ISA-RDF (under development - add URL).</dc:description>
    <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">David Shotton</dc:creator>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">FRAPO, the Funding, Research Administration and Projects Ontology, is a CERIF-compliant ontology  written in OWL 2 DL for describing research project administrative information.  This ontology is available at http://purl.org/cerif/frapo/, and uses the namespace prefix 'frapo'.</rdfs:comment>
    <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2017-09-04</dc:date>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Account">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A mechanism for administering money, typically owned by an agent and dedicated to a particular purpose, for example the equipment account of a university department, to be used only for the purchase or maintenance of items of equipment. </rdfs:comment>
    <rdfs:label xml:lang="en">account</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/building-costs">
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">A budget category to cover the costs of constructing, renovating or modifying a physical building.</rdfs:comment>
    <rdfs:label xml:lang="en">building costs</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasPurchaseDate">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:comment xml:lang="en">A data property linking something to its purchase date.</rdfs:comment>
    <rdfs:label xml:lang="en">has purchase date</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/FinancialEntity">
    <rdfs:comment xml:lang="en">An entity primarily to do with money.</rdfs:comment>
    <rdfs:label xml:lang="en">financial entity</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/identifier">
    <rdfs:comment xml:lang="en">A unique and unambiguous reference to the resource within a given context.

Recommended best practice is to identify the resource by means of a string conforming to a formal identification system.</rdfs:comment>
    <rdfs:label xml:lang="en">identifier</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasReferenceNumber">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    <rdfs:comment xml:lang="en">A data property linking something to a reference number for that item.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has reference number</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Gift">
    <rdfs:label xml:lang="en">gift</rdfs:label>
    <rdfs:comment xml:lang="en">A gift, donation, benefaction or legacy, typically of money.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Funding"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasDispatchDate">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdfs:label xml:lang="en">has dispatch date</rdfs:label>
    <rdfs:comment xml:lang="en">The date on which something, for example an item of equipment, is dispatched by the supplier.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasAcronym">
    <rdfs:comment xml:lang="en">A data property linking an entity to an acronym used to identify it, based on its full name or title.  For example, this ontology has the full name 'Funding, Research Administration and Projects Ontology' and the acronym 'FRAPO'.</rdfs:comment>
    <rdfs:label xml:lang="en">has acronym</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ComputationalService">
    <rdfs:comment xml:lang="en">A service provided by a computer system.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">computational service</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Service"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/BudgetInformation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">budget information</rdfs:label>
    <rdfs:comment xml:lang="en">Information about an aspect of a budget.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/AnnualTurnover">
    <rdfs:label xml:lang="en">annual turnover</rdfs:label>
    <rdfs:comment xml:lang="en">The anual financial turnover of an organization (in a specified currency).</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/owl:sameAs">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="Nc25fc3cab73c4167b069fe62c12fce9c">
    <rdf:first rdf:resource="http://purl.org/cerif/frapo/Budget"/>
    <rdf:rest rdf:nodeID="N91244b9d49ca444b804b86a6437bc2cf"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ConsortiumAgreement">
    <rdfs:label xml:lang="en">consortium agreement</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Contract"/>
    <rdfs:comment xml:lang="en">A formal agreement between members of a consortium, for example between research groups at different universities engaged in a multi-partner research project.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/collaboratesWith">
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label xml:lang="en">collaborates with</rdfs:label>
    <rdfs:comment xml:lang="en">An object property linking agents, for example different organizations, or people typically from different groups or organizations, that work together on a common task, project or endeavour, or towards a common goal.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/appliesFor">
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:comment xml:lang="en">An object property linking an agent to something for which that agent applies, for example a job, membership of an organization, or a grant from a funding agency.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">applies for</rdfs:label>
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/isAppliedForBy"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isPurchasedBy">
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:comment xml:lang="en">An object property relating a purchased entity to its purchaser.</rdfs:comment>
    <dc:description xml:lang="en">Domain constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
    <rdfs:label xml:lang="en">was purchased by</rdfs:label>
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/purchases"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Owner">
    <rdfs:comment xml:lang="en">The agent who owns an entity.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label xml:lang="en">owner</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasBirthDate">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:comment xml:lang="en">A data property permitting specification of the birth date of a person.</rdfs:comment>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/birthday"/>
    <rdfs:label xml:lang="en">has birth date</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Facility">
    <rdfs:comment xml:lang="en">A place or installation built or designed to serve a specific function or provide a specific service, for example an animal breeding centre, a DNA sequencing facility or a confocal imaging laboratory.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/InfrastructureEntity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">facility</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Expenditure">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/BudgetInformation"/>
    <rdfs:comment xml:lang="en">Money going out of an account, because of expenditure on various entities. May be associated with a particular budget category.</rdfs:comment>
    <rdfs:label xml:lang="en">expenditure</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Fellowship">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">The stipend paid to a fellow in a college or university.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Stipend"/>
    <rdfs:label xml:lang="en">fellowship</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N2ccc394e5739406ca6ea0dacc27981ac">
    <rdf:first rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Endeavour">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">In FRAPO, an endeavour is an activity such as a research investigation that is enabled by a project.  An endeavour relate to the academic and practical aspects of undertaking that endeavour - thus for a research investigation the research itself, including the hypotheses being investigated, the experiments being undertaken, the data being gathered and analysed, the interpretations being drawn from the data, and the results obtained.  In contrast, the project relates to the administrative aspects of undertaking an endeavour, including the funding obtained, the people employed, and the management of the project in terms of deadlines and deliverables.</rdfs:comment>
    <rdfs:label xml:lang="en">endeavour</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/DataRepository">
    <rdfs:comment xml:lang="en">A facility in which digital data are deposited for long-term preservation and (optional) publication.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Repository"/>
    <dc:description xml:lang="en">May also be described as a data archive, or as a database.</dc:description>
    <rdfs:label xml:lang="en">data repository</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasLocation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property link an entity to a relevant location, which might be geographical (e.g. New York City) or local (e.g. Freezer B, second shelf).</rdfs:comment>
    <rdfs:label xml:lang="en">has location</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/usesFacility">
    <rdfs:comment xml:lang="en">An object property linking an activity such as an expedition, a research project or an investigation, to a facility employed in that activity.</rdfs:comment>
    <rdfs:range rdf:resource="http://purl.org/cerif/frapo/Facility"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <dc:description xml:lang="en">Domain constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
    <rdfs:label xml:lang="en">uses facility</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isEvaluatedBy">
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:comment xml:lang="en">An object property linking an entity to an agent that evaluates it.</rdfs:comment>
    <rdfs:label xml:lang="en">is evaluated by</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasQualification">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property permitting specification of the qualification held by a person.</rdfs:comment>
    <rdfs:label xml:lang="en">has qualification</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasOrganizationalInformation">
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">It describes information about the organization.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasAgentInformation"/>
    <rdfs:label xml:lang="en">has organizational information</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/FundingProgramme">
    <rdfs:comment xml:lang="en">A programme of related funded activities or projects, typically established by a funding agency.  Often associated with publication of a call for applications (fabio:CallForApplications).</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label xml:lang="en">funding programme</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/consultancy-costs">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdfs:comment xml:lang="en">A budget category to cover the costs of consultancy fees.</rdfs:comment>
    <rdfs:label xml:lang="en">consultancy costs</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Faculty">
    <rdfs:label xml:lang="en">faculty</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Division"/>
    <rdfs:comment xml:lang="en">A grouping of similar departments within a university context, also known as a school or a division, for example the Humanities Division, the Mathematics Faculty, or the School of Medicine.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/PurchaseOrder">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
    <rdfs:label xml:lang="en">purchase order</rdfs:label>
    <rdfs:comment xml:lang="en">An official order from a purchasing institution to a supplier for the purchase of goods or services, often issued in response to the receipt of a quotation from the supplier.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Studentship">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A financial grant to a student to enable him/her to undertake advanced academic study.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Stipend"/>
    <rdfs:label xml:lang="en">studentship</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/providesService">
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label xml:lang="en">provides service</rdfs:label>
    <dc:description xml:lang="en">Range constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">An agent that provides a service.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasDomainName">
    <rdfs:label xml:lang="en">has domain name</rdfs:label>
    <rdfs:comment xml:lang="en">A data property linking an entity to an internet domain name that has been registered for that entity, for example http://www.miidi.org for MIIDI, the Minimal Information standard for reporting an Infectious Disease Investigation.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/title">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/travel-and-subsistence-costs">
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">A budget category to cover the costs of travel and subsistence incurred on behalf of the project.</rdfs:comment>
    <rdfs:label xml:lang="en">travel and subsistence costs</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasCountryCode">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasCode"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has country code</rdfs:label>
    <rdfs:comment xml:lang="en">A data property specifying the two-letter or three-letter upper-case country code used to identify a country (e.g. FR/FRA for France, DE/DEU for Germany) (see http://countrycode.org/).</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/InfrastructureEntity">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <dc:description xml:lang="en">owl:equivalentClass cerif:InfrastructureEntity .</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">An entity that facilitates work.</rdfs:comment>
    <rdfs:label xml:lang="en">infrastructure entity</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasIPROwner">
    <rdfs:comment xml:lang="en">An object property linking an entity such as a project, an endeavour or an output to an agent who owns the IPR arising from or related to that entity.</rdfs:comment>
    <rdfs:label xml:lang="en">has IPR owner</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/isOwnedBy"/>
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/ownsIRPOf"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#versionInfo">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasCode">
    <rdfs:comment xml:lang="en">A data property defining a code.</rdfs:comment>
    <rdfs:label xml:lang="en">has code</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Endowment">
    <rdfs:comment xml:lang="en">A transfer, generally as a gift, of money or property to an institution to provide a permanent fund, the income from which is to be used for a particular purpose.  (Abbreviated from http://legal-dictionary.thefreedictionary.com/endowment).</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">endowment</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Gift"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/firstName">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/College">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
    <rdfs:comment xml:lang="en">An academic organization that is either a small degree-giving university, or (in the OxBridge sense) an independent corporation of scholars within a larger university, providing undergraduate tuition and a social environment for its students and fellows.</rdfs:comment>
    <rdfs:label xml:lang="en">college</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasJobTitle">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property permitting specification of the job title held by a person, such as might appear next to the name in a directory or on a conference name badge.  For example, the person's specific Job Title might be 'Linacre Professor of Zoology' or 'Regius Professor of Moral Philosophy'. 

This job title properly refers to the **role** held by that person (e.g. 'Head of Department'), which can be separately defined using SCoRO, the Scholarly Contributions and Roles Ontology (http://purl.org/spar/scoro/). [see scoro:withJobTitle].

</rdfs:comment>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <rdfs:label xml:lang="en">has job title</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/PostalAddress">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:comment xml:lang="en">A postal address.</rdfs:comment>
    <rdfs:label xml:lang="en">postal address</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasCityOrTown">
    <rdfs:comment xml:lang="en">A data property specifying a city or town, for example as the location of an event.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has city or town</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isManufacturedBy">
    <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/manufactures"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">is manufactured by</rdfs:label>
    <rdfs:comment xml:lang="en">An object property relating a manufactured entity to its manufacturer.</rdfs:comment>
    <rdfs:range rdf:resource="http://purl.org/cerif/frapo/Manufacturer"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Company">
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
    <rdfs:comment xml:lang="en">A commercial company or business run for profit.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">company</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Legacy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">legacy</rdfs:label>
    <rdfs:comment xml:lang="en">A gift of money or property specified in the will of a person, that is made after that person's death.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Gift"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Fee">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/FinancialEntity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">An amount of money paid for something that is not purchased.</rdfs:comment>
    <rdfs:label xml:lang="en">fee</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Organization">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="en">owl:equivalentClass cerf:OrganizationalUnit .</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ServiceContractFee">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Fee"/>
    <rdfs:comment xml:lang="en">A fee paid periodically, typically annually, to obtain access when required to a service such as an equipment maintenance contract."</rdfs:comment>
    <rdfs:label xml:lang="en">service contract fee</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasKeyword">
    <rdfs:label xml:lang="en">has keyword</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property permitting specification of a keyword relating to an entity.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/benefactor">
    <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:label xml:lang="en">benefactor</rdfs:label>
    <rdfs:comment xml:lang="en">A person who gives a gift.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Subscription">
    <rdfs:label xml:lang="en">subscription</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Fee"/>
    <rdfs:comment xml:lang="en">Payment, commonly made annually, to obtain access to something on a periodic or continuing basis, such as a journal.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/funded">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/Status"/>
    <rdfs:label xml:lang="en">funded</rdfs:label>
    <rdfs:comment xml:lang="en">The status of something such as an investigation or a project as being funded.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/academic">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">A member of the academic staff of an academic institution such as a university.</rdfs:comment>
    <rdfs:label xml:lang="en">academic</rdfs:label>
    <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/gender">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/hasPersonalInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="Nc535475a3ab64c78b1ea2c2af0b44dab">
    <rdf:first rdf:resource="http://purl.org/cerif/frapo/Funding"/>
    <rdf:rest rdf:nodeID="Nd63ad5a28a4f43d0ac2a791b1773bf38"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Deliverable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A class defining a 'deliverable' of a project, i.e. something that the project set out to deliver, or succeeds in delivering.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Output"/>
    <rdfs:label xml:lang="en">deliverable</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasSupplierNumber">
    <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/Supplier"/>
    <rdfs:comment xml:lang="en">A data property linking a supplier to an identifying number for that supplier, typically used by the accounting system of the purchasing institution when placing and tracking orders.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has supplier number</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Stipend">
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Funding"/>
    <rdfs:comment xml:lang="en">A sum of money allotted on a regular basis, usually for some specific purpose.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">stipend</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Purchaser">
    <rdfs:comment xml:lang="en">An agent who purchases something from a supplier.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label xml:lang="en">purchaser</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/submits">
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:comment xml:lang="en">An object property linking an agent to an entity that is submitted to some third party, for example a grant application submitted to a funding agency, a manuscript submitted to a publisher, a quotation submitted by a vendor, or a dataset submitted by a researcher to a data repository.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">submits</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/Scholarship">
    <rdfs:comment xml:lang="en">A financial grant to a scholar to enable him/her to undertake advanced academic study.</rdfs:comment>
    <rdfs:label xml:lang="en">scholarship</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Stipend"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/date">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/hasQuotationDate">
