    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">A data property specifying the grant number of a grant provided by a funding agency.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/overhead-costs">
    <rdfs:comment xml:lang="en">A budget category to cover all the non-directly incurred costs of a project, including estates costs and indirect costs, that enable an institution to host an externally-funded project. </rdfs:comment>
    <rdfs:label xml:lang="en">overhead costs</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://purl.org/cerif/frapo/BudgetCategory"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/ProjectBudget">
    <rdfs:label xml:lang="en">project budget</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A budget for a research project.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Budget"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/cerif/frapo/isOwnedBy">
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
