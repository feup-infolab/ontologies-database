    <rdfs:label xml:lang="en">has cost</rdfs:label>
    <rdfs:comment xml:lang="en">A data property enabling the cost of something that can be paid for to be defined.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
