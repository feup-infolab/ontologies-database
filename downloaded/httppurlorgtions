  <rdf:Description rdf:about="http://purl.org/tio/ns#Stop">
    <rdfs:comment xml:lang="en">The initial, final, or any intermediate point in time and space of a particular tio:TransportationService, 
characterized by its location (tio:at), arrival time (tio:arrives), and departure time (tio:departs). 
Each tio:TransportationService has at least one tio:Stop for the point and time of departure and another one for the final destination.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Stop</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/tio/ns#"/>
