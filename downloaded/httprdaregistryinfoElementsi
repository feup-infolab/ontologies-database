  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
  <reg:name xml:lang="en">accompaniedByItem</reg:name>
  <rdfs:label xml:lang="en">is accompanied by (item)</rdfs:label>
  <skos:definition xml:lang="en">Relates an item to an item brought together with another item after being issued.</skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10003" />
  <rdfs:subPropertyOf rdf:resource="http://rdaregistry.info/Elements/i/P40051" />
