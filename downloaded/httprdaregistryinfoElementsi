<!-- Property: has immediate source of acquisition of item -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/i/P40050">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/i/" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
