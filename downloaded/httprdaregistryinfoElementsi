<!-- Property: is analysed in (item) -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/i/P40063">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/i/" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <reg:name xml:lang="en">analysedInItem</reg:name>
  <rdfs:label xml:lang="en">is analysed in (item)</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/i/analysedInItem.en" />
  <skos:definition xml:lang="en">Relates an item to a work that examines a source item to identify its components and their relations.</skos:definition>
  <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10003" />
  <reg:hasUnconstrained rdf:resource="http://rdaregistry.info/Elements/u/P60219" />
  <rdakit:toolkitLabel xml:lang="en">analysed in (item)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="en">A work that examines a source item to identify its components and their relations.</rdakit:toolkitDefinition>
  <rdfs:label xml:lang="fr">est analysé dans (item)</rdfs:label>
  <skos:definition xml:lang="fr">Met un item en relation avec une limitation imposée sur la reproduction, la publication, l’exposition, etc. d’un item.</skos:definition>
  <rdakit:toolkitLabel xml:lang="fr">analysé dans (item)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="fr">Limitation imposée sur la reproduction, la publication, l’exposition, etc. d’un item.</rdakit:toolkitDefinition>
  <rdakit:toolkitLabel xml:lang="es">analizada en (ítem)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="es">Una obra que examina el ítem fuente para identificar sus componentes y sus relaciones.</rdakit:toolkitDefinition>
  <rdfs:subPropertyOf rdf:resource="http://rdaregistry.info/Elements/i/P40062" />
  <rdakit:instructionNumber xml:lang="">23.5</rdakit:instructionNumber>
  <reg:hasSubproperty rdf:resource="http://rdaregistry.info/Elements/i/object/P40063" />
  <reg:name xml:lang="vi">đượcPhânTíchTrongBản</reg:name>
  <rdfs:label xml:lang="vi">được phân tích trong (bản)</rdfs:label>
  <skos:definition xml:lang="vi">Liên kết bản với tác phẩm kiểm tra bản nguồn để nhận dạng các thành phần của nó và quan hệ của chúng.</skos:definition>
  <rdakit:toolkitLabel xml:lang="vi">được phân tích trong (bản)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="vi">Tác phẩm kiểm tra bản nguồn để nhận dạng các thành phần của nó và quan hệ của chúng.</rdakit:toolkitDefinition>
  <reg:name xml:lang="da">analyseretIEksemplar</reg:name>
  <rdfs:label xml:lang="da">er analyseret i (eksemplar)</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/i/analyseretIEksemplar.da" />
  <skos:definition xml:lang="da">Relaterer et eksemplar til et værk, der undersøger et oprindeligt eksemplar for at identificere dets dele og deres relationer.</skos:definition>
  <rdakit:toolkitLabel xml:lang="da">analyseret i (eksemplar)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="da">Værk, der undersøger et oprindeligt eksemplar for at identificere dets dele og deres relationer.</rdakit:toolkitDefinition>
  <rdfs:label xml:lang="de">ist analysiert in (Exemplar)</rdfs:label>
  <skos:definition xml:lang="de">Setzt ein Exemplar mit einem Werk, das ein ursprüngliches Exemplar untersucht, um seine Bestandteile und deren Beziehungen zu identifizieren, in Beziehung.</skos:definition>
