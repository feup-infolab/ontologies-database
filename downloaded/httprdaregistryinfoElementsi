<!-- Property: is digital transfer of (item) -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/i/P40039">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/i/" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
  <reg:name xml:lang="en">digitalTransferOfItem</reg:name>
  <rdfs:label xml:lang="en">is digital transfer of (item)</rdfs:label>
  <skos:definition xml:lang="en">Relates an item to an item in a digital format that is transferred to another digital format.</skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10003" />
