<!-- Property: has annotator -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/i/P40015">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/i/" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
  <reg:name xml:lang="en">annotator</reg:name>
  <rdfs:label xml:lang="en">has annotator</rdfs:label>
  <skos:definition xml:lang="en">Relates an item to a person who makes manuscript annotations on an item.</skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10003" />
  <rdfs:subPropertyOf rdf:resource="http://rdaregistry.info/Elements/i/P40023" />
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/i/annotator.en" />
  <reg:hasUnconstrained rdf:resource="http://rdaregistry.info/Elements/u/P60464" />
  <rdakit:toolkitLabel xml:lang="en">annotator</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="en">A person who makes manuscript annotations on an item.</rdakit:toolkitDefinition>
  <rdfs:label xml:lang="fr">a pour annotateur</rdfs:label>
  <skos:definition xml:lang="fr">Met un item en relation avec une personne qui rédige des annotations manuscrites sur un item.</skos:definition>
  <rdakit:toolkitLabel xml:lang="fr">annotateur</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="fr">Personne qui rédige des annotations manuscrites sur un item.</rdakit:toolkitDefinition>
  <rdakit:toolkitLabel xml:lang="es">anotador</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="es">Una persona que hace anotaciones manuscritas en un ítem.</rdakit:toolkitDefinition>
  <rdakit:instructionNumber xml:lang="">18.5</rdakit:instructionNumber>
  <reg:hasSubproperty rdf:resource="http://rdaregistry.info/Elements/i/object/P40015" />
