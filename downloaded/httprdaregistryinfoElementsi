  <rdakit:toolkitDefinition xml:lang="ca">Ítem aplegat amb un altre ítem després de la publicació.</rdakit:toolkitDefinition>
  <reg:name xml:lang="fi">yhdistettyKappaleeseen</reg:name>
  <rdfs:label xml:lang="fi">yhdistetty kappaleeseen</rdfs:label>
