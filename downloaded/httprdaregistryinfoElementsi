  <rdfs:label xml:lang="en">has immediate source of acquisition of item</rdfs:label>
  <skos:definition xml:lang="en">Relates an item to a source from which an agency directly acquired an item and the circumstances under which it was received.</skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10003" />
