  <reg:hasSubproperty rdf:resource="http://rdaregistry.info/Elements/i/P40035" />
  <reg:hasSubproperty rdf:resource="http://rdaregistry.info/Elements/i/P40036" />
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/i/accompaniedByItem.en" />
  <reg:hasUnconstrained rdf:resource="http://rdaregistry.info/Elements/u/P60196" />
  <rdakit:toolkitLabel xml:lang="en">accompanied by (item)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="en">An item brought together with another item after being issued.</rdakit:toolkitDefinition>
  <rdfs:label xml:lang="fr">est accompagné de (item)</rdfs:label>
  <skos:definition xml:lang="fr">Met un item en relation avec un item placé avec un autre item après publication.</skos:definition>
  <rdakit:toolkitLabel xml:lang="fr">accompagné de (item)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="fr">Item placé avec un autre item après publication.</rdakit:toolkitDefinition>
  <rdakit:toolkitLabel xml:lang="es">acompañado por (ítem)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="es">Un ítem que se ha unido con otro ítem después de su publicación.</rdakit:toolkitDefinition>
  <rdakit:instructionNumber xml:lang="">24.5</rdakit:instructionNumber>
  <reg:hasSubproperty rdf:resource="http://rdaregistry.info/Elements/i/object/P40029" />
  <reg:name xml:lang="vi">đượcKèmTheoBởiBản</reg:name>
  <rdfs:label xml:lang="vi">được kèm theo bởi (bản)</rdfs:label>
  <skos:definition xml:lang="vi">Liên kết bản với bản đi kèm với bản khác sau khi được công bố.</skos:definition>
  <rdakit:toolkitLabel xml:lang="vi">được kèm theo bởi (bản)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="vi">Bản đi kèm với bản khác sau khi được công bố.</rdakit:toolkitDefinition>
  <reg:name xml:lang="da">ledsagetAfEksemplar</reg:name>
