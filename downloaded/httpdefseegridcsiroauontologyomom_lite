	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
	<rdfs:comment>role of related observation with respect to the observation of interest</rdfs:comment>
	<rdfs:domain rdf:resource="http://def.seegrid.csiro.au/ontology/om/om-lite#ObservationContext"/>
