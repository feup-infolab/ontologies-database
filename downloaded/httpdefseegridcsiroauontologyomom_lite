<rdf:Description rdf:about="http://def.seegrid.csiro.au/ontology/om/om-lite#phenomenonTime">
	<rdfs:domain rdf:nodeID="node1b4030homx31"/>
	<rdfs:isDefinedBy rdf:resource="http://def.seegrid.csiro.au/ontology/om/om-lite"/>
