    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/IOLite.owl#reuses -->

    <owl:ObjectProperty rdf:about="#reuses">
        <rdfs:comment rdf:datatype="&xsd;string"
            >Any relation between information entities, where the first one reuses (includes, transforms, reengineers, etc.) the second one.</rdfs:comment>
        <rdfs:range rdf:resource="&DUL;InformationEntity"/>
        <rdfs:domain rdf:resource="&DUL;InformationEntity"/>
        <rdfs:subPropertyOf rdf:resource="&DUL;associatedWith"/>
        <owl:inverseOf rdf:resource="#isReusedBy"/>
    </owl:ObjectProperty>
    


