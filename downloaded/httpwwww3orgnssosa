    sosa:hasFeatureOfInterest <span class="hljs-tag">&lt;<span class="hljs-name">http:</span>//<span class="hljs-attr">dbpedia.org</span>/<span class="hljs-attr">resource</span>/<span class="hljs-attr">Antarctic_ice_sheet</span>&gt;</span> .

