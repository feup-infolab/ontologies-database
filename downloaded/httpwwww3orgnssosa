  sosa:madeBySensor <span class="hljs-tag">&lt;<span class="hljs-name">sensor</span>/<span class="hljs-attr">926</span>&gt;</span> .


# the window opening state is an ActuatableProperty.
# SSN allows to explicitly say that <span class="hljs-tag">&lt;<span class="hljs-name">window</span>/<span class="hljs-attr">104</span>#<span class="hljs-attr">state</span>&gt;</span> is a property of <span class="hljs-tag">&lt;<span class="hljs-name">window</span>&gt;</span>

