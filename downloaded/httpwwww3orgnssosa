# observed on June 6 2017 using only the SOSA core for modelling.

<span class="hljs-tag">&lt;<span class="hljs-name">earthAtmosphere</span>&gt;</span> rdf:type sosa:FeatureOfInterest ;
  rdfs:label "Atmosphere of Earth"@en .


