  rdfs:comment "The precision (= repeatability) of the temperature sensor is +-0.2 Â°C in normal temperature and humidity conditions."@en ;
  schema:minValue 0.2 ;
  schema:maxValue 0.2 ;
  schema:unitCode qudt-unit-1-1:DegreeCelsius .

