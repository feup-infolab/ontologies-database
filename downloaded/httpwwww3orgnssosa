  sosa:madeObservation <span class="hljs-tag">&lt;<span class="hljs-name">Observation</span>/<span class="hljs-attr">235714</span>&gt;</span>, <span class="hljs-tag">&lt;<span class="hljs-name">Observation</span>/<span class="hljs-attr">235715</span>&gt;</span> .

