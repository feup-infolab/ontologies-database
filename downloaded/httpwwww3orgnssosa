  rdfs:label "Bosch Sensortec BMP282"@en ;
  sosa:observes <span class="hljs-tag">&lt;<span class="hljs-name">sensor</span>/<span class="hljs-attr">35-207306-844818-0</span>/<span class="hljs-attr">BMP282</span>/<span class="hljs-attr">atmosphericPressure</span>&gt;</span> .


