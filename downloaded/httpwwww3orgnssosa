  sosa:hasSimpleResult "1021.45 hPa"^^cdt:ucum ;
  sosa:resultTime "2017-06-06T12:36:12Z"^^xsd:dateTime .


# Another observation made a second later by the BMP282 atmospheric pressure sensor
# using the QUDT Ontology for the Units of Measurement
# and the Time Ontology for the instant.

<span class="hljs-tag">&lt;<span class="hljs-name">Observation</span>/<span class="hljs-attr">346345</span>&gt;</span> rdf:type sosa:Observation ;
  sosa:observedProperty <span class="hljs-tag">&lt;<span class="hljs-name">sensor</span>/<span class="hljs-attr">35-207306-844818-0</span>/<span class="hljs-attr">BMP282</span>/<span class="hljs-attr">atmosphericPressure</span>&gt;</span> ;
