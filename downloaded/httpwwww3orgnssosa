     rdf:type qudt-1-1:QuantityValue ;
     qudt-1-1:numericValue "5e-4"^^xsd:double ;
     qudt-1-1:unit qudt-unit-1-1:CentimeterPerSecond ] ;
  sosa:resultTime "2017-04-18T08:23:00-07:00"^^xsd:dateTimeStamp .</pre>
