  sosa:isActedOnBy <span class="hljs-tag">&lt;<span class="hljs-name">actuation</span>/<span class="hljs-attr">188</span>&gt;</span> .


# WindowCloser #987 made actuation #188
# SSN allows to explicitly say that <span class="hljs-tag">&lt;<span class="hljs-name">windowCloser</span>/<span class="hljs-attr">987</span>&gt;</span> is designed to automatically open and close window #104.

<span class="hljs-tag">&lt;<span class="hljs-name">windowCloser</span>/<span class="hljs-attr">987</span>&gt;</span> rdf:type sosa:Actuator ;
  sosa:madeActuation <span class="hljs-tag">&lt;<span class="hljs-name">actuation</span>/<span class="hljs-attr">188</span>&gt;</span> ;
  ssn:forProperty <span class="hljs-tag">&lt;<span class="hljs-name">window</span>/<span class="hljs-attr">104</span>#<span class="hljs-attr">state</span>&gt;</span> .


