  ex:evaluatedBy <span class="hljs-tag">&lt;<span class="hljs-name">Tom</span>&gt;</span> ;
  ex:confidenceValue "6"^^xsd:integer;
  rdfs:comment """Tom gave a confidence value of 6 out of 10 on this observation."""@en .

# one may use some quantity ontology.

@prefix qudt-1-1: <span class="hljs-tag">&lt;<span class="hljs-name">http:</span>//<span class="hljs-attr">qudt.org</span>/<span class="hljs-attr">1.1</span>/<span class="hljs-attr">schema</span>/<span class="hljs-attr">qudt</span>#&gt;</span> .
