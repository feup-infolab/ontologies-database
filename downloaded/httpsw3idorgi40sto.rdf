<rdf:RDF
    xmlns:dc11="http://purl.org/dc/elements/1.1/"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dc="http://purl.org/dc/terms/"
    xmlns:rami="https://w3id.org/i40/rami/"
    xmlns:sto="https://w3id.org/i40/sto#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:voaf="http://purl.org/vocommons/voaf#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:cc="http://creativecommons.org/ns#"
    xmlns:vann="http://purl.org/vocab/vann/"
    xmlns:foaf="http://xmlns.com/foaf/0.1/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#" > 
  <rdf:Description rdf:about="https://w3id.org/i40/sto#name">
    <rdfs:label>Name</rdfs:label>
    <dc:description xml:lang="en">Name of the Organization</dc:description>
    <rdfs:domain rdf:resource="https://w3id.org/i40/sto#SDO"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/i40/sto#hasTag">
    <rdfs:label>Has Tag</rdfs:label>
    <dc:description xml:lang="en">Tag commonly used to refer to the standard </dc:description>
    <rdfs:range rdf:resource="http://purl.org/muto/core#Tag"/>
    <rdfs:domain rdf:resource="https://w3id.org/i40/sto#Standard"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/i40/sto#ramiHierarchyLevel">
    <rdfs:label xml:lang="en">RAMI Hierarchy Level</rdfs:label>
    <dc:description xml:lang="en">Property through to associate the standards with the RAMI level in which they are applicable</dc:description>
    <rdfs:range rdf:resource="https://w3id.org/i40/rami/RAMIHierarchyLevel"/>
    <rdfs:domain rdf:resource="https://w3id.org/i40/sto#Standard"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/i40/sto#hasDBpediaResource">
    <rdfs:label>Has DBpedia resource</rdfs:label>
    <dc:description xml:lang="en">Instance's DBpedia Resource</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/i40/sto#motivation">
    <rdfs:label>motivation</rdfs:label>
    <dc:description xml:lang="en">A motivation for the creation of the standard</dc:description>
    <rdfs:range rdf:resource="http://purl.org/spar/deo/Motivation"/>
    <rdfs:domain rdf:resource="https://w3id.org/i40/sto#Standard"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/i40/sto#published">
    <rdfs:label>Published</rdfs:label>
    <dc:description xml:lang="en">A standard published by this organization</dc:description>
    <rdfs:range rdf:resource="https://w3id.org/i40/sto#Standard"/>
    <rdfs:domain rdf:resource="https://w3id.org/i40/sto#SDO"/>
    <owl:inverseOf rdf:resource="https://w3id.org/i40/sto#publisher"/>
    <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/made"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/i40/sto#abbreviation">
    <rdfs:label>Abbreviated name</rdfs:label>
    <dc:description xml:lang="en">Abbreviated  name of the organization</dc:description>
    <rdfs:domain rdf:resource="https://w3id.org/i40/sto#SDO"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/i40/sto#license">
    <rdfs:label>License</rdfs:label>
    <dc:description xml:lang="en">License under which a standard is published</dc:description>
    <rdfs:domain rdf:resource="https://w3id.org/i40/sto#Standard"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/license"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/i40/sto#hasWikipediaArticle">
    <rdfs:label>Has Wikipedia article</rdfs:label>
    <dc:description xml:lang="en">Instance's Wikipedia article</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/i40/sto#hasWikidataEntity">
    <rdfs:label>Has Wikidata entity</rdfs:label>
    <dc:description xml:lang="en">Instance's Wikidata Entry</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/i40/sto#formationDate">
    <rdfs:label xml:lang="en">Formation Date</rdfs:label>
    <dc:description xml:lang="en">Abbreviated  name of the organization</dc:description>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdfs:domain rdf:resource="https://w3id.org/i40/sto#SDO"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/i40/sto#IndustryDomain">
    <rdfs:label>Industry Domain</rdfs:label>
    <dc:description xml:lang="en">Represents the possible industry domains</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/i40/sto#relatedTo">
    <rdfs:label>Related to</rdfs:label>
    <dc:description xml:lang="en">Both Standards are related</dc:description>
    <rdfs:range rdf:resource="https://w3id.org/i40/sto#Standard"/>
    <rdfs:domain rdf:resource="https://w3id.org/i40/sto#Standard"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/i40/sto#Standard">
    <rdfs:label>Standard</rdfs:label>
    <dc:description xml:lang="en">Standards Class</dc:description>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/i40/sto#hasOfficialResource">
    <rdfs:label>Has Official website</rdfs:label>
    <dc:description xml:lang="en">Instance's Official Website</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/i40/sto#">
    <vann:preferredNamespacePrefix>sto</vann:preferredNamespacePrefix>
    <owl:versionInfo>0.2</owl:versionInfo>
    <dc:creator rdf:resource="http://eis.iai.uni-bonn.de/Irlan"/>
    <dc:publisher rdf:resource="http://de.dbpedia.org/resource/Fraunhofer-Gesellschaft"/>
    <dc:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2017-01-25</dc:created>
    <dc:license rdf:resource="http://creativecommons.org/licenses/by-nc-sa/2.0/"/>
    <dc11:description>A vocabulary to describe the relation between industry standards, and related concepts</dc11:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <vann:preferredNamespaceUri>https://w3id.org/i40/sto#</vann:preferredNamespaceUri>
    <dc:creator rdf:resource="http://data.semanticweb.org/person/steffen-lohmann"/>
    <dc11:title xml:lang="en">Standards Vocabulary</dc11:title>
    <dc:creator rdf:resource="https://www.linkedin.com/in/paul-baptista-601177b2"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/i40/sto#scope">
    <rdfs:label xml:lang="en">Scope</rdfs:label>
    <dc:description xml:lang="en">Property through which relate the standards with industry domains</dc:description>
    <rdfs:range rdf:resource="https://w3id.org/i40/sto#IndustryDomain"/>
    <rdfs:domain rdf:resource="https://w3id.org/i40/sto#Standard"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/i40/sto#publisher">
    <rdfs:label>Publisher</rdfs:label>
    <dc:description xml:lang="en">The organization that published this Standard</dc:description>
    <rdfs:range rdf:resource="https://w3id.org/i40/sto#SDO"/>
    <rdfs:domain rdf:resource="https://w3id.org/i40/sto#Standard"/>
    <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/maker"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/i40/sto#norm">
    <rdfs:label>Norm</rdfs:label>
    <dc:description xml:lang="en">Norm under which the standard is published</dc:description>
    <rdfs:domain rdf:resource="https://w3id.org/i40/sto#Standard"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/i40/sto#SDO">
    <rdfs:label>Standards Developing Organization</rdfs:label>
    <dc:description xml:lang="en">Define the standards organizations that develop and publish industry standards</dc:description>
    <owl:disjointWith rdf:resource="https://w3id.org/i40/sto#Standard"/>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
</rdf:RDF>
