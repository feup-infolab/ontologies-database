		<rdfs:range rdf:resource="http://purl.org/goodrelations/v1#QuantitativeValueInteger" />
		<rdfs:isDefinedBy rdf:resource="http://purl.org/vso/ns" />
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="http://purl.org/vso/ns#mileageFromOdometer">
