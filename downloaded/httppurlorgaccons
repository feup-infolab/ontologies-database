    <a href="http://purl.org/goodrelations/v1#description">gr:description</a> &quot;...more text...&quot;@en ;
    <a href="http://purl.org/acco/ns#petsAllowed">acco:petsAllowed</a> &quot;false&quot;^^xsd:boolean ;
    <a href="http://purl.org/acco/ns#occupancy">acco:occupancy</a> [ a <a href="http://purl.org/goodrelations/v1#QuantitativeValue">gr:QuantitativeValue</a> ;
                     <a href="http://purl.org/goodrelations/v1#hasUnitOfMeasurement">gr:hasUnitOfMeasurement</a> &quot;C62&quot;^^xsd:string ;
                     <a href="http://purl.org/goodrelations/v1#hasMinValue">gr:hasMinValue</a> &quot;1&quot;^^xsd:int ;
                     <a href="http://purl.org/goodrelations/v1#hasMaxValue">gr:hasMaxValue</a> &quot;6&quot;^^xsd:int ] ;
    <a href="http://purl.org/acco/ns#numberOfRooms">acco:numberOfRooms</a> [ a <a href="http://purl.org/goodrelations/v1#QuantitativeValue">gr:QuantitativeValue</a> ;
                         <a href="http://purl.org/goodrelations/v1#hasUnitOfMeasurement">gr:hasUnitOfMeasurement</a> &quot;C62&quot;^^xsd:string ;
                         <a href="http://purl.org/goodrelations/v1#hasValue">gr:hasValue</a> &quot;3&quot;^^xsd:int ] ;
    <a href="http://purl.org/acco/ns#bed">acco:bed</a> [ a <a href="http://purl.org/acco/ns#BedDetails">acco:BedDetails</a> ;
               <a href="http://purl.org/acco/ns#quantity">acco:quantity</a> &quot;3&quot;^^xsd:integer ;
               <a href="http://purl.org/acco/ns#typeOfBed">acco:typeOfBed</a> <a href="http://purl.org/acco/ns#Double">acco:Double</a> ] .
            
foo:HouseOffer a <a href="http://purl.org/goodrelations/v1#Offering">gr:Offering</a> ;
