    <!-- http://purl.org/ipo/core#activates -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#activates">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">activates</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates the action to be activated by a Step.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:range rdf:resource="http://purl.org/ipo/core#Action"/>
        <rdfs:domain rdf:resource="http://purl.org/ipo/core#Step"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#assetOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#assetOf">
        <rdfs:label xml:lang="en">assetOf</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates a IssueEntity that is related to the Asset.
This property can be used to facilitate retrieval records of problems related to the Asset.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <owl:inverseOf rdf:resource="http://purl.org/ipo/core#hasAsset"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#canBeCausedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#canBeCausedBy">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="en">canBeCausedBy</rdfs:label>
        <rdfs:comment xml:lang="en">The Issue can be caused by another Issue directly or indirectly.
This property is transitive, i.e., if the issue &quot;A&quot; can be caused by the issue &quot;B&quot; and the issue &quot;B&quot; can be caused by the issue &quot;C&quot;, then the issue &quot;A&quot; can be caused by the issue &quot;C&quot;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#canBeDependenceOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#canBeDependenceOf">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="en">canBeDependenceOf</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates that an Issue can be a dependence of another Issue directly or indirectly.
This property is also transitive, i.e., if the issue &quot;A&quot; can be dependence of the issue &quot;B&quot; and the issue &quot;B&quot; can be dependence of the issue &quot;C&quot;, then the issue &quot;A&quot; can be dependence of the issue &quot;C&quot;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#canBeDirectDependenceOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#canBeDirectDependenceOf">
        <rdfs:label xml:lang="en">canBeDirectDependenceOf</rdfs:label>
        <rdfs:comment xml:lang="en">This property indicates an Issue that can be direct dependence of another Issue.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ipo/core#canBeDependenceOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#canBeDirectlyCausedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#canBeDirectlyCausedBy">
        <rdfs:label xml:lang="en">canBeDirectlyCausedBy</rdfs:label>
        <rdfs:comment xml:lang="en">An Issue can be  directly caused by another Issue.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ipo/core#canBeCausedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#canCause -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#canCause">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="en">canCause</rdfs:label>
        <rdfs:comment xml:lang="en">An Issue can cause another Issue directly or indirectly. 
This property aims to express a possible causal relationship and has the characteristic of transitivity, i.e., the Issue &quot;A&quot; can cause the Issue &quot;B&quot; and the Issue &quot;B&quot; can cause the Issue &quot;C&quot;, then the Issue &quot;A&quot; can cause the Issue &quot;C&quot;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:domain rdf:resource="http://purl.org/ipo/core#Issue"/>
        <rdfs:range rdf:resource="http://purl.org/ipo/core#Issue"/>
        <owl:inverseOf rdf:resource="http://purl.org/ipo/core#canBeCausedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#canDependOn -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#canDependOn">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="en">canDependOn</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates that an Issue can depend on another Issue directly or indirectly.
This property can express a dependency relationship between Issues and has the characteristic of transitivity, i.e., the Issue &quot;A&quot; can depend on the Issue &quot;B&quot; and the Issue &quot;B&quot; can depend on &quot;C&quot;, then the Issue &quot;A&quot; can depend on the Issue &quot;C&quot;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:domain rdf:resource="http://purl.org/ipo/core#Issue"/>
        <rdfs:range rdf:resource="http://purl.org/ipo/core#Issue"/>
        <owl:inverseOf rdf:resource="http://purl.org/ipo/core#canBeDependenceOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#canDirectlyCause -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#canDirectlyCause">
        <rdfs:label xml:lang="en">canDirectlyCause</rdfs:label>
        <rdfs:comment xml:lang="en">An Issue can directly cause another Issue.
This property aims to express a possible causal relationship, where an Issue can be direct cause of another Issue. For example, flu disease can cause pneumonia, i.e., in some cases, flu causes pneumonia and in another cases not.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <owl:inverseOf rdf:resource="http://purl.org/ipo/core#canBeDirectlyCausedBy"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ipo/core#canCause"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#canDirectlyDependOn -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#canDirectlyDependOn">
        <rdfs:label xml:lang="en">canDirectlyDependOn</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates that an Issue can directly depend on another Issue.
For example, pneumonia can directly depend that the flu is cured to finally be treated.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <owl:inverseOf rdf:resource="http://purl.org/ipo/core#canBeDirectDependenceOf"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ipo/core#canDependOn"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#categoryOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#categoryOf">
        <rdfs:label xml:lang="en">categoryOf</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates a IssueEntity classified in a skos:Concept.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <owl:inverseOf rdf:resource="http://purl.org/ipo/core#hasCategory"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#causativeAssetOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#causativeAssetOf">
        <rdfs:label xml:lang="en">causativeAssetOf</rdfs:label>
        <rdfs:comment xml:lang="en">This property relates a Asset with a Issue caused by this Asset. For example, a virus associated with a disease caused by it.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ipo/core#assetOf"/>
        <owl:inverseOf rdf:resource="http://purl.org/ipo/core#hasCausativeAsset"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#causedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#causedBy">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="en">causedBy</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates that an Issue is caused by another Issue directly or indirectly.
This property also has a transitive characteristic, i.e., if the Issue &quot;A&quot; is caused by Issue &quot;B&quot; and the Issue &quot;B&quot; is caused by Issue &quot;C&quot;, then the Issue &quot;A&quot; is caused by Issue &quot;C&quot;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#causes -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#causes">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="en">causes</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates that an Issue causes another Issue directly or indirectly.
This property expresses a causal relationship between Issues and have the characteristic of transitivity, i.e., if the Issue &quot;A&quot; causes Issue &quot;B&quot; and the Issue &quot;B&quot; causes Issue &quot;C&quot;, then the Issue &quot;A&quot; causes Issue &quot;C&quot;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:range rdf:resource="http://purl.org/ipo/core#Issue"/>
        <rdfs:domain rdf:resource="http://purl.org/ipo/core#Issue"/>
        <owl:inverseOf rdf:resource="http://purl.org/ipo/core#causedBy"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#dependenceOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#dependenceOf">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="en">dependenceOf</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates that an Issue is a dependence of another Issue directly or indirectly. 
This property also has a transitive characteristic, i.e. , if Issue &quot;A&quot; is dependence of the Issue &quot;B&quot;, and the Issue &quot;B&quot; is dependence of Issue &quot;C&quot;, then the Issue &quot;A&quot; is dependence of Issue &quot;C&quot;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#dependsOn -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#dependsOn">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="en">dependsOn</rdfs:label>
        <rdfs:comment xml:lang="en">This property indicates that an Issue depends on another Issue directly or indirectly.
This property expresses a dependence relationship between Issues and has the characteristic of transitivity, i.e. , if Issue &quot;A&quot; depends on the Issue &quot;B&quot;, and the Issue &quot;B&quot; depends on Issue &quot;C&quot;, then the Issue &quot;A&quot; depends on Issue &quot;C&quot;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:range rdf:resource="http://purl.org/ipo/core#Issue"/>
        <rdfs:domain rdf:resource="http://purl.org/ipo/core#Issue"/>
        <owl:inverseOf rdf:resource="http://purl.org/ipo/core#dependenceOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#depictionOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#depictionOf">
        <rdfs:label xml:lang="en">depictionOf</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates a IssueEntity illustrated by the image.
This property is functional, i.e., an Image is related by this property with only one IssueEntity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <owl:inverseOf rdf:resource="http://purl.org/ipo/core#hasDepiction"/>
        <rdfs:subPropertyOf rdf:resource="&foaf;depicts"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#directCategoryOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#directCategoryOf">
        <rdfs:label xml:lang="en">directCategoryOf</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates a IssueEntity directly classified in a skos:Concept.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ipo/core#categoryOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#directDependenceOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#directDependenceOf">
        <rdfs:label xml:lang="en">directDependenceOf</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates that a Issue is direct dependence of another Issue.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ipo/core#dependenceOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#directlyCausedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#directlyCausedBy">
        <rdfs:label xml:lang="en">directlyCausedBy</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates that an Issue is directly caused by another Issue.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ipo/core#causedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#directlyCauses -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#directlyCauses">
        <rdfs:label xml:lang="en">directlyCauses</rdfs:label>
        <rdfs:comment xml:lang="en">This property indicates that an Issue directly causes another Issue.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ipo/core#causes"/>
        <owl:inverseOf rdf:resource="http://purl.org/ipo/core#directlyCausedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#directlyDependsOn -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#directlyDependsOn">
        <rdfs:label xml:lang="en">directlyDependsOn</rdfs:label>
        <rdfs:comment xml:lang="en">This relation indicates that a issue directly depends on another Issue.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ipo/core#dependsOn"/>
        <owl:inverseOf rdf:resource="http://purl.org/ipo/core#directDependenceOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#hasAsset -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#hasAsset">
        <rdfs:label xml:lang="en">hasAsset</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates an Asset to IssueEntity, i.e., any &quot;thing&quot; which is related to IssueEntity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:range rdf:resource="http://purl.org/ipo/core#Asset"/>
        <rdfs:domain rdf:resource="http://purl.org/ipo/core#IssueEntity"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#hasCategory -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#hasCategory">
        <rdfs:label xml:lang="en">hasCategory</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates a category (skos: Concept) directly or indirectly related to a IssueEntity.
The Concept class of SKOS ontology has properties that allow you to create hierarchies of categories (Concepts), still allowing express transitivity between categories. This approach should be used as an alternative to classification by subclass of IssueEntity when it are not intrinsic classifications, just grouping. For example, in the field of medicine, disease can be grouped as viral diseases, bacterial diseases, etc.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:domain rdf:resource="http://purl.org/ipo/core#IssueEntity"/>
        <rdfs:range rdf:resource="&skos;Concept"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://purl.org/ipo/core#hasCategory"/>
            <rdf:Description rdf:about="&skos;broaderTransitive"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#hasCausativeAsset -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#hasCausativeAsset">
        <rdfs:label xml:lang="en">hasCausativeAsset</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates an Asset that is the causer of the Issue.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:domain rdf:resource="http://purl.org/ipo/core#Issue"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ipo/core#hasAsset"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#hasDepiction -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#hasDepiction">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label xml:lang="en">hasDepiction</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates an image that illustrates the IssueEntity.
Seeking a better description of IssueEntity, one can make use of this property to relate an illustrative image. For example, an image of a software screen containing an error occurred can help describe the problem.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:range rdf:resource="http://purl.org/ipo/core#Image"/>
        <rdfs:domain rdf:resource="http://purl.org/ipo/core#IssueEntity"/>
        <rdfs:subPropertyOf rdf:resource="&foaf;depiction"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#hasDirectCategory -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#hasDirectCategory">
        <rdfs:label xml:lang="en">hasDirectCategory</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates a category (skos: Concept) directly related to a IssueEntity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <owl:inverseOf rdf:resource="http://purl.org/ipo/core#directCategoryOf"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ipo/core#hasCategory"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#hasFirstStep -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#hasFirstStep">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label xml:lang="en">hasFirstStep</rdfs:label>
        <rdfs:comment xml:lang="en">A CompoundAction has one or more steps, so this property indicates the first step should be executed.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ipo/core#hasStep"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#hasGuardCondition -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#hasGuardCondition">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label xml:lang="en">hasGuardCondition</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates a BooleanExpression for that a Transition occurs.
For example, a Transition can have as guardCondition that the Step of origin be executed 10 times. So while this Step does is not executes 10 times, will not be started the Step of destination. This example illustrates a repeating structure within the workflow.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:range rdf:resource="http://purl.org/ipo/core#BooleanExpression"/>
        <rdfs:domain rdf:resource="http://purl.org/ipo/core#Transition"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#hasHostAsset -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#hasHostAsset">
        <rdfs:label xml:lang="en">hasHostAsset</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates an Asset that is the host (harbourer) of the Issue.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:domain rdf:resource="http://purl.org/ipo/core#Issue"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ipo/core#hasAsset"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#hasIncoming -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#hasIncoming">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label xml:lang="en">hasIncoming</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates the Transition that starts the execution of this Step.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:domain rdf:resource="http://purl.org/ipo/core#Step"/>
        <rdfs:range rdf:resource="http://purl.org/ipo/core#Transition"/>
        <owl:inverseOf rdf:resource="http://purl.org/ipo/core#hasTarget"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#hasMaker -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#hasMaker">
        <rdfs:label xml:lang="en">hasMaker</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates a Person or Organization (Party) who created or registered the IssueEntity. This property is also used by Asset class to relate to its manufacturer, developer, inventor, etc.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:range rdf:resource="http://purl.org/ipo/core#Party"/>
        <owl:inverseOf rdf:resource="http://purl.org/ipo/core#makerOf"/>
        <rdfs:subPropertyOf rdf:resource="&foaf;maker"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/ipo/core#Asset"/>
                    <rdf:Description rdf:about="http://purl.org/ipo/core#IssueEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#hasMember -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#hasMember">
        <rdfs:label xml:lang="en">hasMember</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates a Person who is a member of this Organization or another Organization that is member of this Organization, representing here a department or business unit.
</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:domain rdf:resource="http://purl.org/ipo/core#Organization"/>
        <rdfs:range rdf:resource="http://purl.org/ipo/core#Party"/>
        <owl:inverseOf rdf:resource="http://purl.org/ipo/core#memberOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#hasOutcoming -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#hasOutcoming">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label xml:lang="en">hasOutcoming</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates the Transition that occurs after execution of this Step and that activates the next Step to be executed.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:domain rdf:resource="http://purl.org/ipo/core#Step"/>
        <rdfs:range rdf:resource="http://purl.org/ipo/core#Transition"/>
        <owl:inverseOf rdf:resource="http://purl.org/ipo/core#hasSource"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#hasPostCondition -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#hasPostCondition">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label xml:lang="en">hasPostCondition</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates a postcondition (effect) will be reached after executing the Action.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:domain rdf:resource="http://purl.org/ipo/core#Action"/>
        <rdfs:range rdf:resource="http://purl.org/ipo/core#BooleanExpression"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#hasPreCondition -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#hasPreCondition">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label xml:lang="en">hasPreCondition</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates a precondition for the Action be executed.

A precondition may represent a prerequisite for execution of the Action, without which it is not guaranteed correct execution.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:domain rdf:resource="http://purl.org/ipo/core#Action"/>
        <rdfs:range rdf:resource="http://purl.org/ipo/core#BooleanExpression"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#hasSource -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#hasSource">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">hasSource</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates the Step of origin of this Transition.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#hasStep -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#hasStep">
        <rdfs:label xml:lang="en">hasStep</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates a Step that composes the compoundAction.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:domain rdf:resource="http://purl.org/ipo/core#CompoundAction"/>
        <rdfs:range rdf:resource="http://purl.org/ipo/core#Step"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://purl.org/ipo/core#hasStep"/>
            <rdf:Description rdf:about="http://purl.org/ipo/core#hasOutcoming"/>
            <rdf:Description rdf:about="http://purl.org/ipo/core#hasTarget"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#hasTarget -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#hasTarget">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">hasTarget</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates the Step of destination of this Transition.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#hostAssetOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#hostAssetOf">
        <rdfs:label xml:lang="en">hostAssetOf</rdfs:label>
        <rdfs:comment xml:lang="en">Relates an Asset with a Issue that occurs in it.
This property can be used to relate a person with a disease whose the person is where the disease occurs.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ipo/core#assetOf"/>
        <owl:inverseOf rdf:resource="http://purl.org/ipo/core#hasHostAsset"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#indicatedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#indicatedBy">
        <rdfs:label xml:lang="en">indicatedBy</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates a Symptom of this Issue.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#indicates -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#indicates">
        <rdfs:label xml:lang="en">indicates</rdfs:label>
        <rdfs:comment xml:lang="en">This property relates a Issue indicated by this Symptom.
A Symptom may indicate many Issues, such as a Symptom of fever can indicate many diseases.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:range rdf:resource="http://purl.org/ipo/core#Issue"/>
        <rdfs:domain rdf:resource="http://purl.org/ipo/core#Symptom"/>
        <owl:inverseOf rdf:resource="http://purl.org/ipo/core#indicatedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#makerOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#makerOf">
        <rdfs:label xml:lang="en">makerOf</rdfs:label>
        <rdfs:comment xml:lang="en">This property can be used to indicates an Asset created/manufactured by Party or a IssueEntity created/registered by the Party.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:subPropertyOf rdf:resource="&foaf;made"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#memberOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#memberOf">
        <rdfs:label xml:lang="en">memberOf</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates a organization that the Party is participant.
This property can be used to indicates that an employee is a member of an organization.
Another approach is to use this property to relate two organizations thus representing a department that is a member of his company.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#solvedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#solvedBy">
        <rdfs:label xml:lang="en">solvedBy</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates a solution (Procedure) for this Issue.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ipo/core#solves -->

    <owl:ObjectProperty rdf:about="http://purl.org/ipo/core#solves">
        <rdfs:label xml:lang="en">solves</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates a Issue that this Procedure solves.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:range rdf:resource="http://purl.org/ipo/core#Issue"/>
        <rdfs:domain rdf:resource="http://purl.org/ipo/core#Procedure"/>
        <owl:inverseOf rdf:resource="http://purl.org/ipo/core#solvedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#broaderTransitive -->

    <owl:ObjectProperty rdf:about="&skos;broaderTransitive"/>
    


    <!-- http://xmlns.com/foaf/0.1/depiction -->

    <owl:ObjectProperty rdf:about="&foaf;depiction"/>
    


    <!-- http://xmlns.com/foaf/0.1/depicts -->

    <owl:ObjectProperty rdf:about="&foaf;depicts"/>
    


    <!-- http://xmlns.com/foaf/0.1/made -->

    <owl:ObjectProperty rdf:about="&foaf;made"/>
    


    <!-- http://xmlns.com/foaf/0.1/maker -->

    <owl:ObjectProperty rdf:about="&foaf;maker"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/description -->

    <owl:DatatypeProperty rdf:about="&dcterms;description"/>
    


    <!-- http://purl.org/dc/terms/title -->

    <owl:DatatypeProperty rdf:about="&dcterms;title"/>
    


    <!-- http://purl.org/ipo/core#description -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ipo/core#description">
        <rdfs:label xml:lang="en">description</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates text that describes something in detail.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&dcterms;description"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ipo/core#goal -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ipo/core#goal">
        <rdfs:label xml:lang="en">goal</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates the description of the goal to be reached after the execution of Procedure.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:domain rdf:resource="http://purl.org/ipo/core#Procedure"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ipo/core#name -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ipo/core#name">
        <rdfs:label xml:lang="en">name</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates a name to identify something.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ipo/core#title -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ipo/core#title">
        <rdfs:label xml:lang="en">title</rdfs:label>
        <rdfs:comment xml:lang="en">Title (word or phrase) that briefly describes something.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&dcterms;title"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/ipo/core#Action -->

    <owl:Class rdf:about="http://purl.org/ipo/core#Action">
        <rdfs:label xml:lang="en">Action</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/ipo/core#IssueEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ipo/core#hasPostCondition"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ipo/core#BooleanExpression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ipo/core#hasPreCondition"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ipo/core#BooleanExpression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Represents an action to be performed. An Action can only be a primitive action (Task) or a set of actions (CompoundAction).
An Action has a precondition that will enable the execution of the Action, and a postcondition (effect) which will be validated after running the Action.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
    </owl:Class>
    


    <!-- http://purl.org/ipo/core#Activity -->

    <owl:Class rdf:about="http://purl.org/ipo/core#Activity">
        <rdfs:label xml:lang="en">Activity</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/ipo/core#CompoundAction"/>
        <owl:disjointWith rdf:resource="http://purl.org/ipo/core#Procedure"/>
        <rdfs:comment xml:lang="en">A set of steps that perform an activity, but does not aim to solve a particular Issue.
Can be used to group multiple steps that are repeated in various Procedures to facilitate reuse.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
    </owl:Class>
    


    <!-- http://purl.org/ipo/core#Asset -->

    <owl:Class rdf:about="http://purl.org/ipo/core#Asset">
        <rdfs:label xml:lang="en">Asset</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ipo/core#title"/>
                <owl:someValuesFrom rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Any &quot;thing&quot; that a IssueEntity is related. 
For example, a problem (disease) diagnosed in a patient, can have a virus and the patient as Asset, since the virus is the causative agent of the problem and the patient is the host in which the problem manifests itself.
An Asset can be a person, an object, a report, a document, etc.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
    </owl:Class>
    


    <!-- http://purl.org/ipo/core#BooleanExpression -->

    <owl:Class rdf:about="http://purl.org/ipo/core#BooleanExpression">
        <rdfs:label xml:lang="en">BooleanExpression</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ipo/core#description"/>
                <owl:someValuesFrom rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A logical expression whose value is true or false, aiming to validate a Transition between two Steps, or serve with a pre-condition for the execution of a Action or serve as a post-condition to validate the execution of an Action.
	   </rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
    </owl:Class>
    


    <!-- http://purl.org/ipo/core#CompoundAction -->

    <owl:Class rdf:about="http://purl.org/ipo/core#CompoundAction">
        <rdfs:label xml:lang="en">CompoundAction</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/ipo/core#Action"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ipo/core#hasStep"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ipo/core#Step"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ipo/core#hasFirstStep"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ipo/core#Step"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://purl.org/ipo/core#Task"/>
        <rdfs:comment xml:lang="en">An action composed of several other actions. A CompoundAction may have the goal of solving one or more Issues, representing a Procedure or not having an explicit goal, just be a group of Actions to be reused, behaving as an Activity. A CompoundAction has one or more execution steps that activate one Action (Task or other CompoundAction), thus allowing, that a compoundAction reuse another.
A CompoundAction can be used to create a workflow structure, aimed at better structuring of actions that compose it.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ipo/core"/>
    </owl:Class>
    


    <!-- http://purl.org/ipo/core#Image -->

    <owl:Class rdf:about="http://purl.org/ipo/core#Image">
        <rdfs:label xml:lang="en">Image</rdfs:label>
        <owl:equivalentClass rdf:resource="&foaf;Image"/>
        <rdfs:comment xml:lang="en">An artifact that illustrates or records a visual perception.

It can be used to illustrate a IssueEntity seeking a better understanding of it.</rdfs:comment>
