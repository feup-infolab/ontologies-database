      <owl:Restriction>
        <owl:hasValue rdf:resource="http://purl.org/procurement/public-contracts-procedure-types#"/>
        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
      </owl:Restriction>
    </rdfs:range>
  </owl:FunctionalProperty>
  <owl:DatatypeProperty rdf:about="http://purl.org/procurement/public-contracts#profile">
