    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
    <rdfs:label xml:lang="cs">Cena</rdfs:label>
    <rdfs:label xml:lang="en">Price</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/goodrelations/v1#PriceSpecification"/>
  </owl:ObjectProperty>
  <owl:FunctionalProperty rdf:about="http://purl.org/procurement/public-contracts#contractingAuthority">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="cs">Instituce, která vypisuje veřejnou zakázku, přijímá nabídky a vybírá vhodného dodavatele na základě pravidel stanovených zakázkou. Kardinalita 0..1</rdfs:comment>
    <rdfs:comment xml:lang="en">Institution which issues a public contract, receives tenders to the contract and chooses a suitable supplier on the base of the conditions given by the contract. Cardinality 0..1</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/procurement/public-contracts#Contract"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
    <rdfs:label xml:lang="en">Contracting authority</rdfs:label>
    <rdfs:label xml:lang="cs">Zadavatel</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/goodrelations/v1#BusinessEntity"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="http://purl.org/procurement/public-contracts#criterionWeight">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">Property for criterion weight for tender evaluation. Cardinality 0..1</rdfs:comment>
    <rdfs:comment xml:lang="cs">Vlastnost pro specifikaci váhy kritéria pro hodnocení nabídky. Kardinalita 0..1</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/procurement/public-contracts#CriterionWeighting"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
