    <rdfs:label xml:lang="cs">Váha kritéria pro výběr dodavatele zakázky</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/procurement/public-contracts-datatypes#percentage"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="http://purl.org/procurement/public-contracts#documentationPrice">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">Property for price of documentation for public contract. Cardinality 0..1</rdfs:comment>
    <rdfs:comment xml:lang="cs">Vlastnost pro cenu zadávací dokumentace k zakázce. Kardinalita 0..1</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/procurement/public-contracts#Contract"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
    <rdfs:label xml:lang="cs">Cena zadávací dokumentace k zakázce</rdfs:label>
    <rdfs:label xml:lang="en">Price of documentation for public contract</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/goodrelations/v1#PriceSpecification"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="http://purl.org/procurement/public-contracts#documentationRequestDeadline">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">Property for time limit for receipt of requests for documentation. Cardinality 0..1</rdfs:comment>
    <rdfs:comment xml:lang="cs">Vlastnost pro lhůtu pro vyžádání dokumentace k veřejné zakázce. Kardinalita 0..1</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/procurement/public-contracts#Contract"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
    <rdfs:label xml:lang="cs">Lhůta pro vyžádání dokumentace k veřejné zakázce</rdfs:label>
