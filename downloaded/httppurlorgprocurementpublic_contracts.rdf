    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="http://purl.org/procurement/public-contracts#duration">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">Property for duration of contract. The duration may be described in days, months, years, etc. Cardinality 0..1</rdfs:comment>
    <rdfs:comment xml:lang="cs">Vlastnost pro dobu trvání zakázky. Doba trvání může být uvedena ve dnech, měsících, letech apod. Kardinalita 0..1</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/procurement/public-contracts#Contract"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
    <rdfs:label xml:lang="cs">Doba trvání zakázky</rdfs:label>
    <rdfs:label xml:lang="en">Duration of contract</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#duration"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="http://purl.org/procurement/public-contracts#estimatedEndDate">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">Property for estimated end date of public contract realization. Cardinality 0..1</rdfs:comment>
    <rdfs:comment xml:lang="cs">Vlastnost pro očekávané datum konce realizace zakázky. Kardinalita 0..1</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/procurement/public-contracts#Contract"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
    <rdfs:label xml:lang="en">Estimated end date of contract</rdfs:label>
    <rdfs:label xml:lang="cs">Očekávané datum konce realizace zakázky</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="http://purl.org/procurement/public-contracts#estimatedPrice">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">Property for estimated price of public contract. It can be an exact estimation or an interval. Cardinality 0..1</rdfs:comment>
    <rdfs:comment xml:lang="cs">Vlastnost pro odhadovanou cenu veřejné zakázky. Může jít o přesně odhadovanou částku nebo interval. Kardinalita 0..1</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
    <rdfs:label xml:lang="en">Exact estimated price</rdfs:label>
    <rdfs:label xml:lang="cs">Odhadovaná cena zakázky</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/procurement/public-contracts#contractPrice"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="http://purl.org/procurement/public-contracts#expectedNumberOfOperators">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">Expected number of operators of framework agreement. Cardinality 0..1</rdfs:comment>
    <rdfs:comment xml:lang="cs">Očekávaný počet účastníků rámcové smlouvy. Kardinalita 0..1</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/procurement/public-contracts#FrameworkAgreement"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
    <rdfs:label xml:lang="en">Expected number of operators of framework agreement</rdfs:label>
    <rdfs:label xml:lang="cs">Očekávaný počet účastníků rámcové smlouvy</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/goodrelations/v1#QuantitativeValueInteger"/>
  </owl:FunctionalProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/procurement/public-contracts#frameworkAgreement">
    <rdfs:comment xml:lang="en">Property for a framework agreement on which base a public contract was issued. Cardinality 0..1</rdfs:comment>
    <rdfs:comment xml:lang="cs">Vlastnost pro rámcovou smlouvu, na jejímž základě byla veřejná zakázka zadána. Kardinalita 0..1</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/procurement/public-contracts#Contract"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
    <rdfs:label xml:lang="cs">Na základě rámcové smlouvy</rdfs:label>
    <rdfs:label xml:lang="en">On the base of a framework agreement</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/procurement/public-contracts#FrameworkAgreement"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/procurement/public-contracts#item">
    <rdfs:comment xml:lang="en">An item inquired by a public contract. It can be a combination of several products or services which can be expressed using properties of gr:Offering. Cardinality 0..*</rdfs:comment>
    <rdfs:comment xml:lang="cs">Položka poptávaná v rámci veřejné zakázky. Může být kombinací několika produktů či služeb, což lze vyjádřit pomocí vlastností gr:Offering. Kardinalita 0..*</rdfs:comment>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="http://purl.org/procurement/public-contracts#Contract"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="http://purl.org/procurement/public-contracts#Tender"/>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>
          </rdf:Description>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
    <rdfs:range rdf:resource="http://purl.org/goodrelations/v1#Offering"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/hasPart"/>
  </owl:ObjectProperty>
  <owl:FunctionalProperty rdf:about="http://purl.org/procurement/public-contracts#kind">
    <rdf:type rdf:resource="http://purl.org/linked-data/cube#CodedProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="cs">Kategorie veřejné zakázky specifikuje základní zaměření veřejné zakázky na stavební práce, dodávku zboží nebo služby. Lze použít pouze jeden z předdefinovaných SKOS konceptů. Kardinalita 0..1</rdfs:comment>
    <rdfs:comment xml:lang="en">Kind of a public contract specifies a basic focus of the contract on works, supplies or services. Only a predefined SKOS concept can be used. Cardinality 0..1</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/procurement/public-contracts#Contract"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
    <rdfs:label xml:lang="en">Contract kind</rdfs:label>
    <rdfs:label xml:lang="cs">Kategorie zakázky</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:range>
      <owl:Restriction>
        <owl:hasValue rdf:resource="http://purl.org/procurement/public-contracts-kinds#"/>
        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/type"/>
  </owl:FunctionalProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/procurement/public-contracts#location">
    <rdfs:comment xml:lang="cs">Místo, kde dojde k dodání produktu nebo plnění služby nasmlouvané ve veřejné zakázce. Kardinalita 0..*</rdfs:comment>
    <rdfs:comment xml:lang="en">Place of delivery of contracted product or performance of contracted service. Cardinality 0..*</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/procurement/public-contracts#Contract"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
    <rdfs:label xml:lang="cs">Místo uskutečnění zakázky</rdfs:label>
    <rdfs:label xml:lang="en">Place of contract's realization</rdfs:label>
    <rdfs:range rdf:resource="http://schema.org/Place"/>
    <rdfs:subPropertyOf rdf:resource="http://schema.org/location"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/procurement/public-contracts#lot">
    <rdfs:comment xml:lang="en">Property for specifying contract lot. When contract is too big, it is split to lots by contracting authority. Bidders then tender each lot separately. For each lot the authority awards a separate tender. The contract itself has no tenders. Cardinality 0..*</rdfs:comment>
    <rdfs:comment xml:lang="cs">Vlastnost pro specifikaci části zakázky. Zakázku rozděluje na části zadavatel, pokud je zakázka přiliš velká. Zájemci o zakázku podávají nabídky na každou část odděleně a pro každou část zakázky je vybrán samostatný dodavatel. Pro samotnou zakázku nabídky nejsou podávány. Kardinalita 0..*</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/procurement/public-contracts#Contract"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
    <rdfs:label xml:lang="en">Contract lot</rdfs:label>
    <rdfs:label xml:lang="cs">Část zakázky</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/procurement/public-contracts#Contract"/>
  </owl:ObjectProperty>
  <owl:FunctionalProperty rdf:about="http://purl.org/procurement/public-contracts#mainActivity">
    <rdf:type rdf:resource="http://purl.org/linked-data/cube#CodedProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">Property for main activity of contracting authority. Only a predefined SKOS concept can be used. Cardinality 0..1</rdfs:comment>
    <rdfs:comment xml:lang="cs">Vlastnost pro hlavní aktivitu zadavatele. Lze použít pouze jeden z předdefinovaných SKOS konceptů. Kardinalita 0..1</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/goodrelations/v1#BusinessEntity"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
    <rdfs:label xml:lang="cs">Hlavní aktivita</rdfs:label>
    <rdfs:label xml:lang="en">Main activity</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:range>
      <owl:Restriction>
        <owl:hasValue rdf:resource="http://purl.org/procurement/public-contracts-activities#"/>
        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
      </owl:Restriction>
    </rdfs:range>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="http://purl.org/procurement/public-contracts#mainObject">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">CPV (Common Procurement Vocabulary) code describing the main object of a public contract. Cardinality 0..1</rdfs:comment>
    <rdfs:comment xml:lang="cs">CPV (Common Procurement Vocabulary) kód popisující hlavní předmět veřejné zakázky. Kardinalita 0..1</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/procurement/public-contracts#Contract"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
    <rdfs:label xml:lang="cs">Hlavní předmět zakázky</rdfs:label>
    <rdfs:label xml:lang="en">Main object of contract</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/subject"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="http://purl.org/procurement/public-contracts#numberOfTenders">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">Property for number of tenders received. Cardinality 0..1</rdfs:comment>
    <rdfs:comment xml:lang="cs">Vlastnost pro počet obdržených nabídek. Kardinalita 0..1</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/procurement/public-contracts#Contract"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
    <rdfs:label xml:lang="en">Number of tenders received</rdfs:label>
    <rdfs:label xml:lang="cs">Počet obdržených nabídek</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="http://purl.org/procurement/public-contracts#offeredPrice">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">Property for price offered by supplier. Cardinality 0..1</rdfs:comment>
    <rdfs:comment xml:lang="cs">Vlastnost pro cenu nabídnutou zájemcem o zakázku. Kardinalita 0..1</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/procurement/public-contracts#Tender"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
    <rdfs:label xml:lang="cs">Nabídnutá cena v nabídce</rdfs:label>
    <rdfs:label xml:lang="en">Offered price in tender</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/goodrelations/v1#PriceSpecification"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/goodrelations/v1#hasPriceSpecification"/>
  </owl:FunctionalProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/procurement/public-contracts#onBehalfOf">
    <rdfs:comment xml:lang="en">Contract award on behalf of other contracting authorities. Cardinality 0..*</rdfs:comment>
    <rdfs:comment xml:lang="cs">Zadání zakázky jménem jiných zadavatelů. Kardinalita 0..*</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/procurement/public-contracts#Contract"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
    <rdfs:label xml:lang="cs">Jménem</rdfs:label>
    <rdfs:label xml:lang="en">On behalf of</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/goodrelations/v1#BusinessEntity"/>
  </owl:ObjectProperty>
  <owl:FunctionalProperty rdf:about="http://purl.org/procurement/public-contracts#procedureType">
    <rdf:type rdf:resource="http://purl.org/linked-data/cube#CodedProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="cs">Druh řízení veřejné zakázky specifikuje proceduru, podle které je zakázka uveřejňována a podle které je vybírán dodavatel. Lze použít pouze jednu z předdefinovaných hodnot. Kardinalita 0..1</rdfs:comment>
    <rdfs:comment xml:lang="en">Type of procedure of a public contract specifies a procedure which is applied to publish information about the contract and choose a supplier. Only a predefined value can be used. Cardinality 0..1</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/procurement/public-contracts#Contract"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
