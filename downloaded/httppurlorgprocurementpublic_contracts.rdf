    <rdfs:comment xml:lang="cs">Vlastnost pro datum začátku realizace zakázky, které je uvedené ve smlouvě. Kardinalita 0..1</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/procurement/public-contracts#Contract"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
    <rdfs:label xml:lang="cs">Datum začátku realizace zakázky</rdfs:label>
