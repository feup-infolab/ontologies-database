    <rdfs:comment xml:lang="cs">Vlastnost pro připojení dokumentů obsahujících zadávací dokumentaci veřejné zakázky. Kardinalita 0..*</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts"/>
