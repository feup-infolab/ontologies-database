    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:isDefinedBy rdf:resource="http://www.linkedmodel.org/2.0/schema/vaem"/>
    <dct:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A discipline is a specific area of work or study. Examples of disciplines in the engineering domain are electrical engineering, mechanical engineering and, in the financial domain, risk assessment, investment analysis.  In VAEM, discipline is used as a dimension for characterizing an ontology graph</dct:description>
  </owl:Class>
  <owl:Class rdf:about="#Dimension">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
