	<owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
	<owl:onProperty rdf:resource="http://def.seegrid.csiro.au/isotc211/iso19156/2011/sampling#shape"/>
</rdf:Description>

