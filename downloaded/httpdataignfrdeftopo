	rdfs:subClassOf topo:Bati.

topo:PisteAerodrome a owl:Class;
	rdfs:comment "Aire située sur un aérodrome, aménagée afin de servir au roulement des aéronefs, au décollage et à l’atterrissage, en dur ou en herbe."@fr;
	rdfs:isDefinedBy <http://data.ign.fr/def/topo>;
