#-----------------------------------------------------------------
# ------ Concepts of the Scheme TypeDeVegetation -------
#-----------------------------------------------------------------
# ------ direct subclasses of TypeDeVegetation -----

veg:ZoneArboree a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Espace peuplé d’arbres d’essence quelconque."@fr;
	skos:inScheme veg:liste;
	skos:prefLabel "zone arborée"@fr,"wooded area"@en.

# ------ subclasses of veg:ZoneArboree -----

veg:ForetFermeeFeuillus a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Espace peuplé d’arbres forestiers couvrant au moins 40% du sol, et composée de plus de 75% de feuillus (taux de couvert relatif)."@fr;
	skos:inScheme veg:liste;
	skos:prefLabel "forêt fermée de feuillus"@fr,"closed hardwood forest"@en;
	skos:broader bati:ZoneArboree.	

veg:ForetFermeeMixte a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Espace peuplé d’arbres forestiers couvrant au moins 40% du sol, et composée de plus de 25% et moins de 75% de feuillus (taux de couvert relatif)."@fr;
	skos:inScheme veg:liste;
	skos:prefLabel "forêt fermée mixte"@fr,"closed mixed forest"@en;
	skos:broader bati:ZoneArboree.

veg:ForetFermeeConiferes a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Espace peuplé d’arbres forestiers couvrant au moins 40% du sol et composée essentiellement de conifères (soit un taux de couvert relatif de plus de 75%)."@fr;
	skos:inScheme veg:liste;
	skos:prefLabel "forêt fermée de conifères"@fr,"closed softwood forest"@en;
	skos:broader bati:ZoneArboree.

veg:ForetOuverte a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Espace peuplé d’arbres forestiers dont le couvert absolu est compris entre 10% et 40%."@fr;
	skos:inScheme veg:liste;
	skos:prefLabel "forêt ouverte"@fr,"open forest"@en;
	skos:broader bati:ZoneArboree.

veg:Peupleraie a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Zone plantée de peupliers."@fr;
	skos:inScheme veg:liste;
	skos:prefLabel "peupleraie"@fr,"poplar plantation"@en;
	skos:broader bati:ZoneArboree.

veg:Haie a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Clôture naturelle composée d’arbres, d’arbustes, d’épines ou de branchages et servant à limiter ou à protéger un champ. Alignement d’arbres ou plantation d’arbres fruitiers dont la largeur est inférieure à 25 m."@fr;
	skos:inScheme veg:liste;
	skos:prefLabel "haie"@fr,"hedge"@en;
	skos:broader bati:ZoneArboree.
	
veg:LandeLigneuse a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Végétation spontanée qui comprend une proportion importante de plantes ligneuses (bruyères, genêts, ajoncs, épineux divers, etc.) et semi ligneuses (fougères, phragmites…) dont la hauteur ne dépasse pas 5 mètres. Cette végétation ligneuse présente un couvert végétal d’au moins 25%."@fr;
	skos:inScheme veg:liste;
	skos:prefLabel "lande ligneuse"@fr,"woody heath"@en;
	skos:broader bati:ZoneArboree.	
	
