topo:importance a owl:DatatypeProperty;
	rdfs:comment "Importance du toponyme dans son environnement. Le toponyme de valeur 1 est plus important que le toponyme de valeur 2, lui-même plus important que le toponyme de valeur 3, etc."@fr;
	rdfs:isDefinedBy <http://data.ign.fr/def/topo>;
	rdfs:domain  topo:EntiteTopographique;
	rdfs:label "importance"@fr;
	rdfs:label "importance"@en;
	rdfs:range [ a owl:DataRange;  owl:oneOf ("1" "2" "3" "4" "5" "6" "7" "8" "NC" "NR") ].

