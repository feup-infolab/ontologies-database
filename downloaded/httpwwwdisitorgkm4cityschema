    <!-- http://schema.org/location -->

    <ObjectProperty rdf:about="&schema;location">
        <rdfs:label xml:lang="en">Location</rdfs:label>
        <rdfs:label xml:lang="it">Locazione</rdfs:label>
        <rdfs:comment xml:lang="en">Property of the schema.org ontology.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà dell&apos;ontologia schema.org.</rdfs:comment>
        <rdfs:domain rdf:resource="&schema;Event"/>
        <rdfs:range rdf:resource="&schema;Place"/>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#accessToElement -->

    <ObjectProperty rdf:about="&km4c;accessToElement">
        <rdfs:label xml:lang="en">Access to element</rdfs:label>
        <rdfs:label xml:lang="it">Accede all&apos;elemento</rdfs:label>
        <rdfs:comment xml:lang="en">An entry rule refers to a specific Road element.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una regola di accesso si riferisce ad uno specifico elemento stradale.</rdfs:comment>
        <rdfs:domain rdf:resource="&km4c;EntryRule"/>
        <rdfs:range rdf:resource="&km4c;RoadElement"/>
        <inverseOf rdf:resource="&km4c;hasRule"/>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#approvedByPa -->

    <ObjectProperty rdf:about="&km4c;approvedByPa">
        <rdfs:label xml:lang="en">Approved by PA</rdfs:label>
        <rdfs:label xml:lang="it">Approvato dalla PA</rdfs:label>
        <rdfs:comment xml:lang="en">To connect a resolution to the municipality that have approved it.</rdfs:comment>
        <rdfs:comment xml:lang="it">Collega una delibera all&apos;ente pubblico che la ha emanata.</rdfs:comment>
        <rdfs:range rdf:resource="&km4c;Pa"/>
        <rdfs:domain rdf:resource="&km4c;Resolution"/>
        <inverseOf rdf:resource="&km4c;hasResolution"/>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#arrangedOnRoad -->

    <ObjectProperty rdf:about="&km4c;arrangedOnRoad">
        <rdfs:label xml:lang="en">Arranged on road</rdfs:label>
        <rdfs:label xml:lang="it">Installato nella strada</rdfs:label>
        <rdfs:comment xml:lang="en">To connect each beacon to the road in which it is installed.</rdfs:comment>
        <rdfs:comment xml:lang="it">Collega il lampione alla strada su cui è installato.</rdfs:comment>
        <rdfs:domain rdf:resource="&km4c;Beacon"/>
        <rdfs:range rdf:resource="&km4c;Road"/>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#atBusStop -->

    <ObjectProperty rdf:about="&km4c;atBusStop">
        <rdfs:label xml:lang="en">At bus stop</rdfs:label>
        <rdfs:label xml:lang="it">Alla fermata</rdfs:label>
        <rdfs:comment xml:lang="en">Each forecast refers to a specific bus stop.</rdfs:comment>
        <rdfs:comment xml:lang="it">Ogni previsione si riferisce ad una specifica fermata.</rdfs:comment>
        <rdfs:range rdf:resource="&km4c;BusStop"/>
        <rdfs:domain rdf:resource="&km4c;BusStopForecast"/>
        <inverseOf rdf:resource="&km4c;hasForecast"/>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#beginsAtJunction -->

    <ObjectProperty rdf:about="&km4c;beginsAtJunction">
        <rdfs:label xml:lang="en">Begins at junction</rdfs:label>
        <rdfs:label xml:lang="it">Inizia alla giunzione</rdfs:label>
        <rdfs:comment xml:lang="en">Each routeLink, has a beginning and an end junction.</rdfs:comment>
        <rdfs:comment xml:lang="it">Ciascun tratto della linea ha una giunzione di inizio e una giunzione di fine.</rdfs:comment>
        <rdfs:range rdf:resource="&km4c;RouteJunction"/>
        <rdfs:domain rdf:resource="&km4c;RouteLink"/>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#belongToMunicipality -->

    <ObjectProperty rdf:about="&km4c;belongToMunicipality">
        <rdfs:label xml:lang="en">Belong to municipality</rdfs:label>
        <rdfs:label xml:lang="it">Appartiene al comune</rdfs:label>
        <rdfs:comment xml:lang="en">A municipality can be divided in districts.</rdfs:comment>
        <rdfs:comment xml:lang="it">Un comune può essere suddiviso in frazioni o quartieri.</rdfs:comment>
        <rdfs:domain rdf:resource="&km4c;District"/>
        <rdfs:range rdf:resource="&km4c;Municipality"/>
        <inverseOf rdf:resource="&km4c;hasDistrict"/>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#belongToRoad -->

    <ObjectProperty rdf:about="&km4c;belongToRoad">
        <rdfs:label xml:lang="en">Belong to road</rdfs:label>
        <rdfs:label xml:lang="it">Appartiene alla strada</rdfs:label>
        <rdfs:comment xml:lang="en">Each road has street numbers.</rdfs:comment>
        <rdfs:comment xml:lang="it">Ogni strada ha i numeri civici.</rdfs:comment>
        <rdfs:range rdf:resource="&km4c;Road"/>
        <rdfs:domain rdf:resource="&km4c;StreetNumber"/>
        <inverseOf rdf:resource="&km4c;hasStreetNumber"/>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#coincideWith -->

    <ObjectProperty rdf:about="&km4c;coincideWith">
        <rdfs:label xml:lang="en">Coincide with</rdfs:label>
        <rdfs:label xml:lang="it">Coincide con</rdfs:label>
        <rdfs:comment xml:lang="en">There is no exact match between Road and AdministrativeRoad.</rdfs:comment>
        <rdfs:comment xml:lang="it">Non c&apos;è una corrispondenza esatta tra toponimi ed estese amministrative.</rdfs:comment>
        <rdfs:range rdf:resource="&km4c;AdministrativeRoad"/>
        <rdfs:domain rdf:resource="&km4c;Road"/>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#composeDirection -->

    <ObjectProperty rdf:about="&km4c;composeDirection">
        <rdfs:label xml:lang="en">Compose direction</rdfs:label>
        <rdfs:label xml:lang="it">Compone direttrice</rdfs:label>
        <rdfs:comment xml:lang="en">A railway Direction is composed by more railway element.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una direzione ferroviaria è formata da diversi elementi ferroviari.</rdfs:comment>
        <rdfs:range rdf:resource="&km4c;RailwayDirection"/>
        <rdfs:domain rdf:resource="&km4c;RailwayElement"/>
        <inverseOf rdf:resource="&km4c;consistOfElement"/>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#composeSection -->

    <ObjectProperty rdf:about="&km4c;composeSection">
        <rdfs:label xml:lang="en">Compose section</rdfs:label>
        <rdfs:label xml:lang="it">Compone sezione</rdfs:label>
        <rdfs:comment xml:lang="en">A railway Section is composed by more railway element.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una sezione ferroviaria è formata da più elementi ferroviari.</rdfs:comment>
        <rdfs:domain rdf:resource="&km4c;RailwayElement"/>
        <rdfs:range rdf:resource="&km4c;RailwaySection"/>
        <inverseOf rdf:resource="&km4c;isComposedByElement"/>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#concernLine -->

    <ObjectProperty rdf:about="&km4c;concernLine">
        <rdfs:label xml:lang="en">Concern line</rdfs:label>
        <rdfs:label xml:lang="it">Riguarda la linea</rdfs:label>
        <rdfs:comment xml:lang="en">An AVMrecord concert to a unique line.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una rilevazione AVM si riferisce ad una specifica linea del TPL.</rdfs:comment>
        <rdfs:domain rdf:resource="&km4c;AVMRecord"/>
        <rdfs:range rdf:resource="&km4c;PublicTransportLine"/>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#concerningNode -->

    <ObjectProperty rdf:about="&km4c;concerningNode">
        <rdfs:label xml:lang="en">Concerning node</rdfs:label>
        <rdfs:label xml:lang="it">Riguarda il nodo</rdfs:label>
        <rdfs:comment xml:lang="en">A maneuver takes place in correspondence of a node.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una manovra è eseguita in corrispondenza di un nodo.</rdfs:comment>
        <rdfs:domain rdf:resource="&km4c;Maneuver"/>
        <rdfs:range rdf:resource="&km4c;Node"/>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#consistOfElement -->

    <ObjectProperty rdf:about="&km4c;consistOfElement">
        <rdfs:label xml:lang="en">Consist of element</rdfs:label>
        <rdfs:label xml:lang="it">Formato da elemento</rdfs:label>
        <rdfs:comment xml:lang="en">A railway Direction is composed by more railway element.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una direzione ferroviaria è formata da più elementi ferroviari.</rdfs:comment>
        <rdfs:domain rdf:resource="&km4c;RailwayDirection"/>
        <rdfs:range rdf:resource="&km4c;RailwayElement"/>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#containsElement -->

    <ObjectProperty rdf:about="&km4c;containsElement">
        <rdfs:label xml:lang="en">Contains element</rdfs:label>
        <rdfs:label xml:lang="it">Contiene elemento</rdfs:label>
        <rdfs:comment xml:lang="en">A road is formed by more elements road.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una strada è formata da più elementi stradali.</rdfs:comment>
        <rdfs:domain rdf:resource="&km4c;Road"/>
        <rdfs:range rdf:resource="&km4c;RoadElement"/>
        <inverseOf rdf:resource="&km4c;isPartOfRoad"/>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#correspondToJunction -->

    <ObjectProperty rdf:about="&km4c;correspondToJunction">
        <rdfs:label xml:lang="en">Correspond to junction</rdfs:label>
        <rdfs:label xml:lang="it">Corrisponde alla giunzione</rdfs:label>
        <rdfs:comment xml:lang="en">Train station and good yard are located precisely on a junction.</rdfs:comment>
        <rdfs:comment xml:lang="it">Le stazioni ferroviarie e gli scali merci si trovano esattamente in corrispondenza di una giunzione ferroviaria.</rdfs:comment>
        <rdfs:range rdf:resource="&km4c;RailwayJunction"/>
        <rdfs:domain>
            <Class>
                <unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&km4c;GoodsYard"/>
                    <rdf:Description rdf:about="&km4c;TrainStation"/>
                </unionOf>
            </Class>
        </rdfs:domain>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#correspondsTo -->

    <ObjectProperty rdf:about="&km4c;correspondsTo">
        <rdfs:label xml:lang="en">Corresponds to</rdfs:label>
        <rdfs:label xml:lang="it">Corrisponde a</rdfs:label>
        <rdfs:comment xml:lang="en">There is no exact match between Road and AdministrativeRoad.</rdfs:comment>
        <rdfs:comment xml:lang="it">Non c&apos;è una corrispondenza esatta tra toponimi ed estese amministrative.</rdfs:comment>
        <rdfs:domain rdf:resource="&km4c;AdministrativeRoad"/>
        <rdfs:range rdf:resource="&km4c;Road"/>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#endAtJunction -->

    <ObjectProperty rdf:about="&km4c;endAtJunction">
        <rdfs:label xml:lang="en">End at junction</rdfs:label>
        <rdfs:label xml:lang="it">Termina alla giunzione</rdfs:label>
        <rdfs:comment xml:lang="en">A railway element is delimited by two junctions.</rdfs:comment>
        <rdfs:comment xml:lang="it">Un elemento ferroviario è delimitato da due giunzioni ferroviarie.</rdfs:comment>
        <rdfs:domain rdf:resource="&km4c;RailwayElement"/>
