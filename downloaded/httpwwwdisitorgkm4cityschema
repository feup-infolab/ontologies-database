    <!-- http://www.disit.org/km4city/schema#endingJunction -->

    <ObjectProperty rdf:about="&km4c;endingJunction">
        <rdfs:label xml:lang="en">Ending junction</rdfs:label>
        <rdfs:label xml:lang="it">Giunzione di fine</rdfs:label>
        <rdfs:comment xml:lang="en">A road link is delimited by two junctions.</rdfs:comment>
        <rdfs:comment xml:lang="it">Un tratto lineare di strada è delimitato da due giunzioni.</rdfs:comment>
        <rdfs:range rdf:resource="&km4c;Junction"/>
        <rdfs:domain rdf:resource="&km4c;RoadLink"/>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#endsAtNode -->

    <ObjectProperty rdf:about="&km4c;endsAtNode">
        <rdfs:label xml:lang="en">Ends at node</rdfs:label>
        <rdfs:label xml:lang="it">Finisce al nodo</rdfs:label>
        <rdfs:comment xml:lang="en">A road element is delimited by 2 nodes.</rdfs:comment>
        <rdfs:comment xml:lang="it">Un elemento stradale è delimitato da due nodi.</rdfs:comment>
        <rdfs:range rdf:resource="&km4c;Node"/>
        <rdfs:domain rdf:resource="&km4c;RoadElement"/>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#endsAtStop -->

    <ObjectProperty rdf:about="&km4c;endsAtStop">
        <rdfs:label xml:lang="en">Ends at stop</rdfs:label>
        <rdfs:label xml:lang="it">Finisce alla fermata</rdfs:label>
        <rdfs:comment xml:lang="en">A route section is delimited by two consecutive bus stops.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una sezione di tratta del TPL è delimitata da due fermate consecutive.</rdfs:comment>
        <rdfs:range rdf:resource="&km4c;BusStop"/>
