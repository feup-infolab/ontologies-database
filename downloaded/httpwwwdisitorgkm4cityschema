            <Restriction>
                <onProperty rdf:resource="&km4c;concentration"/>
                <qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</qualifiedCardinality>
                <onDataRange rdf:resource="&xsd;string"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Subclass of observations on auto concentration.</rdfs:comment>
        <rdfs:comment xml:lang="it">Sottoclasse delle osservazioni relative alla concentrazione di auto.</rdfs:comment>
    </Class>
    


