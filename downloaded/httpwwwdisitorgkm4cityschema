        <rdfs:comment xml:lang="en">It provides the minimum value of humidity detected by the weather station over the previous day.</rdfs:comment>
        <rdfs:comment xml:lang="it">Riporta il minimo valore di umidità rilevato dalla stazione meteo nell&apos;arco della giornata precedente.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&km4c;humidity"/>
    </DatatypeProperty>
    


    <!-- http://www.disit.org/km4city/schema#yesterdayMinWaterLevel -->

    <DatatypeProperty rdf:about="&km4c;yesterdayMinWaterLevel">
        <rdfs:label xml:lang="en">Yesterday min water level</rdfs:label>
        <rdfs:label xml:lang="it">Minimo livello delle acque giornata precedente</rdfs:label>
        <rdfs:label xml:lang="it">Valore minimo del livello delle acque registrato nell&apos;arco della giornata precedente.</rdfs:label>
        <rdfs:comment xml:lang="en">Minimum water level recorded over the previous day.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&km4c;waterLevel"/>
    </DatatypeProperty>
    


