    <!-- http://www.disit.org/km4city/schema#Entry -->

    <Class rdf:about="&km4c;Entry">
        <rdfs:label xml:lang="en">Entry</rdfs:label>
        <rdfs:label xml:lang="it">Accesso</rdfs:label>
        <rdfs:subClassOf rdf:resource="&geo;SpatialThing"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&geo;lat"/>
                <maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</maxCardinality>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&geo;long"/>
                <maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</maxCardinality>
            </Restriction>
        </rdfs:subClassOf>
        <disjointWith rdf:resource="&km4c;StreetNumber"/>
        <rdfs:comment>Classe le cui istanze sono i possibili ingressi ai numeri civici</rdfs:comment>
    </Class>
    


