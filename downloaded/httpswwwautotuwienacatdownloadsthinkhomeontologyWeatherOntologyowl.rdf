    </owl:DatatypeProperty>
    


    <!-- https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/WeatherOntology.owl#hasStartTime -->

    <owl:DatatypeProperty rdf:about="&WeatherOntology;hasStartTime">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
        <rdfs:domain rdf:resource="&time;Interval"/>
    </owl:DatatypeProperty>
    


    <!-- https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/WeatherOntology.owl#hasTime -->

    <owl:DatatypeProperty rdf:about="&WeatherOntology;hasTime">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
        <rdfs:domain rdf:resource="&time;Instant"/>
    </owl:DatatypeProperty>
    


