    <owl:DatatypeProperty rdf:about="&wgs84_pos;alt">
        <rdfs:comment xml:lang="en">The WGS84 altitude of a SpatialThing (decimal meters 
above the local reference ellipsoid).</rdfs:comment>
        <rdfs:comment xml:lang="en">altitude</rdfs:comment>
        <rdfs:domain rdf:resource="&wgs84_pos;SpatialThing"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#lat -->

    <owl:DatatypeProperty rdf:about="&wgs84_pos;lat">
        <rdfs:label>latitude</rdfs:label>
        <rdfs:comment xml:lang="en">The WGS84 latitude of a SpatialThing (decimal degrees).</rdfs:comment>
        <rdfs:domain rdf:resource="&wgs84_pos;SpatialThing"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#long -->

    <owl:DatatypeProperty rdf:about="&wgs84_pos;long">
        <rdfs:label>longitude</rdfs:label>
        <rdfs:comment xml:lang="en">The WGS84 longitude of a SpatialThing (decimal degrees)</rdfs:comment>
        <rdfs:domain rdf:resource="&wgs84_pos;SpatialThing"/>
    </owl:DatatypeProperty>
    


    <!-- https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/WeatherOntology.owl#hasCloudAltitude -->

    <owl:DatatypeProperty rdf:about="&WeatherOntology;hasCloudAltitude">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&xsd;int"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&WeatherOntology;CloudCover"/>
                    <rdf:Description rdf:about="&WeatherOntology;WeatherState"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/WeatherOntology.owl#hasCloudCoverValue -->

    <owl:DatatypeProperty rdf:about="&WeatherOntology;hasCloudCoverValue">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&WeatherOntology;CloudCover"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;nonNegativeInteger"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:maxExclusive rdf:datatype="&xsd;integer">10</xsd:maxExclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/WeatherOntology.owl#hasDirection -->

    <owl:DatatypeProperty rdf:about="&WeatherOntology;hasDirection">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">Wind direction</rdfs:comment>
        <rdfs:comment xml:lang="en">Wind direction represented in degrees</rdfs:comment>
        <rdfs:domain rdf:resource="&WeatherOntology;Wind"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;float"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:maxExclusive rdf:datatype="&xsd;float">360.0</xsd:maxExclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/WeatherOntology.owl#hasEndTime -->

    <owl:DatatypeProperty rdf:about="&WeatherOntology;hasEndTime">
