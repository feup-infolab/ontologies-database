    </owl:DatatypeProperty>
    


    <!-- https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/WeatherOntology.owl#hasSpeed -->

    <owl:DatatypeProperty rdf:about="&WeatherOntology;hasSpeed">
