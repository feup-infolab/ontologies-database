  <rdf:Description rdf:about="https://bmake.th-brandenburg.de/spv#ResMethReferenceModeling">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:comment xml:lang="en">The reference modeling produces inductively (out of observations) or deductively (eg from theories or models) mostly simplified and optimized projections (ideal concepts) of systems, in order to deepen existing knowledge and generate design templates from this.</rdfs:comment>
    <rdfs:label xml:lang="en">Reference Modeling (Wilde/Hess)</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://bmake.th-brandenburg.de/spv#"/>
    <vs:term_status>stable</vs:term_status>
    <schema:url>http://doi.org/10.1007/s11576-007-0064-z</schema:url>
    <rdf:type rdf:resource="https://bmake.th-brandenburg.de/spv#ResearchMethod"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://bmake.th-brandenburg.de/spv#ResMethActionResearch">
    <schema:url>http://doi.org/10.1007/s11576-007-0064-z</schema:url>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:label xml:lang="en">Action Research (Wilde/Hess)</rdfs:label>
    <vs:term_status>stable</vs:term_status>
    <rdf:type rdf:resource="https://bmake.th-brandenburg.de/spv#ResearchMethod"/>
    <rdfs:comment xml:lang="en">Solving a practice problem by a mixed group of scientists and practical experts. Several cycles of analysis, action and evaluation steps, each with a low level of structured instruments such as group discussions or planning games, are conducted.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="https://bmake.th-brandenburg.de/spv#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://bmake.th-brandenburg.de/spv#ResMethLiteratureAnalysis">
    <schema:url>https://lccn.loc.gov/2015048510</schema:url>
    <rdfs:comment xml:lang="en">A literature review is a written document that presents a logically argued case founded on a comprehensive understanding of the current state of knowledge about a topic of study. This case establishes a convincing thesis to answer the study’s question.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:label xml:lang="en">Literature Analysis (Machi/McEvoy)</rdfs:label>
    <rdf:type rdf:resource="https://bmake.th-brandenburg.de/spv#ResearchMethod"/>
    <vs:term_status>stable</vs:term_status>
    <rdfs:isDefinedBy rdf:resource="https://bmake.th-brandenburg.de/spv#"/>
  </rdf:Description>
