	 <owl:Class rdf:ID="Requirement">		
		<rdfs:label xml:lang="en">Requirement</rdfs:label>
		<rdfs:comment xml:lang="en">The application to secure has requirements (e.g., high sensitive data or specific technologies).</rdfs:comment>
	</owl:Class>
	
	 <owl:Class rdf:ID="DataTypeSensitive">		
		<rdfs:label xml:lang="en">DataTypeSensitive</rdfs:label>
		<rdfs:comment xml:lang="en">DataTypeSensitive (low, medium or high sensitive). The data to secure.</rdfs:comment>
	</owl:Class>
     
     <owl:Class rdf:ID="Technology">		
		<rdfs:label xml:lang="en">Technology</rdfs:label>
		<rdfs:comment xml:lang="en">Technologies can be wireless (Wi-Fi, Wimax), cellular (2G, 3G, 4G), sensor  networks, web application, etc.</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#Requirement"/>
	</owl:Class>
	
	<owl:Class rdf:ID="Architecture">		
		<rdfs:label xml:lang="en">Architecture</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
	</owl:Class>
	
	<owl:Class rdf:ID="ArchitectureComponent">		
		<rdfs:label xml:lang="en">Architecture component</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
	</owl:Class>
		
	<owl:Class rdf:ID="Feature">		
		<rdfs:label xml:lang="en">Feature</rdfs:label>
		<rdfs:comment xml:lang="en">Features of security mechanisms. For example a sensor technology is low energy consuming, and the symmetric algorithm (a SecurityMechanism) is low energy consumming.</rdfs:comment>
	</owl:Class>	
	
	<owl:Class rdf:ID="PositiveFeature">		
		<rdfs:label xml:lang="en">Positive Feature</rdfs:label>
		<rdfs:comment xml:lang="en">Positive Feature of security mechanisms</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#Feature"/>
	</owl:Class>
	
	<owl:Class rdf:ID="NegativeFeature">		
		<rdfs:label xml:lang="en">Negative Feature</rdfs:label>
		<rdfs:comment xml:lang="en">Negative Feature of security mechanisms</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#Feature"/>
	</owl:Class>	
	
	<owl:Class rdf:ID="SecurityProperty">		
		<rdfs:label xml:lang="en">SecurityProperty</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
		<rdfs:subClassOf rdf:resource="#Requirement"/>
		<rdfs:seeAlso rdf:resource="&herzog;Goal"/>
		<rdfs:seeAlso rdf:resource="&denker;SecurityNotation"/>
		<rdfs:seeAlso rdf:resource="&kim;SecurityObjective"/>
		<rdfs:seeAlso rdf:resource="&maso;Objectif"/>
		<rdfs:seeAlso rdf:resource="&securitysw;Services"/>
		<rdfs:seeAlso rdf:resource="&secberlin;SecurityProtectionMechanism"/>
	</owl:Class>
	
	<owl:Class rdf:ID="Attack">		
		<rdfs:label xml:lang="en">Attack</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
	</owl:Class>
	
	<!-- SecurityMechanism -->
	
	<owl:Class rdf:ID="SecurityMechanism">		
		<rdfs:label xml:lang="en">SecurityMechanism</rdfs:label>
		<rdfs:comment xml:lang="en">Security Mechanisms are processes that are used to detect, prevent and recover from security attacks.</rdfs:comment>
		<dc:description xml:lang="en">Security Mechanisms are processes that are used to detect, prevent and recover from security attacks.</dc:description>
	</owl:Class>
	
	<owl:Class rdf:ID="SecurityTool">		
		<rdfs:label xml:lang="en">Security Tool</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
		<rdfs:subClassOf rdf:resource="#SecurityMechanism"/>	
	</owl:Class>
		
	<owl:Class rdf:ID="AuthenticationServer">		
		<rdfs:label xml:lang="en">Authentication Server (AS)</rdfs:label>
		<rdfs:comment xml:lang="en">It identifies the supplicant, checks its credentials, and define privileges and restrictions, and allows or denies it access to the network and services.</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#AuthenticationMethod"/>	
	</owl:Class>	
	
	<owl:Class rdf:ID="Obfuscation">		
		<rdfs:label xml:lang="en">Obfuscation</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
		<rdfs:subClassOf rdf:resource="#SecurityTool"/>	
		<rdfs:subClassOf>
			<owl:Restriction>
            	<owl:onProperty rdf:resource="#occursInLayer"/>
             	<owl:someValuesFrom rdf:resource="#ApplicationLayer"/>
        	</owl:Restriction>
        </rdfs:subClassOf>
	</owl:Class>
	
	<owl:Class rdf:ID="NetworkManagementSecurityTool">		
		<rdfs:label xml:lang="en">Network Management Security Tool</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
		<rdfs:subClassOf rdf:resource="#SecurityTool"/>	
		<rdfs:subClassOf rdf:resource="#NetworkManagementSecurityMechanism"/>	
	</owl:Class>
	
	<owl:Class rdf:ID="MessageEncryptionTool">		
		<rdfs:label xml:lang="en">Message Encryption Tool</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
		<rdfs:subClassOf rdf:resource="#SecurityTool"/>	
	</owl:Class>
	
	<owl:Class rdf:ID="Protocol">		
		<rdfs:label xml:lang="en">Protocol</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
		<rdfs:subClassOf rdf:resource="#SecurityMechanism"/>	
	</owl:Class>

	<!-- Model OSI: 7 layers -->
	
	<owl:Class rdf:ID="OSIModel">		
		<rdfs:label xml:lang="en"></rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
        <owl:oneOf rdf:parseType="Collection">
          <rdf:Description rdf:about="#PhysicalLayer"/>
          <rdf:Description rdf:about="#LinkLayer"/>
          <rdf:Description rdf:about="#NetworkLayer"/>
          <rdf:Description rdf:about="#TransportLayer"/>
          <rdf:Description rdf:about="#SessionLayer"/>	  
          <rdf:Description rdf:about="#PresentationLayer"/>
          <rdf:Description rdf:about="#ApplicationLayer"/>
        </owl:oneOf>
	</owl:Class>
	
