	<owl:Class rdf:ID="OSIModelLayer">		
		<rdfs:label xml:lang="en">OSI Model Layer</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
	</owl:Class>
		
	<!-- ***************************** STAC ONTOLOGY PROPERTIES *************************************** -->

	<owl:ObjectProperty rdf:ID="isReplacedBy">
		<rdf:type rdf:resource="&owl;ReflexiveProperty"/>
		<rdfs:label xml:lang="en">isReplacedBy</rdfs:label>
		<rdfs:comment xml:lang="en">When a SecurityMechanism is not secured anymore, it is replaced by a more secure SecurityMechanism. This is also the case for technologies: a technology can be replaced by another one more recent. (E.G., 2G has been replaced by 3G)</rdfs:comment>
		<rdfs:domain rdf:resource="#SecurityMechanism"/>		
		<rdfs:range rdf:resource="#SecurityMechanism"/>		
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:ID="occursInLayer">
		<rdfs:domain rdf:resource="#Attack"/>		
		<rdfs:range rdf:resource="#OSIModelLayer"/>		
		<rdfs:label xml:lang="en">occursInLayer</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:ID="protectsInLayer">
		<rdfs:domain rdf:resource="#SecurityMechanism"/>		
		<rdfs:range rdf:resource="#OSIModelLayer"/>		
		<rdfs:label xml:lang="en">protectsInLayer</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:ID="hasSecurityMechanism">
		<owl:inverseOf>
        	<owl:ObjectProperty rdf:about="#isSecurityMechanismOf" />
     	</owl:inverseOf>
		<rdfs:domain rdf:resource="#Attack"/>		
		<rdfs:range rdf:resource="#SecurityMechanism"/>		
		<rdfs:label xml:lang="en">hasSecurityMechanism</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
	</owl:ObjectProperty>
	
	    
    <owl:ObjectProperty rdf:ID="satisfies">
    	<rdfs:label xml:lang="en">satisfies</rdfs:label>
		<rdfs:comment xml:lang="en">A security mechanism (e.g., VPN) satisfies some security properties (e.g., authentication)</rdfs:comment>
 		<rdfs:domain rdf:resource="#SecurityMechanism" />    
 		<rdfs:range  rdf:resource="#SecurityProperty"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:ID="threatens">
		<rdfs:label xml:lang="en">threatens</rdfs:label>
		<rdfs:comment xml:lang="en">An attack (e.g., eavesdropping) threatens some security properties (e.g., confidentiality)</rdfs:comment>
 		<rdfs:domain rdf:resource="#Attack" />    
 		<rdfs:range  rdf:resource="#SecurityProperty"/>
	</owl:ObjectProperty>  
	
	<owl:ObjectProperty rdf:ID="isSecurityMechanismOf"> 
	   	<owl:inverseOf>
        	<owl:ObjectProperty rdf:about="#hasSecurityMechanism" />
     	</owl:inverseOf>
     	<rdfs:domain rdf:resource="#SecurityMechanism"/>		
		<rdfs:range rdf:resource="#Attack"/>		
		<rdfs:label xml:lang="en">isSecurityMechanismOf</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
     </owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:ID="isProtectedBy"> 
	   	<owl:inverseOf>
        	<owl:ObjectProperty rdf:about="#protects" />
     	</owl:inverseOf>
     	<rdfs:domain rdf:resource="#Technology"/>		
		<rdfs:range rdf:resource="#SecurityMechanism"/>		
		<rdfs:label xml:lang="en">isProtectedBy</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
     </owl:ObjectProperty>
     
     	<owl:ObjectProperty rdf:ID="protects"> 
	   	<owl:inverseOf>
        	<owl:ObjectProperty rdf:about="#isProtectedBy" />
     	</owl:inverseOf>
     	<rdfs:domain rdf:resource="#SecurityMechanism"/>		
		<rdfs:range rdf:resource="#Technology"/>		
		<rdfs:label xml:lang="en">protects</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
     </owl:ObjectProperty>
     
     <owl:ObjectProperty rdf:ID="hasVulnerability"> 
     	<owl:inverseOf>
        	<owl:ObjectProperty rdf:about="#isVulnerabilityOf" />
     	</owl:inverseOf>
     	<rdfs:domain rdf:resource="#Technology"/>		
		<rdfs:range rdf:resource="#Attack"/>		
		<rdfs:label xml:lang="en">has vulnerability</rdfs:label>
		<rdfs:comment xml:lang="en">A technology can be vulnerable to some attacks.</rdfs:comment>
     </owl:ObjectProperty>
     
     <owl:ObjectProperty rdf:ID="isVulnerabilityOf"> 
     	<owl:inverseOf>
        	<owl:ObjectProperty rdf:about="#hasVulnerability" />
     	</owl:inverseOf>
     	<rdfs:domain rdf:resource="#Attack"/>		
		<rdfs:range rdf:resource="#Technology"/>		
		<rdfs:label xml:lang="en">is Vulnerability Of</rdfs:label>
		<rdfs:comment xml:lang="en">Some attacks cannot be thwart in technologies. For example, the Denial of Service is difficult to thwart in different technologies.</rdfs:comment>
     </owl:ObjectProperty>          
     		
	<owl:ObjectProperty rdf:ID="hasFeature">	
		<rdfs:label xml:lang="en">hasFeature</rdfs:label>
		<rdfs:comment xml:lang="en">A SecurityMechanism has specific features (secured, depreceted, low energy consuming). For example, an asymmetric algorithm is high energy consuming.</rdfs:comment>
	    <rdfs:domain rdf:resource="#SecurityMechanism"/>		
		<rdfs:range rdf:resource="#Feature"/>	
		<owl:inverseOf>
        	<owl:ObjectProperty rdf:about="#isFeatureOf"/>
     	</owl:inverseOf>
	</owl:ObjectProperty>	
	
		<owl:ObjectProperty rdf:ID="isFeatureOf">	
		<rdfs:label xml:lang="en">isFeatureOf</rdfs:label>
		<rdfs:comment xml:lang="en">Features are related to security mechanisms. For example, an asymmetric algorithm is high energy consuming.</rdfs:comment>
	    <rdfs:domain rdf:resource="#Feature"/>		
		<rdfs:range rdf:resource="#SecurityMechanism"/>	
		<owl:inverseOf>
        	<owl:ObjectProperty rdf:about="#hasFeature"/>
     	</owl:inverseOf>
	</owl:ObjectProperty>	
	
	<owl:ObjectProperty rdf:ID="hasArchitecture">
		<rdfs:domain rdf:resource="#Technology"/>		
		<rdfs:range rdf:resource="#Architecture"/>		
		<rdfs:label xml:lang="en">hasArchitecture</rdfs:label>
		<rdfs:comment xml:lang="en">A technology has a specific architecture.</rdfs:comment>
	</owl:ObjectProperty>	
	
	<!-- ################################################ attack type ############################################## -->
	
	<owl:Class rdf:ID="Worm">		
		<rdfs:label xml:lang="en">Worm</rdfs:label>
		<rdfs:comment xml:lang="en">Worms are self-propagating pieces of malicious software. They propagate from one computer/device to another via a network link.</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#WebAttack"/>
		<rdfs:subClassOf rdf:resource="#GPRSAttack"/>
		<dc:description xml:lang="en">See Paper: Malicious actions against the GPRS technology [Xenakis 2006]</dc:description>   
		<rdfs:subClassOf>
			<owl:Restriction>
            	<owl:onProperty rdf:resource="#occursInLayer"/>
             	<owl:someValuesFrom rdf:resource="#ApplicationLayer"/>
        	</owl:Restriction>
        </rdfs:subClassOf>
	</owl:Class>

	<owl:Class rdf:ID="Virus">		
		<rdfs:label xml:lang="en">Virus</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
		<rdfs:subClassOf rdf:resource="#GPRSAttack"/>
		<rdfs:subClassOf rdf:resource="#RfidAttack"/>
		<rdfs:subClassOf rdf:resource="#WebAttack"/>
		<rdfs:subClassOf rdf:resource="#NetworkManagementAttack"/>
		<dc:description xml:lang="en">See Paper: Malicious actions against the GPRS technology [Xenakis 2006], Paper: Survey of Security and Privacy Issues of Internet of Things [Borgohain et al. 2015]</dc:description> 
		<rdfs:subClassOf>
			<owl:Restriction>
            	<owl:onProperty rdf:resource="#hasSecurityMechanism"/>
             	<owl:someValuesFrom rdf:resource="#Antivirus"/>
        	</owl:Restriction>
        </rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
            	<owl:onProperty rdf:resource="#occursInLayer"/>
             	<owl:someValuesFrom rdf:resource="#ApplicationLayer"/>
        	</owl:Restriction>
        </rdfs:subClassOf>
	</owl:Class>
	
	<owl:Class rdf:ID="Spam">		
		<rdfs:label xml:lang="en">Spam</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
		<rdfs:subClassOf rdf:resource="#WebAttack"/>
		<rdfs:subClassOf>
			<owl:Restriction>
            	<owl:onProperty rdf:resource="#hasSecurityMechanism"/>
             	<owl:someValuesFrom rdf:resource="#AntiSpam"/>
        	</owl:Restriction>
        </rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
            	<owl:onProperty rdf:resource="#occursInLayer"/>
             	<owl:someValuesFrom rdf:resource="#ApplicationLayer"/>
        	</owl:Restriction>
        </rdfs:subClassOf>
	</owl:Class>
	
	<owl:Class rdf:ID="Spyware">		
		<rdfs:label xml:lang="en">Spyware</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
		<rdfs:subClassOf rdf:resource="#WebAttack"/>
	</owl:Class>
	
	<owl:Class rdf:ID="Troyan">		
		<rdfs:label xml:lang="en">Troyan</rdfs:label>
		<rdfs:comment xml:lang="en">A Trojan horse is a malicious piece of software hidden in a program that performs normal tasks. Program that does something malicious (such as capturing passwords) when run by an unsuspecting user.</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#Virus"/>
		<dc:description xml:lang="en">See Paper: Malicious actions against the GPRS technology [Xenakis 2006]</dc:description>   
		<rdfs:subClassOf>
			<owl:Restriction>
            	<owl:onProperty rdf:resource="#isVulnerabilityOf"/>
             	<owl:someValuesFrom rdf:resource="#WebAttack"/>
        	</owl:Restriction>
        </rdfs:subClassOf>
	</owl:Class>
	
	<owl:Class rdf:ID="PolymorphicVirus">		
		<rdfs:label xml:lang="en">Polymorphic Virus</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
		<rdfs:subClassOf rdf:resource="#Virus"/>
	</owl:Class>
	
	<!-- ################################################ SecurityMechanism type ############################################## -->

	<owl:Class rdf:ID="Antivirus">		
		<rdfs:label xml:lang="en">Antivirus</rdfs:label>
		<rdfs:comment xml:lang="en">Anti-viruses softwares applications will reduce viruses and malware on M2M equipment.</rdfs:comment>
		<dc:description xml:lang="en">See Paper: A study of information security for M2M and IoT [Jiang et al. 2010]</dc:description>   
		<rdfs:subClassOf rdf:resource="#WebSecurityMechanism"/>
		<rdfs:subClassOf rdf:resource="#M2MSecurityMechanism"/>
		<rdfs:subClassOf rdf:resource="#NetworkManagementSecurityMechanism"/>
		<rdfs:subClassOf rdf:resource="#IntegrityMethod"/>	
	</owl:Class>
	
	<owl:Class rdf:ID="Honeypot">		
		<rdfs:label xml:lang="en">Honeypot</rdfs:label>
