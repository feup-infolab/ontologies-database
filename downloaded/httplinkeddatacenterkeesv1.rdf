<!DOCTYPE rdf:RDF [
    <!ENTITY void "http://rdfs.org/ns/void#" >
    <!ENTITY dcterms "http://purl.org/dc/terms/" >
    <!ENTITY prov "http://www.w3.org/ns/prov#" >
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY v1 "http://linkeddata.center/kees/v1#" >
    <!ENTITY llo "http://lodlaundromat.org/ontology/" >
    <!ENTITY wv "http://vocab.org/waiver/terms/norms" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY sd "http://www.w3.org/ns/sparql-service-description#" >
]>


<rdf:RDF xmlns="http://linkeddata.center/kees/v1#"
     xml:base="http://linkeddata.center/kees/v1"
     xmlns:void="http://rdfs.org/ns/void#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:llo="http://lodlaundromat.org/ontology/"
     xmlns:sd="http://www.w3.org/ns/sparql-service-description#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:v1="http://linkeddata.center/kees/v1#"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:wv="http://vocab.org/waiver/terms/norms">
    <owl:Ontology rdf:about="http://linkeddata.center/kees/v1">
        <rdfs:label xml:lang="en">KEES Ontology</rdfs:label>
        <owl:versionInfo>V 1.1.19 Release 2016-02-04</owl:versionInfo>
        <dc:creator xml:lang="en">Enrico Fagnoni</dc:creator>
        <dc:description xml:lang="en">KEES (Knowledge Exchenge Engine Schema ) ontology describes a knowledge base configuration in terms of ABox and TBox statements together with their accrual and reasoning policies.  This vocabulary is designed to drive automatic data ingestion in a graph database according KEES (Knowledge Exchange Engine Schema) and Linked (Open) Data principles.  For a pratical use  of KEES ontology see  http://linkeddata.center/help/devop/kees-profile article.</dc:description>
        <dc:subject xml:lang="en">Knowledge Exchange Engine Schema Ontology.</dc:subject>
        <dc:title xml:lang="en">The KEES Vocabulary for knowledge  base management</dc:title>
        <dc:rights xml:lang="en">The KEES ontology is available under the Creative Commons Attribution 3.0 Unported license; see http://creativecommons.org/licenses/by/3.0/. In a nutshell, you are free to copy, distribute and transmit the work; to remix/adapt the work (e.g. to import the ontology and create specializations of its elements), as long as you attribute the work in the manner specified by the author or licensor (but not in any way that suggests that they endorse you or your use of the work). Proper Attribution: Simply include the statement &quot;This work is based on the KEES ontology, developed by Enrico Fagnoni&quot; and link back to http:/linkeddata.center/kees/v1</dc:rights>
        <dcterms:license rdf:resource="http://creativecommons.org/licenses/by/4.0/"/>
        <rdfs:seeAlso rdf:resource="http://linkeddata.center/help/devop/kees-profile"/>
        <foaf:homepage rdf:resource="http://linkeddata.center/kees/"/>
        <dc:description rdf:resource="http://linkeddata.center/kees/images/cycle.png"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/contributor -->

    <owl:AnnotationProperty rdf:about="&dc;contributor"/>
    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:AnnotationProperty rdf:about="&dc;creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="&dc;description"/>
    


    <!-- http://purl.org/dc/elements/1.1/modifed -->

    <owl:AnnotationProperty rdf:about="&dc;modifed"/>
    


    <!-- http://purl.org/dc/elements/1.1/rights -->

    <owl:AnnotationProperty rdf:about="&dc;rights"/>
    


    <!-- http://purl.org/dc/elements/1.1/subject -->

    <owl:AnnotationProperty rdf:about="&dc;subject"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="&dc;title"/>
    


    <!-- http://purl.org/dc/terms/license -->

    <owl:AnnotationProperty rdf:about="&dcterms;license"/>
    


    <!-- http://www.w3.org/2002/07/owl#deprecated -->

    <owl:AnnotationProperty rdf:about="&owl;deprecated"/>
    


    <!-- http://xmlns.com/foaf/0.1/homepage -->

    <owl:AnnotationProperty rdf:about="&foaf;homepage"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#success -->

    <rdf:Description rdf:about="&xsd;success">
        <rdfs:label xml:lang="en">has input</rdfs:label>
        <owl:priorVersion xml:lang="en">task  processor input as a json structure.</owl:priorVersion>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://linkeddata.center/kees/v1#hasABoxGraph -->

    <owl:ObjectProperty rdf:about="&v1;hasABoxGraph">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has ABox graph</rdfs:label>
        <rdfs:comment xml:lang="en">The related resource is one of the ABox graphs that contain linked data facts in the described  knowlege base.</rdfs:comment>
        <rdfs:range rdf:resource="&v1;ABoxGraph"/>
        <rdfs:domain rdf:resource="&v1;KnowledgeBase"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasAccrualPolicy -->

    <owl:ObjectProperty rdf:about="&v1;hasAccrualPolicy">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has accrual policy</rdfs:label>
        <rdfs:comment xml:lang="en">The related resource is a policy governing the addition of a named graph to the described knowledge base. Very similar to the dct:accrualPolicy property.</rdfs:comment>
        <rdfs:range rdf:resource="&v1;KnowledgeAccrualPolicy"/>
        <rdfs:domain rdf:resource="&v1;KnowledgeBase"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasBootGraph -->

    <owl:ObjectProperty rdf:about="&v1;hasBootGraph">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has boot graph</rdfs:label>
        <rdfs:comment xml:lang="en">This property states that annotated knowledge base as a referened boot graph.</rdfs:comment>
        <rdfs:range rdf:resource="&v1;BootGraph"/>
        <rdfs:domain rdf:resource="&v1;KnowledgeGraph"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasConfigGraph -->

    <owl:ObjectProperty rdf:about="&v1;hasConfigGraph">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has config graph</rdfs:label>
        <rdfs:comment xml:lang="en">The related resource is one of the configuration graph that caracterize the described knowledge base.</rdfs:comment>
        <rdfs:range rdf:resource="&v1;KBConfigGraph"/>
        <rdfs:domain rdf:resource="&v1;KnowledgeBase"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasInferenceGraph -->

    <owl:ObjectProperty rdf:about="&v1;hasInferenceGraph">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has inference graph</rdfs:label>
        <rdfs:comment xml:lang="en">The related resource is the graph where reasoners places all materialized triples.</rdfs:comment>
        <rdfs:range rdf:resource="&v1;InferredKnowledgeGraph"/>
        <rdfs:domain rdf:resource="&v1;KnowledgeBase"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasIngestionActivityReportGraph -->

    <owl:ObjectProperty rdf:about="&v1;hasIngestionActivityReportGraph">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has ingestion activity report graph</rdfs:label>
        <rdfs:comment xml:lang="en">The related resource is a graph used to store logs  about a kees cycle executions.</rdfs:comment>
        <rdfs:range rdf:resource="&v1;IngestionActivityReportGraph"/>
        <rdfs:domain rdf:resource="&v1;KnowledgeBase"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasPagingData -->

    <owl:ObjectProperty rdf:about="&v1;hasPagingData">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has Paging Data</rdfs:label>
        <rdfs:comment xml:lang="en">The related resource is a dataset that contains the page info about the annotated paged resource.</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;Page"/>
        <rdfs:range rdf:resource="&v1;PagingData"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasReasoningRule -->

    <owl:ObjectProperty rdf:about="&v1;hasReasoningRule">
        <rdfs:label xml:lang="en">hasReasoningPolicy</rdfs:label>
        <rdfs:comment xml:lang="en">this property states that the annotated Knowledge Accrual has a reasoning policy.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasTBoxGraph -->

    <owl:ObjectProperty rdf:about="&v1;hasTBoxGraph">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has TBox graph</rdfs:label>
        <rdfs:comment xml:lang="en">The related resource is one of the ABox graphs that contain vocabularies and taxonomies used in the described  knowlege base.</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;KnowledgeBase"/>
        <rdfs:range rdf:resource="&v1;TBoxGraph"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasUpdatePolicy -->

    <owl:ObjectProperty rdf:about="&v1;hasUpdatePolicy">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has update policy</rdfs:label>
        <rdfs:comment xml:lang="en">The related resource is a policy that suggest a time frame, expressed in seconds after witch the described accrual policy should check data for update.  If not specified , a period of one hour is guessed . A policy that specify a long interval greatly improves ingestion engines performances.</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;KnowledgeAccrualPolicy"/>
        <rdfs:range rdf:resource="&v1;UpdatePolicy"/>
        <rdfs:seeAlso rdf:resource="&dcterms;accrualPeriodicity"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#nextPage -->

    <owl:ObjectProperty rdf:about="&v1;nextPage">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">next page</rdfs:label>
        <rdfs:comment xml:lang="en">States that referred Page has a next page.</rdfs:comment>
        <rdfs:range rdf:resource="&v1;Page"/>
        <rdfs:domain rdf:resource="&v1;Page"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#onFetchingError -->

    <owl:ObjectProperty rdf:about="&v1;onFetchingError">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">on fetching error</rdfs:label>
        <rdfs:comment xml:lang="en">The related resource declares the policy to adopt when an error occurs during the execution of the described accrual policy. If not present any error in data loading removes the related graph the in knowledge base.</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;KnowledgeAccrualPolicy"/>
        <rdfs:range rdf:resource="&v1;RetentionPolicy"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#prevPage -->

    <owl:ObjectProperty rdf:about="&v1;prevPage">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">previous page</rdfs:label>
        <rdfs:comment xml:lang="en">states that referred Page has a previous page</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;Page"/>
        <rdfs:range rdf:resource="&v1;Page"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#requiresAuthentication -->

    <owl:ObjectProperty rdf:about="&v1;requiresAuthentication">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">requires authentication</rdfs:label>
        <rdfs:comment xml:lang="en">The related resource is an authentication method to be used to access dataset during the execution of the described accrual policy. If not present, no authentication required.</rdfs:comment>
        <rdfs:range rdf:resource="&v1;AuthenticationPolicy"/>
        <rdfs:domain rdf:resource="&v1;KnowledgeAccrualPolicy"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://linkeddata.center/kees/v1#byteCount -->

    <owl:DatatypeProperty rdf:about="&v1;byteCount">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">byte count</rdfs:label>
        <rdfs:comment xml:lang="en">is the number of bytes processed by an ingestion activity.</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;SuccessReport"/>
        <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#constructQuery -->

    <owl:DatatypeProperty rdf:about="&v1;constructQuery">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">construct query</rdfs:label>
        <rdfs:comment xml:lang="en">The SPARQL 1.1.  query to extract data from a SPARQL endpoint. No paging constructs allowed ( OFSET and LIMIT ), use pageSize, fromPage and toPage instead. If not specified &quot;CONSTRUCT { ?s ?p ?o }  WHERE { ?s ?p ?o }&quot; apply</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;SparqlServiceAccrualPolicy"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#createsGraphName -->

    <owl:DatatypeProperty rdf:about="&v1;createsGraphName">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>creates graph name</rdfs:label>
        <rdfs:comment xml:lang="en">The related uri is used as as the name of the graph produced according the described accrual policy. If not specified the same value of kees:url is inferred.</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;KnowledgeAccrualPolicy"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#executionPriority -->

    <owl:DatatypeProperty rdf:about="&v1;executionPriority">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">execution priority</rdfs:label>
        <rdfs:comment xml:lang="en">An optional integer that determine the ordere of execution of a rule ( ascending).</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;ReasoningPolicy"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#expectedParts -->

    <owl:DatatypeProperty rdf:about="&v1;expectedParts">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">expected parts</rdfs:label>
        <rdfs:comment xml:lang="en">the number of expected resource partitions (i.e pages).</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;IngestionPlan"/>
        <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#expectedTriples -->

    <owl:DatatypeProperty rdf:about="&v1;expectedTriples">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">expected triples</rdfs:label>
        <rdfs:comment xml:lang="en">the expected maximum number of triple to be indexed by an activity.</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;IngestionPlan"/>
        <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#forceAccept -->

    <owl:DatatypeProperty rdf:about="&v1;forceAccept">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">force accept</rdfs:label>
        <rdfs:comment xml:lang="en">Define specified http accept header in requesting source. The sintax must be complain to RFC 2616 for HTTP 1.1. accept header. If not specified &apos;application/n-triples,application/ld+json,application/rdf+xml,text/turtle,*/*;q=0.4&apos; it is used.</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;KnowledgeAccrualPolicy"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#fromPage -->

    <owl:DatatypeProperty rdf:about="&v1;fromPage">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">from page</rdfs:label>
        <rdfs:comment xml:lang="en">The first page returned by a SPARQL in a query iteration. Defaults to 1 if not present.</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;SparqlServiceAccrualPolicy"/>
        <rdfs:range rdf:resource="&xsd;positiveInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#generatesGraphType -->

    <owl:DatatypeProperty rdf:about="&v1;generatesGraphType">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">generates  graph type</rdfs:label>
        <rdfs:comment xml:lang="en">The  described accrual policy has the objective to generate/maintain a knowledge graph of the related type. If the property is not specified, nor explicitely nor implicitely (by axioms), kees:LinkedDataGraph is inferred.</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;KnowledgeAccrualPolicy"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasFrequencyPeriod -->

    <owl:DatatypeProperty rdf:about="&v1;hasFrequencyPeriod">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has frequency period</rdfs:label>
        <rdfs:comment xml:lang="en">Frequency period in seconds.</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;UpdatePolicy"/>
        <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
        <rdfs:seeAlso rdf:resource="http://www.w3.org/TR/swbp-xsch-datatypes/#section-duration"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasLimit -->

    <owl:DatatypeProperty rdf:about="&v1;hasLimit">
        <rdfs:label xml:lang="en">has limit</rdfs:label>
        <rdfs:comment xml:lang="en">the max size in RDF triple of a paged resource</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasOffset -->

    <owl:DatatypeProperty rdf:about="&v1;hasOffset">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has offset</rdfs:label>
        <rdfs:comment xml:lang="en">first record ofset of a page.</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;Page"/>
        <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasResilience -->

    <owl:DatatypeProperty rdf:about="&v1;hasResilience">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has resilience</rdfs:label>
        <rdfs:comment xml:lang="en">The number of times  ABOX graph is retained in storage after a failure in fetchin it. The default is 0 that means that when a graph fails to load it is removed from the knowledge base. If the value of this attribute is, for instance 3, this means that the Graph will be keept in the knowledge base untill its loading fails for three consecutive  times (the fourth error drives a graph drop). A succesfull loading reset the fail counter.N.B. a &quot;not modified&quot; message does not reset the fail counter.</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;RetentionPolicy"/>
        <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#havingTargetGraphName -->

    <owl:DatatypeProperty rdf:about="&v1;havingTargetGraphName">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">having target graph name</rdfs:label>
        <rdfs:comment xml:lang="en">States that the described ingestion plan is about the creation/maintenance of a graph named as the referenced object.</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;IngestionPlan"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#includes -->

    <owl:DatatypeProperty rdf:about="&v1;includes">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">include</rdfs:label>
        <rdfs:comment xml:lang="en">The URL of a web resource exposing a Kees configuration data.</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;ConfigAccrualPolicy"/>
        <rdfs:subPropertyOf rdf:resource="&v1;url"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#isLastPage -->

    <owl:DatatypeProperty rdf:about="&v1;isLastPage">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">is the last page</rdfs:label>
        <rdfs:comment xml:lang="en">states that the referenced page is the last one. If not present defautlt is false.</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;Page"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#lodLaundromatUrl -->

    <owl:DatatypeProperty rdf:about="&v1;lodLaundromatUrl">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">lod laundromat url</rdfs:label>
        <rdfs:comment xml:lang="en">An url of a resource stored in the  lod laundromat wardrobe. similar to llo:url.</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;LodLaundromatAccrualPolicy"/>
        <rdfs:subPropertyOf rdf:resource="&v1;url"/>
        <rdfs:seeAlso rdf:resource="http://lodlaundromat.org/"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#pageNum -->

    <owl:DatatypeProperty rdf:about="&v1;pageNum">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">page number</rdfs:label>
        <rdfs:comment xml:lang="en">A page sequence number .</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;Page"/>
        <rdfs:range rdf:resource="&xsd;positiveInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#pageSize -->

    <owl:DatatypeProperty rdf:about="&v1;pageSize">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">page size</rdfs:label>
        <rdfs:comment xml:lang="en">The size for a SPARQL query page. Equivalent  with LIMIT clause. Defaults to 1000 if not defined</rdfs:comment>
        <rdfs:domain rdf:resource="&v1;SparqlServiceAccrualPolicy"/>
        <rdfs:range rdf:resource="&xsd;positiveInteger"/>
