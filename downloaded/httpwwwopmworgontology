</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/p-plan#isOutputVarOf"/>
        <rdfs:domain rdf:resource="http://www.opmw.org/ontology/WorkflowTemplateArtifact"/>
