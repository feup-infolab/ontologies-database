    <rdfs:domain rdf:resource="#Ambient"/>
    <rdfs:label xml:lang="en">manner</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="consequence">
    <rdfs:comment xml:lang="en">The involuntary goal of an action.
Example: The player missed [to hit the ball].</rdfs:comment>
    <rdfs:domain rdf:resource="#Ambient"/>
    <rdfs:label xml:lang="en">consequence</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#origin">
