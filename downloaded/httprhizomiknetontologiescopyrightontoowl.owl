        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="origin"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">Some copyright laws include a right to control importation of copies as a means of preventing erosion of the principle of territoriality of copyright.</rdfs:comment>
    <rdfs:label xml:lang="en">Importation Right</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="CreationConcept">
    <rdfs:comment xml:lang="en">Something that cannot exist at a paticular place and time without some physical encoding or embodiment. Also known as Mental Concept.</rdfs:comment>
    <rdfs:label xml:lang="en">Abstract</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://schema.org/CreativeWork"/>
  </owl:Class>
  <owl:Class rdf:ID="Improvise">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#theme"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#Work"/>
        </owl:allValuesFrom>
