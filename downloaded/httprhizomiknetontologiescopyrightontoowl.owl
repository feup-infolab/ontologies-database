    <rdfs:label xml:lang="en">location</rdfs:label>
    <rdfs:range rdf:resource="http://schema.org/Place"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#manner">
    <rdfs:comment xml:lang="en">The way an event develops.
