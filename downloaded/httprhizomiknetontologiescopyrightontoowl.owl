    <rdfs:label xml:lang="en">share percentage</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>
  <owl:TransitiveProperty rdf:ID="hasDerivation">
    <rdfs:subPropertyOf rdf:resource="#creationModelProperty"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:domain rdf:resource="#Work"/>
    <rdfs:label xml:lang="en">has derivation</rdfs:label>
    <owl:inverseOf>
      <owl:TransitiveProperty rdf:ID="isDerivationOf"/>
