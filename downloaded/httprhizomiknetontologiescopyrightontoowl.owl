    <rdfs:domain rdf:resource="#Instance"/>
    <rdfs:range rdf:resource="#ManifestationOrRecording"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="completion">
    <rdfs:label xml:lang="en">completion</rdfs:label>
    <rdfs:comment xml:lang="en">The goal of a temporal process.
Example: Mary waited until [noon].</rdfs:comment>
    <rdfs:domain rdf:resource="#Temporal"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="reason">
    <rdfs:label xml:lang="en">reason</rdfs:label>
    <rdfs:domain rdf:resource="#Ambient"/>
    <rdfs:comment xml:lang="en">The initial cause of an event.
Example: The airplane fell due to [a malfunction].</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#aim">
    <rdfs:comment xml:lang="en">The voluntary goal of an action.
Example: The player tries [to hit the ball].</rdfs:comment>
    <rdfs:domain rdf:resource="#Ambient"/>
    <rdfs:label xml:lang="en">aim</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasDirectPart">
    <rdfs:subPropertyOf rdf:resource="#hasPart"/>
    <owl:inverseOf rdf:resource="#isDirectPartOf"/>
    <rdfs:label xml:lang="en">has direct part</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#location">
    <rdfs:comment xml:lang="en">An spatial essential participant.
Example: Vehicles arrive at [a station].</rdfs:comment>
