    <rdfs:subPropertyOf rdf:resource="#creationModelProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#agent">
    <rdfs:comment xml:lang="en">The voluntary initiator of an event.
Example: [Eve] bit an apple.</rdfs:comment>
    <rdfs:label xml:lang="en">agent</rdfs:label>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Action"/>
          <owl:Class rdf:about="#Process"/>
          <owl:Class rdf:about="#Transfer"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#result">
    <rdfs:comment xml:lang="en">An inanimate goal of an event.
Example: Eric built a [house].</rdfs:comment>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Action"/>
          <owl:Class rdf:about="#Process"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:label xml:lang="en">result</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="destination">
    <rdfs:comment xml:lang="en">A goal of a spatial process.
Example: Bob went to [Danbury].</rdfs:comment>
    <rdfs:label xml:lang="en">destination</rdfs:label>
    <rdfs:domain rdf:resource="#Spatial"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#condition">
    <rdfs:domain rdf:resource="#Ambient"/>
    <rdfs:comment xml:lang="en">The necessary circumstance for an action.
Example: In order to see the film [you must pay the entrance].</rdfs:comment>
    <rdfs:label xml:lang="en">condition</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="instrument">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Action"/>
          <owl:Class rdf:about="#Transfer"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:label xml:lang="en">instrument</rdfs:label>
    <rdfs:comment xml:lang="en">A resource that is not changed by an event.
Example: [The key] opened the door.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#patient">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Process"/>
          <owl:Class rdf:about="#Action"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:label xml:lang="en">patient</rdfs:label>
    <rdfs:comment xml:lang="en">An essential participant that undergoes some structural change as a result of the event.
Example: The cat swallowed [the canary].</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasPart">
    <rdfs:label xml:lang="en">has part</rdfs:label>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="isPartOf"/>
    </owl:inverseOf>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="isDirectPartOf">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="hasDirectPart"/>
    </owl:inverseOf>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#isPartOf"/>
    </rdfs:subPropertyOf>
    <rdfs:label xml:lang="en">is direct part of directly</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="effector">
    <rdfs:domain rdf:resource="#Action"/>
    <rdfs:comment xml:lang="en">The involuntary initiator of an event.
Example: [The tree] produced new leaves.</rdfs:comment>
    <rdfs:label xml:lang="en">effector</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#recipient">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Action"/>
          <owl:Class rdf:about="#Process"/>
          <owl:Class rdf:about="#Transfer"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:label xml:lang="en">recipient</rdfs:label>
    <rdfs:comment xml:lang="en">An animate goal of an event.
Example: Sue sent the gift to [Bob].</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="isInstanceOf">
    <owl:inverseOf>
      <owl:InverseFunctionalProperty rdf:ID="hasInstance"/>
    </owl:inverseOf>
    <rdfs:subPropertyOf rdf:resource="#creationModelProperty"/>
