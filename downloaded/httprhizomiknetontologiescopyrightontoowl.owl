      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#recipient"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="#LegalPerson"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">A Transfer of some economic value in exchange of something, the aim.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Ambient"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#Transfer"/>
    <rdfs:subClassOf>
      <owl:Restriction>
