    <rdfs:domain rdf:resource="#Process"/>
    <rdfs:comment xml:lang="en">A resource that is changed by the event.
Example: The gun was carved out of [soap].</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#medium">
