          <owl:Class rdf:about="#Spatial"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:label xml:lang="en">origin</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="path">
    <rdfs:label xml:lang="en">path</rdfs:label>
    <rdfs:comment xml:lang="en">A spatial resource.
Example: The pizza was shipped via [Albany and Buffalo].</rdfs:comment>
    <rdfs:domain rdf:resource="#Spatial"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasImprovisation">
