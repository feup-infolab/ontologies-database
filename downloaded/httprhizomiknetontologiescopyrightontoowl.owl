    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Spatial"/>
    </rdfs:subClassOf>
