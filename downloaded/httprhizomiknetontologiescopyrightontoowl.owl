    <rdfs:label xml:lang="en">Official Act Right</rdfs:label>
    <rdfs:comment xml:lang="en">To use for certain administrative, judicial or security proceedings and religious or official ceremonies.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#BroadcastMedium">
    <rdfs:subClassOf rdf:resource="#CommunicationMedium"/>
