    <rdfs:comment xml:lang="en">The right to withdraw the work.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="MoralRights"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">Withdrawal Right</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#IPRAgreement">
