    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="http://schema.org/Action"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Copyright"/>
    </owl:disjointWith>
  </owl:Class>
