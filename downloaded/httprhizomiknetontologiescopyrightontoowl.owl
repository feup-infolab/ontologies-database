    ></rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="IPRAgreement"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Tune">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="start"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
