  <reg:name xml:lang="da">orkesterleder</reg:name>
  <rdfs:label xml:lang="da">har orkesterleder</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/e/orkesterleder.da" />
