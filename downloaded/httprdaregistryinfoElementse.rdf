  <reg:name xml:lang="da">vært</reg:name>
  <rdfs:label xml:lang="da">har vært</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/e/vært.da" />
  <skos:definition xml:lang="da">Relaterer et udtryk til en performer, der har bidraget til et udtryk af et værk ved at lede et program, ofte transmitteret, der omfatter andre gæster, optrædende etc.</skos:definition>
  <rdakit:toolkitLabel xml:lang="da">vært</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="da">Performer, der har bidraget til et udtryk af et værk ved at lede et program, ofte transmitteret, der omfatter andre gæster, optrædende etc.</rdakit:toolkitDefinition>
