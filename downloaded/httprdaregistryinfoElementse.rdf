  <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10006" />
  <rdfs:subPropertyOf rdf:resource="http://rdaregistry.info/Elements/e/P20012" />
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/e/voiceActor.en" />
  <reg:hasUnconstrained rdf:resource="http://rdaregistry.info/Elements/u/P60152" />
