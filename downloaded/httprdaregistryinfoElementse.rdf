  <skos:scopeNote xml:lang="vi">Biểu diễn của ca sỹ có thể hoặc không bao gồm các từ thực sự</skos:scopeNote>
  <rdakit:toolkitLabel xml:lang="vi">ca sỹ</rdakit:toolkitLabel>
