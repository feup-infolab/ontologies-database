<!-- Property: has host -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/e/P20016">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/e/" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
  <reg:name xml:lang="en">host</reg:name>
  <rdfs:label xml:lang="en">has host</rdfs:label>
  <skos:definition xml:lang="en">Relates an expression to a performer contributing to an expression of a work by leading a program, often broadcast, that includes other guests, performers, etc.</skos:definition>
