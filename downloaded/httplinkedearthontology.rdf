<?xml version="1.0"?>
<rdf:RDF xmlns="http://linked.earth/ontology#"
     xml:base="http://linked.earth/ontology"
     xmlns:vaem="http://www.linkedmodel.org/schema/vaem#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:vann="http://purl.org/vocab/vann/">
    <owl:Ontology rdf:about="http://linked.earth/ontology#">
        <rdfs:comment xml:lang="en">The Linked Earth Ontology aims to provide a common vocabulary for annotating paleoclimatology data</rdfs:comment>
        <vann:preferredNamespacePrefix>le</vann:preferredNamespacePrefix>
        <owl:versionInfo xml:lang="en">1.2.0</owl:versionInfo>
        <dcterms:description xml:lang="en">The Linked Earth Ontology aims to provide a common vocabulary for annotating paleoclimatology data</dcterms:description>
        <vann:preferredNamespaceUri>http://linked.earth/ontology#</vann:preferredNamespaceUri>
        <dcterms:license rdf:resource="http://creativecommons.org/licenses/by/2.0/"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/vocab/vann/example -->

    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/example"/>
    


    <!-- http://www.linkedmodel.org/schema/vaem#rationale -->

    <owl:AnnotationProperty rdf:about="http://www.linkedmodel.org/schema/vaem#rationale"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2000/01/rdf-schema#Literal -->

    <rdf:Description rdf:about="http://www.w3.org/2000/01/rdf-schema#Literal">
        <owl:equivalentClass rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://linked.earth/ontology#author -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#author">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/creator"/>
        <rdfs:subPropertyOf rdf:resource="http://schema.org/author"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://linked.earth/ontology#Compilation"/>
                    <rdf:Description rdf:about="http://linked.earth/ontology#Dataset"/>
                    <rdf:Description rdf:about="http://linked.earth/ontology#Publication"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Person"/>
        <vann:example rdf:resource="http://wiki.linked.earth/Publication.10.1002/2013PA002534"/>
        <vaem:rationale xml:lang="en">Give credit to the original author of the resource</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: Who wrote (i.e., created) the resource, such as a Publication?</rdfs:comment>
        <rdfs:label xml:lang="en">author</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#calibratedVia -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#calibratedVia">
        <rdfs:domain rdf:resource="http://linked.earth/ontology#InferredVariable"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#CalibrationModel"/>
        <vann:example rdf:resource="http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement2.sst"/>
        <vaem:rationale xml:lang="en">Provides calibration information, especially useful for legacy datasets where the raw measurements may not be available and for comparison of datasets.</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: What calibration process was used to relate the InferredVariable to the MeasuredVariable?</rdfs:comment>
        <rdfs:label xml:lang="en">calibrated via</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#calibrationReferences -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#calibrationReferences">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/references"/>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#CalibrationModel"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Publication"/>
        <vann:example rdf:resource="http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement1.mgcarub.Calibration"/>
        <vaem:rationale xml:lang="en">Relationship needed to link to a Publication desribing the calibration being applied to the dataset</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: In what Reference(s) is the Calibration described/justified?</rdfs:comment>
        <rdfs:label xml:lang="en">calibration references</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#chronModeledBy -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#chronModeledBy">
        <rdfs:subPropertyOf rdf:resource="http://linked.earth/ontology#modeledBy"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://linked.earth/ontology#ChronData"/>
                    <rdf:Description rdf:about="http://linked.earth/ontology#ChronProxySystem"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://linked.earth/ontology#ChronModel"/>
        <vaem:rationale xml:lang="en">Subproperty of modeledBy. The subdivision between chronological and paleoenvironmental information reflects the fact that Datasets are fundamentally characterized by two distinct components</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: What is the ChronModel for the ChronData or ProxySystem under consideration?</rdfs:comment>
        <rdfs:label xml:lang="en">chron modeled by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#collectedFrom -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#collectedFrom">
        <rdfs:subPropertyOf rdf:resource="http://schema.org/contentLocation"/>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Dataset"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Location"/>
        <vann:example rdf:resource="http://wiki.linked.earth/MD982181.Khider.2014"/>
        <vaem:rationale xml:lang="en">Describe the geographical coordinates for the provenance of the ProxyArchive where the dataset was collected from</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: Where (in the world) was the data in the Dataset collected? Refers to the location of the ProxyArchive</rdfs:comment>
        <rdfs:label xml:lang="en">collected from</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#contributor -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#contributor">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/contributor"/>
        <rdfs:subPropertyOf rdf:resource="http://schema.org/contributor"/>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Dataset"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Person"/>
        <vann:example rdf:resource="http://wiki.linked.earth/MD982181.Khider.2014"/>
        <vaem:rationale xml:lang="en">Allows distinction between contributor to a resource and the author</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: Who contributed the resource? A contributor may be the same as the authors of the resource or different. For instance, in the case of a legacy dataset, a Linked Earth member may contribute a previously-published dataset that he/she may not have authored</rdfs:comment>
        <rdfs:label xml:lang="en">contributor</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#distributionTableGeneratedByModel -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#distributionTableGeneratedByModel">
        <rdfs:subPropertyOf rdf:resource="http://linked.earth/ontology#generatedByModel"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#DistributionTable"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://linked.earth/ontology#ChronModel"/>
                    <rdf:Description rdf:about="http://linked.earth/ontology#PaleoModel"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <vann:example rdf:resource="http://wiki.linked.earth/ODP1098B1_2.ChronData1.Model1"/>
        <vaem:rationale xml:lang="en">&quot;Subproperty of foundinTable to Link the Model to its outputs&quot;</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: In which DistributionTable is the Model output found?</rdfs:comment>
        <rdfs:label xml:lang="en">distribution table generated by Model</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#ensembleTableGeneratedByModel -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#ensembleTableGeneratedByModel">
        <rdfs:subPropertyOf rdf:resource="http://linked.earth/ontology#generatedByModel"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#EnsembleTable"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://linked.earth/ontology#ChronModel"/>
                    <rdf:Description rdf:about="http://linked.earth/ontology#PaleoModel"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <vaem:rationale rdf:resource="http://wiki.linked.earth/ODP1098B1_2.ChronData1.Model1"/>
        <vaem:rationale xml:lang="en">&quot;Subproperty of foundinTable to link the Model to its outputs&quot;</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: In which EnsembleTable is the Model output found?</rdfs:comment>
        <rdfs:label xml:lang="en">ensemble table generated by model</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#foundInMeasurementTable -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#foundInMeasurementTable">
        <rdfs:subPropertyOf rdf:resource="http://linked.earth/ontology#foundInTable"/>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Data"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#MeasurementTable"/>
        <vann:example rdf:resource="http://wiki.linked.earth/MD982181.Khider.2014.PaleoData1"/>
        <vaem:rationale xml:lang="en">Subproperty of foundinTable to link the Data to the Variables</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: In which MeasurementTable are the Variables found?</rdfs:comment>
        <rdfs:label xml:lang="en">found in measurement table</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#foundInTable -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#foundInTable">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://linked.earth/ontology#Data"/>
                    <rdf:Description rdf:about="http://linked.earth/ontology#Variable"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://linked.earth/ontology#DataTable"/>
        <vaem:rationale xml:lang="en">Allows to link the data to the csv table and the models to its output</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: In which DataTable can the information/data be found?</rdfs:comment>
        <rdfs:label xml:lang="en">found in table</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#fundedBy -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#fundedBy">
        <rdfs:subPropertyOf rdf:resource="http://schema.org/funder"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://linked.earth/ontology#Compilation"/>
                    <rdf:Description rdf:about="http://linked.earth/ontology#Dataset"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Funding"/>
        <vann:example rdf:resource="http://wiki.linked.earth/MD982181.Khider.2014"/>
        <vaem:rationale xml:lang="en">Links a dataset to its source of funding</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: What was the Funding source for the Dataset?</rdfs:comment>
        <rdfs:label xml:lang="en">funded by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#generatedByModel -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#generatedByModel">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#wasDerivedFrom"/>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#DataTable"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Model"/>
        <vaem:rationale xml:lang="en">Needed to link the model to the data tables it generates</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: which model was used to generated this data table?</rdfs:comment>
        <rdfs:label xml:lang="en">generated by model</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#hasCode -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#hasCode">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Model"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#SoftwareCode"/>
        <vaem:rationale xml:lang="en">Links a Model to its SoftwareCode.</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: What SoftwareCode was used to generate the Model output?</rdfs:comment>
        <rdfs:label xml:lang="en">has code</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#hasIntegrationTime -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#hasIntegrationTime">
        <rdfs:domain rdf:resource="http://linked.earth/ontology#ProxySensor"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#IntegrationTime"/>
        <vaem:rationale xml:lang="en">Property added to link the proxySystem to the IntegrationTime class</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: What is the integration time needed for a ProxySensor to imprint environmental information on the ProxyArchive? Both are included as part of a ProxySystem</rdfs:comment>
        <rdfs:label xml:lang="en">has integration time</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#hasProxySystem -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#hasProxySystem">
        <rdfs:domain rdf:resource="http://linked.earth/ontology#MeasuredVariable"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#ProxySystem"/>
        <vann:example rdf:resource="http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement1.mgcarub"/>
        <vaem:rationale xml:lang="en">Links the MeasuredVariable to its ProxySystem. For instance, Mg/Ca measurements on foraminiferal shells have ProxySystem: MarineSediment.Foraminifera.Mg/Ca</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: What is the ProxySystem associated with this MeasuredVariable?</rdfs:comment>
        <rdfs:label xml:lang="en">has proxy system</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#hasResolution -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#hasResolution">
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Variable"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Resolution"/>
        <vann:example xml:lang="en">http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement1.mgcarub.ICPAES</vann:example>
        <vaem:rationale xml:lang="en">Needed to link a Variable to Resolution</vaem:rationale>
        <rdfs:comment>Answers the question: What is the Resolution of the Variable?</rdfs:comment>
        <rdfs:label xml:lang="en">has resolution</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#hasUncertainty -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#hasUncertainty">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://linked.earth/ontology#Instrument"/>
                    <rdf:Description rdf:about="http://linked.earth/ontology#Model"/>
                    <rdf:Description rdf:about="http://linked.earth/ontology#Variable"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Uncertainty"/>
        <vann:example rdf:resource="http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement1.mgcarub"/>
        <vaem:rationale xml:lang="en">Links to the Uncertainty class</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: What is the Uncertainty associated with the Variable, Model, or Instrument?</rdfs:comment>
        <rdfs:label xml:lang="en">has uncertainty</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#includesChronData -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#includesChronData">
        <rdfs:subPropertyOf rdf:resource="http://linked.earth/ontology#includesData"/>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Dataset"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#ChronData"/>
        <vann:example rdf:resource="http://wiki.linked.earth/MD982181.Khider.2014"/>
        <vaem:rationale xml:lang="en">Subproperty of includesData. The subdivision between chronological and paleoenvironmental information reflects the fact that Datasets are fundamentally characterized by two distinct components</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: Which ChronData are included in the Dataset?</rdfs:comment>
        <rdfs:label xml:lang="en">includes chron data</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#includesData -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#includesData">
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Dataset"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Data"/>
        <vaem:rationale xml:lang="en">Links Dataset to Data</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: Which Data are included in the Dataset?</rdfs:comment>
        <rdfs:label xml:lang="en">includes data</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#includesPaleoData -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#includesPaleoData">
        <rdfs:subPropertyOf rdf:resource="http://linked.earth/ontology#includesData"/>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Dataset"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#PaleoData"/>
        <vann:example rdf:resource="http://wiki.linked.earth/MD982181.Khider.2014"/>
        <vaem:rationale xml:lang="en">Subproperty of includesData. The subdivision between chronological and paleoenvironmental information reflects the fact that Datasets are fundamentally characterized by two distinct components</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: Which PaleoData are included in the Dataset?</rdfs:comment>
        <rdfs:label xml:lang="en">includes paleo data</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#includesVariable -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#includesVariable">
        <rdfs:domain rdf:resource="http://linked.earth/ontology#DataTable"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Variable"/>
        <vann:example rdf:resource="http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement2"/>
        <vaem:rationale xml:lang="en">Links DataTable to Variable</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: Which Variables are included in the DataTable?</rdfs:comment>
        <rdfs:label xml:lang="en">includes variable</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#inferredFrom -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#inferredFrom">
        <rdfs:domain rdf:resource="http://linked.earth/ontology#InferredVariable"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#MeasuredVariable"/>
        <vann:example rdf:resource="http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement2.sst"/>
        <vaem:rationale xml:lang="en">Provides a link between the Measured and Inferred Variables. For instance, SST is inferred from Mg/Ca measurements</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: From which MeasuredVariable(s) is (are) the InferredVariable inferred from?</rdfs:comment>
        <rdfs:label xml:lang="en">inferred from</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#inferredVariableType -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#inferredVariableType">
        <rdfs:domain rdf:resource="http://linked.earth/ontology#InferredVariable"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#InferredVariable"/>
        <vann:example rdf:resource="http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement2.sst"/>
        <vaem:rationale xml:lang="en">Links the InferredVariable values to their type. For instance, the value of 29.1 degC is of type SST. This is important to normalize the variations in VariableNames (e.g. SST, sst, sea-surface temp)</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: What type of InferredVariable does the InferredVariable belongs to?</rdfs:comment>
        <rdfs:label xml:lang="en">inferred variable type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#interpretationReferences -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#interpretationReferences">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/references"/>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Interpretation"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Publication"/>
        <vann:example rdf:resource="http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement1.mgcarub.Calibration"/>
        <vaem:rationale xml:lang="en">Links an interpretation to a reference citation (can be the same as the calibrationReferences for a Dataset)</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: In which Publication(s) is the Interpretation substantiated?</rdfs:comment>
        <rdfs:label xml:lang="en">interpretation references</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#interpretedAs -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#interpretedAs">
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Variable"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Interpretation"/>
        <vann:example rdf:resource="http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement2.sst"/>
        <vaem:rationale xml:lang="en">Links the Variable to its Interpretation</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: What is the Variable&apos;s Interpretation?</rdfs:comment>
        <rdfs:label xml:lang="en">interpreted as</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#measuredBy -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#measuredBy">
        <rdfs:subPropertyOf rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#observedBy"/>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#MeasuredVariable"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Instrument"/>
        <vann:example rdf:resource="http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement2.mgcarub"/>
        <vaem:rationale xml:lang="en">Links the MeasuredVariable to the Instrument responsible for its creation</vaem:rationale>
        <rdfs:comment xml:lang="en">Answers the question: Which Instrument was used to produce the MeasuredVariable?</rdfs:comment>
        <rdfs:label xml:lang="en">measured by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#measuredOn -->

