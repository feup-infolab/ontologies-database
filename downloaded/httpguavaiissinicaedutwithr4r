      rdfs:label "Code" ;
      
      owl:equivalentClass :Data ;
      
      rdfs:subClassOf :RRObject ;
      
      rdfs:isDefinedBy "http://guava.iis.sinica.edu.tw/r4r/code"^^xsd:anyURI ;
      
      :inReusingContext "http://www.w3.org/TR/vocab-dcat/#class-dataset"^^xsd:anyURI ;
      
      :Definition "Any collection of computer instructions is written in human-readable computer language, and served for programming and executing the Data of RRObject." .



###  http://guava.iis.sinica.edu.tw/r4r/Data

:Data rdf:type owl:Class ;
      
      rdfs:label "Data" ;
      
      rdfs:subClassOf :RRObject ;
      
      rdfs:isDefinedBy "http://guava.iis.sinica.edu.tw/r4r/data"^^xsd:anyURI ;
      
