###  http://guava.iis.sinica.edu.tw/r4r/RRObject

:RRObject rdf:type owl:Class ;
          
          rdfs:label "RRObject" ;
          
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :locateAt ;
                                owl:onClass rdfs:Resource ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasTime ;
                                owl:onClass <http://www.w3.org/TR/owl-time/#Temporal> ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                              ] ;
          
          owl:disjointWith :RRPolicy ;
          
          rdfs:isDefinedBy "http://guava.iis.sinica.edu.tw/r4r/rrobject"^^xsd:anyURI ;
          
          :Definition "Any resource served as a component for publication and reusing is defined as a Reusing Related Object (RRObject). It can be a knowledge unit for research purposes, or any resource that has relations similar to the relations between article, data and code." .



###  http://guava.iis.sinica.edu.tw/r4r/RRPolicy

:RRPolicy rdf:type owl:Class ;
          
          rdfs:label "RRPolicy" ;
          
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :isPackagedWith ;
                                owl:someValuesFrom :RRObject
                              ] ;
          
          rdfs:isDefinedBy "http://guava.iis.sinica.edu.tw/r4r/rrpolicy"^^xsd:anyURI ;
          
          :Definition "Any mechanism designed for relating to RRObject for reusing purposes is defined as a Reusing Related Policy (RRPolicy). RRPolicy is free to be extended for more issues such as security and trust for its subclasses concepts." .



