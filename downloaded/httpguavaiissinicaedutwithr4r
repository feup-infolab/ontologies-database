:Provenance rdf:type owl:Class ;
            
            rdfs:label "Provenance" ;
            
            rdfs:subClassOf :RRPolicy ;
            
            rdfs:isDefinedBy "http://guava.iis.sinica.edu.tw/r4r/provenance"^^xsd:anyURI ;
            
