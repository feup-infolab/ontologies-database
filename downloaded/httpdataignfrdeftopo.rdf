	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedezai/liste"/>
	<skos:prefLabel xml:lang="fr">menhir</skos:prefLabel>
	<skos:prefLabel xml:lang="en">menhir</skos:prefLabel>
	<skos:broader rdf:resource="http://data.ign.fr/id/codes/topo/typedezai/PAICultureLoisirs"/>
</rdf:Description>

