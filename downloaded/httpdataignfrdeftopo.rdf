	<cc:license rdf:resource="http://creativecommons.org/licenses/by/2.0/"/>
	<cc:license rdf:resource="http://opendatacommons.org/licenses/by/"/>
	<dcterms:creator rdf:resource="http://recherche.ign.fr/labos/cogit/cv.php?prenom=Nathalie&amp;nom=Abadie"/>
	<dcterms:contributor rdf:resource="http://www.eurecom.fr/~atemezin/"/>
	<dcterms:publisher rdf:resource="http://fr.dbpedia.org/resource/Institut_national_de_l%27information_g%C3%A9ographique_et_foresti%C3%A8re"/>
</rdf:Description>

