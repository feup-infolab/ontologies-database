	<skos:broader rdf:resource="http://data.ign.fr/id/codes/topo/typedezai/PAISport"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typedezai/Stade">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeZAI"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Grande enceinte, terrain aménagé pour la pratique des sports, et le plus souvent entouré de gradins, de tribunes : stade, terrain de sports, vélodrome découvert, circuit auto-moto, complexe sportif pluridisciplinaire. Seules les enceintes incluant des aménagements conséquents (piste 'construite', tribunes,etc.) sont incluses. Les terrains d'entraînement incluant seulement 2 ou 3 terrains de football et de petits vestiaires sont exclus (voir aussi la classe TERRAIN_SPORT).</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedezai/liste"/>
	<skos:prefLabel xml:lang="fr">stade</skos:prefLabel>
	<skos:prefLabel xml:lang="en">stadium</skos:prefLabel>
	<skos:broader rdf:resource="http://data.ign.fr/id/codes/topo/typedezai/PAISport"/>
</rdf:Description>

