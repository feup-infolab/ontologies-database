	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedezai/liste"/>
	<skos:prefLabel xml:lang="fr">poste de police</skos:prefLabel>
	<skos:prefLabel xml:lang="en">police post</skos:prefLabel>
	<skos:altLabel xml:lang="fr">hôtel de police</skos:altLabel>
	<skos:altLabel xml:lang="en">police station</skos:altLabel>
	<skos:broader rdf:resource="http://data.ign.fr/id/codes/topo/typedezai/PAIAdministratifOuMilitaire"/>
