	<skos:definition xml:lang="fr">Voie ferrée déclassée, voie ferrée fermée à tout trafic, voie ferrée neutralisée.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedevoieferree/liste"/>
	<skos:prefLabel xml:lang="fr">voie non exploitée</skos:prefLabel>
	<skos:prefLabel xml:lang="en">unused track</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typedevoieferree/VoieTransportUrbain">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeVoieFerree"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Ligne de métro aérien ou de tramway.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedevoieferree/liste"/>
	<skos:prefLabel xml:lang="fr">voie de transport urbain</skos:prefLabel>
	<skos:prefLabel xml:lang="en">urban transport railway</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typedevoieferree/Funiculaire">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeVoieFerree"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Funiculaire ou voie ferrée à crémaillère.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedevoieferree/liste"/>
	<skos:prefLabel xml:lang="fr">Funiculaire</skos:prefLabel>
	<skos:prefLabel xml:lang="en">Funicular railway</skos:prefLabel>
	<skos:altLabel xml:lang="fr">voie ferrée à crémaillère</skos:altLabel>
	<skos:altLabel xml:lang="en">rack-railway</skos:altLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typedetransportcable/CableTransporteur">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeTransportCable"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Moyen de transport constitué d'un ou de plusieurs câbles porteurs et servant à assurer le transport de matériel.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedetransportcable/liste"/>
	<skos:prefLabel xml:lang="fr">câble transporteur</skos:prefLabel>
	<skos:prefLabel xml:lang="en">cableway</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typedetransportcable/Telepherique">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeTransportCable"/>
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeZAI"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Téléphériques, y compris les remonte-pentes, remontées mécaniques, télécabines, téléskis, télébennes, télésièges.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedetransportcable/liste"/>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedezai/liste"/>
	<skos:prefLabel xml:lang="fr">téléphérique</skos:prefLabel>
	<skos:prefLabel xml:lang="en">aerial ropeway</skos:prefLabel>
	<skos:broader rdf:resource="http://data.ign.fr/id/codes/topo/typedezai/PAITransport"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typedefranchissement/Pont">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeFranchissement"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Ouvrage d’art susceptible d’enjamber un ou plusieurs éléments du réseau routier, ferré ou hydrographique ou permettant le franchissement d'une vallée : pont, passerelle, pont isolé, pont mobile. Portion du réseau située au dessus du niveau du sol.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedeconstruction/liste"/>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedezai/liste"/>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedefranchissement/liste"/>
	<skos:prefLabel xml:lang="fr">pont</skos:prefLabel>
	<skos:prefLabel xml:lang="en">bridge</skos:prefLabel>
	<skos:broader rdf:resource="http://data.ign.fr/id/codes/topo/typedezai/PAITransport"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typedefranchissement/Tunnel">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeFranchissement"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Portion souterraine du réseau.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedefranchissement/liste"/>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedezai/liste"/>
	<skos:prefLabel xml:lang="fr">tunnel</skos:prefLabel>
	<skos:prefLabel xml:lang="en">tunnel</skos:prefLabel>
	<skos:broader rdf:resource="http://data.ign.fr/id/codes/topo/typedezai/PAITransport"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typedefranchissement/Gue">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeFranchissement"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Passage naturel ou aménagé permettant aux véhicules de travaerser un cours d'eau sans le recours d'un pont ou d'un bateau.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedefranchissement/liste"/>
	<skos:prefLabel xml:lang="fr">gué</skos:prefLabel>
	<skos:prefLabel xml:lang="en">ford</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typedefranchissement/Cascade">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeZAI"/>
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeFranchissement"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Chute d’eau, cascade.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedefranchissement/liste"/>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedezai/liste"/>
	<skos:prefLabel xml:lang="fr">cascade</skos:prefLabel>
	<skos:prefLabel xml:lang="en">waterfall</skos:prefLabel>
	<skos:broader rdf:resource="http://data.ign.fr/id/codes/topo/typedezai/PAIHydrographie"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typedefranchissement/Ecluse">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeFranchissement"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Ouvrage hydraulique formé essentiellement de portes munies de vannes destiné à retenir ou à lâcher l’eau selon les besoins : Ascenseur à bateaux, écluse.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedefranchissement/liste"/>
	<skos:prefLabel xml:lang="fr">écluse</skos:prefLabel>
	<skos:prefLabel xml:lang="en">lock</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typedefranchissement/PontCanal">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeFranchissement"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Tronçon de cours d’eau artificiel passant sur un pont.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedefranchissement/liste"/>
	<skos:prefLabel xml:lang="fr">pont-canal</skos:prefLabel>
	<skos:prefLabel xml:lang="en">canal aqueduct</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typederoute/Autoroute">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeRoute"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Routes sans croisement, accessibles seulement en des points aménagés à cet effet et réservées aux véhicules à propulsion mécanique. Le classement dans la catégorie des autoroutes est prononcé par décret du conseil d’état, pris après enquête publique.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typederoute/liste"/>
	<skos:prefLabel xml:lang="fr">autoroute</skos:prefLabel>
	<skos:prefLabel xml:lang="en">motorway</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typederoute/QuasiAutoroute">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeRoute"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Routes de même définition que l’autoroute mais non classées officiellement dans cette catégorie. Ce sont des routes à chaussées séparées par un terre-plein central, qui ne possèdent pas de croisement à niveau avec le reste du réseau routier.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typederoute/liste"/>
	<skos:prefLabel xml:lang="fr">quasi-autoroute</skos:prefLabel>
	<skos:prefLabel xml:lang="en">non-classified motorway</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typederoute/Bretelle">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeRoute"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Bretelles de liaison (ou d’échangeur) ou voies d’accès à une aire de service ou de repos.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typederoute/liste"/>
	<skos:prefLabel xml:lang="fr">bretelle</skos:prefLabel>
	<skos:prefLabel xml:lang="en">ramp</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typederoute/RouteADeuxChaussees">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeRoute"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Routes comportant 2 chaussées séparées par un obstacle physique éventuellement ouvert aux carrefours. Elles possèdent donc des croisements à niveau, ce qui leur interdit d'être classées dans la catégorie autoroute ou quasi-autoroute.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typederoute/liste"/>
	<skos:prefLabel xml:lang="fr">route à deux chaussées</skos:prefLabel>
	<skos:prefLabel xml:lang="en">dual carriageway road</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typederoute/RouteAUneChaussee">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeRoute"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Routes comportant 1 chaussée. Toutes les routes goudronnées qui ne sont pas classées en Route à 2 chaussées, Quasi-autoroute ou Autoroute se retrouvent dans cette classe.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typederoute/liste"/>
	<skos:prefLabel xml:lang="fr">route à une chaussée</skos:prefLabel>
	<skos:prefLabel xml:lang="en">single carriageway road</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typederoute/RouteEmpierree">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeRoute"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Routes sommairement revêtues (pas de revêtement de surface ou revêtement très dégradé), mais permettant la circulation de véhicules automobiles de tourisme par tout temps. Toutes les routes empierrées sont incluses.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typederoute/liste"/>
	<skos:prefLabel xml:lang="fr">route empierrée</skos:prefLabel>
	<skos:prefLabel xml:lang="en">gravel road</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typederoute/Chemin">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeRoute"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Les chemins sont prévus pour la circulation de véhicules ou d’engins d’exploitation. Ils ne sont pas forcément carrossables pour tous les véhicules et par tout temps.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typederoute/liste"/>
	<skos:prefLabel xml:lang="fr">chemin</skos:prefLabel>
	<skos:prefLabel xml:lang="en">dirt road</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typederoute/BacAuto">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeRoute"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Trajets du bateau servant à passer des véhicules d’une rive à l’autre d’un cours d’eau ou d’un bras de mer.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typederoute/liste"/>
	<skos:prefLabel xml:lang="fr">bac auto</skos:prefLabel>
	<skos:prefLabel xml:lang="en">car ferry</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typederoute/BacPieton">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeRoute"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Trajets du bateau servant à passer des piétons d’une rive à l’autre d’un cours d’eau ou d’un bras de mer.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typederoute/liste"/>
	<skos:prefLabel xml:lang="fr">bac piéton</skos:prefLabel>
	<skos:prefLabel xml:lang="en">passenger ferry</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typederoute/PisteCyclable">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeRoute"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Tronçons de chaussée en site propre, réservés aux cycles. Les bandes cyclables sont exclues.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typederoute/liste"/>
	<skos:prefLabel xml:lang="fr">piste cyclable</skos:prefLabel>
	<skos:prefLabel xml:lang="en">bicycle path</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typederoute/Sentier">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeRoute"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Chemins étroits ne permettant pas le passage de véhicules. Seuls les principaux sentiers sont inclus. Passerelles supportant une allée, directement reliés au réseau routier. Les passerelles ont une position par rapport au sol supérieure à 0.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typederoute/liste"/>
	<skos:prefLabel xml:lang="fr">sentier</skos:prefLabel>
	<skos:prefLabel xml:lang="en">footpath</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typederoute/Escalier">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeRoute"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Escaliers directement reliés au réseau routier, assurant la jonction entre deux voies de communication ou entre le réseau routier et un élément adressable.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typederoute/liste"/>
	<skos:prefLabel xml:lang="fr">escalier</skos:prefLabel>
	<skos:prefLabel xml:lang="en">stair way street</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typederoute/RouteNationale">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeRoute"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Voie du domaine public routier national autre que les autoroutes.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typederoute/liste"/>
	<skos:prefLabel xml:lang="fr">route nationale</skos:prefLabel>
	<skos:prefLabel xml:lang="en">main road</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typederoute/RouteDepartementale">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeRoute"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Voie qui fait partie du domaine public routier départemental.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typederoute/liste"/>
	<skos:prefLabel xml:lang="fr">route départementale</skos:prefLabel>
	<skos:prefLabel xml:lang="en">secondary road</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typedepointdeau/Citerne">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDePointDEau"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Réservoir où l'on recueille et conserve les eaux de pluie.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedepointdeau/liste"/>
	<skos:prefLabel xml:lang="fr">citerne</skos:prefLabel>
	<skos:prefLabel xml:lang="en">cistern</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typedepointdeau/Fontaine">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDePointDEau"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Construction comportant une alimentation en eau et généralement un bassin.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedepointdeau/liste"/>
	<skos:prefLabel xml:lang="fr">fontaine</skos:prefLabel>
	<skos:prefLabel xml:lang="en">fountain</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typedepointdeau/Source">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDePointDEau"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Point d'émergence à la surface du sol de l'eau emmagasinée à l'intérieur. Exsurgence, résurgence, source.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedepointdeau/liste"/>
	<skos:prefLabel xml:lang="fr">source</skos:prefLabel>
	<skos:prefLabel xml:lang="en">spring</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typedepointdeau/SourceCaptee">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDePointDEau"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Installation de captage d’une source.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedepointdeau/liste"/>
	<skos:prefLabel xml:lang="fr">source captée</skos:prefLabel>
	<skos:prefLabel xml:lang="en">water catchment</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/id/codes/topo/typedepointdeau/StationDePompage">
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDePointDEau"/>
	<rdf:type rdf:resource="http://data.ign.fr/def/topo#TypeDeZAI"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Installation comprenant une pompe destinée à pomper l'eau du sous-sol, d'une rivière, ou à monter de l'eau dans un réservoir ou un château d'eau.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedepointdeau/liste"/>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedezai/liste"/>
