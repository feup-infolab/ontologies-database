	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Terrain ouvert au public et consacré à la pratique du golf. Les terrains de moins de 9 trous et les minigolfs sont exclus.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedezai/liste"/>
	<skos:prefLabel xml:lang="fr">golf</skos:prefLabel>
