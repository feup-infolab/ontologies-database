	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="fr">Construction nommée habitée ou associée à un groupe d'habitations : construction diverse, pigeonnier, moulin à vent.</skos:definition>
	<skos:inScheme rdf:resource="http://data.ign.fr/id/codes/topo/typedezai/liste"/>
	<skos:prefLabel xml:lang="fr">construction nommée</skos:prefLabel>
	<skos:prefLabel xml:lang="en">named building</skos:prefLabel>
	<skos:broader rdf:resource="http://data.ign.fr/id/codes/topo/typedezai/PAICultureLoisirs"/>
