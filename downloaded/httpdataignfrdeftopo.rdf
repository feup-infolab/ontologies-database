	<skos:prefLabel xml:lang="en">toll-gate</skos:prefLabel>
	<skos:relatedMatch rdf:resource="http://data.ign.fr/id/codes/topo/typedebatiment/Peage"/>
	<skos:broader rdf:resource="http://data.ign.fr/id/codes/topo/typedezai/PAITransport"/>
