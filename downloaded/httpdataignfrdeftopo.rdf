<rdf:Description rdf:about="http://data.ign.fr/def/topo#EntiteTopographique">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Un phénomène du monde réel qui est associé à une localisation sur la terre</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Entité topographique</rdfs:label>
	<rdfs:label xml:lang="en">Topographic entity</rdfs:label>
	<owl:equivalentClass rdf:resource="http://www.geonames.org/ontology#Feature"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#ZoneActiviteInteret">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Bâtiment ou site ayant une vocation ou un caractère particuliers (administratif, religieux, sportif, industriel, etc.).</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Zone d'activité ou d'intérêt</rdfs:label>
	<rdfs:label xml:lang="en">Working area or area of interest</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#EntiteTopographique"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#Bati">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Bâtiments (indifférenciés, industriels et remarquables) et autres constructions.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Bâti</rdfs:label>
	<rdfs:label xml:lang="en">Buildings and structures</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#EntiteTopographique"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#Batiment">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Construction d'une certaine importance destinée à abriter des personnes, des choses ou des animaux.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Bâtiment</rdfs:label>
	<rdfs:label xml:lang="en">Building</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#Bati"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#Construction">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Ouvrage de génie civil.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Construction</rdfs:label>
	<rdfs:label xml:lang="en">Structure</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#Bati"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#Reservoir">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Réservoir (eau, matières industrielles,...) de plus de 10m de diamètre.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Réservoir</rdfs:label>
	<rdfs:label xml:lang="en">Tank</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#Bati"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#Cimetiere">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Lieu où l’on enterre les morts.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Cimetière</rdfs:label>
	<rdfs:label xml:lang="en">Cemetery</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#Bati"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#TerrainDeSport">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Équipement sportif de plein air.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Terrain de sport</rdfs:label>
	<rdfs:label xml:lang="en">Sports ground</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#Bati"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#PisteAerodrome">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Aire située sur un aérodrome, aménagée afin de servir au roulement des aéronefs, au décollage et à l’atterrissage, en dur ou en herbe.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Piste d'aérodrome</rdfs:label>
	<rdfs:label xml:lang="en">Taxiway</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#Bati"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#ZoneDeVegetation">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Espace végétal naturel ou non différencié selon le couvert forestier.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Zone de végétation</rdfs:label>
	<rdfs:label xml:lang="en">Vegetation area</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#EntiteTopographique"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#ElementDuRelief">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Entité topographique correspondant à une rupture de pente artificielle, ou à un élément remarquable du relief nommé.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Élément du relief</rdfs:label>
	<rdfs:label xml:lang="en">Relief feature</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#EntiteTopographique"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#InfrastructureTransportEnergie">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Éléments du réseau de transport d'électricité à une haute et très haute tension, et conduites utilisées pour le transport de matière première.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Infrastructure de transport d'énergie</rdfs:label>
	<rdfs:label xml:lang="en">Energy transport infrastructure</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#EntiteTopographique"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#Conduite">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Conduite (autre que canalisation d’eau) ou tapis roulant utilisés pour le transport de matière première (gaz, hydrocarbure, minerai, etc.) ou canalisation de nature inconnue.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Conduite</rdfs:label>
	<rdfs:label xml:lang="en">Pipeline</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#InfrastructureTransportEnergie"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#LigneElectrique">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Portion de ligne électrique homogène pour l'ensemble des attributs qui la concernent.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Ligne électrique</rdfs:label>
	<rdfs:label xml:lang="en">Electricity transmission line</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#InfrastructureTransportEnergie"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#PosteTransformation">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Enceinte à l'intérieur de laquelle le courant transporté par une ligne électrique est transformé.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Poste de transformation</rdfs:label>
	<rdfs:label xml:lang="en">Transformation sub-station</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#InfrastructureTransportEnergie"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#Pylone">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Support de ligne électrique. Pylône, portique.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Pylône</rdfs:label>
	<rdfs:label xml:lang="en">Pylon</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#InfrastructureTransportEnergie"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#ElementReseauFerroviaire">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Éléments du réseau de transport ferroviaire: voies ferrées et aires de triage.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Élément du réseau ferroviaire</rdfs:label>
	<rdfs:label xml:lang="en">Transport by rail feature</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#EntiteTopographique"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#AireDeTriage">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Surface qui englobe l’ensemble des tronçons de voies, voies de garage, aiguillages permettant le tri des wagons et la composition des trains. Aire de triage, faisceau de voies.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Aire de triage</rdfs:label>
	<rdfs:label xml:lang="en">Marshalling yard</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#ElementReseauFerroviaire"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#VoieFerree">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Portion de voie ferrée homogène pour l'ensemble des attributs qui la concernent. Dans le cas d’une ligne composée de deux à quatre voies parallèles, l’ensemble des voies est modélisé par un seul objet.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Voie ferrée</rdfs:label>
	<rdfs:label xml:lang="en">Railway track</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#ElementReseauFerroviaire"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#LigneTransportCable">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Moyen de transport constitué d'un ou de plusieurs câbles porteurs.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Ligne de transport par câble</rdfs:label>
	<rdfs:label xml:lang="en">Cableway transport line</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#EntiteTopographique"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#ElementReseauRoutier">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Éléments du réseau de transport routier: voies de communication revêtues et non revêtues, réseau 'adressé', et une sélection des chemins et sentiers.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Élément du réseau routier</rdfs:label>
	<rdfs:label xml:lang="en">Road transport feature</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#EntiteTopographique"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#Route">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Portion de voie de communication destinée aux automobiles, aux piétons, aux cycles ou aux animaux, homogène pour l'ensemble des attributs et des relations qui la concerne. Le tronçon de route peut être revêtu ou non revêtu (pas de revêtement de surface ou revêtement de surface fortement dégradé).</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Route</rdfs:label>
	<rdfs:label xml:lang="en">Road</rdfs:label>
</rdf:Description>

<rdf:Description rdf:nodeID="node19l4a76cpx1">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
	<owl:someValuesFrom rdf:resource="http://data.ign.fr/def/topo#Voie"/>
	<owl:onProperty rdf:resource="http://purl.org/dc/terms/isPartOf"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#Route">
	<rdfs:subClassOf rdf:nodeID="node19l4a76cpx1"/>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#ElementReseauRoutier"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#Voie">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Une voie est un ensemble de tronçons de route associés à un même nom. Une voie est identifiée par son nom dans une commune donnée. Le nom de voie est celui qui sert à l’adressage.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Voie nommée</rdfs:label>
	<rdfs:label xml:lang="en">Named street</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#ElementReseauRoutier"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#BornePostale">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Borne postale située à l'extrémité d'un tronçon de route.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Borne postale</rdfs:label>
	<rdfs:label xml:lang="en">Civic number marker</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#ElementReseauRoutier"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#Parking">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Zone aménagée pour le parcage des automobiles. Parking non couvert, public ou privé, aire de stationnement d’environ un demi hectare et plus.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Parking</rdfs:label>
	<rdfs:label xml:lang="en">Car park</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#ElementReseauRoutier"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#AireDePeage">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Emprise de la chaussée d'une aire de péage.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Aire de péage</rdfs:label>
	<rdfs:label xml:lang="en">Toll-gate area</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#ElementReseauRoutier"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#Place">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<rdfs:comment xml:lang="fr">Place ou carrefour revêtu de grande largeur.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://data.ign.fr/def/topo"/>
	<rdfs:label xml:lang="fr">Place</rdfs:label>
	<rdfs:label xml:lang="en">Square</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/topo#ElementReseauRoutier"/>
</rdf:Description>

<rdf:Description rdf:about="http://data.ign.fr/def/topo#EntiteHydrographiqueTerrestre">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
