  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#EventTrigger">
    <rdfs:comment xml:lang="en">Event triggers are used to invoke the transition between game sections and activate the game events within a game scenario</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Event Trigger</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#Force">
    <rdfs:comment xml:lang="en">A motion is subject to a force</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Force</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GameAct">
    <rdfs:comment xml:lang="en">The performance of a game object in a game event. It consists of a game object and a game acting script that describe how the game object should move, animate, sound and interact with other game object in a game scenario.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Game Act</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GameActingScript">
    <rdfs:comment xml:lang="en">Is composed by one or more acting coordination that instruct how a specific game object act in a defined sequence.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Game Acting Script</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GameAttribute">
    <rdfs:comment xml:lang="en">Data properties that represent a game objects vitality, physical being, cognitive state and ownership.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Game Attribute</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GameContext">
    <rdfs:comment xml:lang="en">Describes the type of game content presented to game players in a game presentation or game simulation.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Game Context</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GameControl">
    <rdfs:comment xml:lang="en">Provides game player the channel to control game objects</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Game Control</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GameControlInterface">
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Game Control Interface</rdfs:label>
    <rdfs:comment xml:lang="en">Game Control Interface</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <owl:disjointWith rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#InputEvent"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GameDimension">
    <rdfs:comment xml:lang="en">Represents the virtual space in which the game simulation takes place in 2D or 3D.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Game Dimension</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GameEnvironment">
    <rdfs:comment xml:lang="en">Represents the composition of the virtual world by populating the virtual space with game objects.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Game Environment</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GameEvent">
    <rdfs:comment xml:lang="en">Is the happening associated to a game scenario. A game event is composed of a set of game act and an event trigger which initiate the game event.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Game Event</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GameInteractionRule">
    <rdfs:comment xml:lang="en">It dictates the outcome of the interaction from two game objects.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Game Interaction Rule</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#GameRule"/>
    <owl:disjointWith rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#GameScoringRule"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GameInteractionRuleOutcome">
    <rdfs:comment xml:lang="en">Outcome of an Interaction Rule</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Interaction Rule Outcome</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GameKnowledgeBase">
    <rdfs:comment xml:lang="en">A Knowledge Base to be used by the game.</rdfs:comment>
    <rdfs:label xml:lang="en">Game Knowledge Base</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://rdfs.org/ns/void#Dataset">
      </owl:Class>
    </rdfs:subClassOf>

  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GameObject">
    <rdfs:comment xml:lang="en">Represents virtual things that populate the game world and can be designed to have a combination of abilities such as decision making, moving, acting and responding to surroundings and game players input simulating their existence in the game world.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Game Object</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GameObjective">
    <rdfs:comment xml:lang="en">Represents the goal associated to a game scenario</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Game Objective</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GamePhysics">
    <rdfs:comment xml:lang="en">Defines the physicality of the game world. It encompasses the collision world and environment forces.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Game Physics</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GamePlayer">
    <rdfs:comment xml:lang="en">The user of the game application who provides inputs to the game system as part of the gaming activity.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Game Player</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GamePresentation">
    <rdfs:comment xml:lang="en">A virtual canvas that holds media components and GUI components to form a game menu, a game notification or a cut-scene to present information about the game and allow them to navigate through the game structure</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Game Presentation</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#GameContext"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GameRecord">
    <rdfs:comment xml:lang="en">A game record is a log of game players achievements in the game</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Game Record</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GameResult">
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Game Result</rdfs:label>
    <rdfs:comment xml:lang="en">Game Result</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GameRule">
    <rdfs:comment xml:lang="en">States the relationship between game objects and game world, and the effect of an interaction. It can either be a Game Scoring Rule or Game Interaction Rule.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Game Rule</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GameScenario">
    <rdfs:comment xml:lang="en">A description of a situation which require game player to overcome a number of challenges in order to achieve the defined objectives. It is also commonly termed as game level in gaming jargon.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Game Scenario</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GameScoringRule">
    <rdfs:comment xml:lang="en">Only applies to game player and it defines what to be awarded to the game player when a scoring condition is met.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Game Scoring Rule</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#GameRule"/>
    <ns1:term_status>stable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#GameSimulation">
    <rdfs:comment xml:lang="en">Represents the simulation of a game scenario and is governed by a set of rules that define the interactivity, physical and temporal properties of the virtual world. It has game rules, game dimension, game tempo and game physics.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Game Simulation</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#GameContext"/>
