  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#DifficultyIndicator">
    <rdfs:comment xml:lang="en">A value associated to the difficulty of the game scenario</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
    <rdfs:label xml:lang="en">Difficulty Indicator</rdfs:label>
