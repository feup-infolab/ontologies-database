  <owl:Class rdf:about="http://ns.inria.fr/ludo/v1/gamemodel#ArtRequirement">
    <rdfs:comment xml:lang="en">Art requirements related the game through expressive written text</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://ns.inria.fr/ludo/v1/gamemodel#"/>
