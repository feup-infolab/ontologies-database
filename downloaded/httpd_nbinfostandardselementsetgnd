		<dc:title xml:lang="en">GND Ontology</dc:title>
		<dc:title xml:lang="de">GND-Ontologie</dc:title>
		<!-- For Linked Open Vocabularies -->
		<vann:preferredNamespaceUri>http://d-nb.info/standards/elementset/gnd#</vann:preferredNamespaceUri>
		<vann:preferredNamespacePrefix>gndo</vann:preferredNamespacePrefix>
		
		<!-- Versioning information -->
		<dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2012-06-30</dct:issued>
