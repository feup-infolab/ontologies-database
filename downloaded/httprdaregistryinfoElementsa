<!-- Property: is transcriber of -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/a/P50142">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/a/" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
  <reg:name xml:lang="en">transcriberOf</reg:name>
  <rdfs:label xml:lang="en">is transcriber of</rdfs:label>
  <skos:definition xml:lang="en">Relates an agent contributing to an expression of a work by writing down or notating previously unwritten or unnotated content, or by changing it from one system of notation to another to an expression.</skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10002" />
  <rdfs:subPropertyOf rdf:resource="http://rdaregistry.info/Elements/a/P50161" />
