<!-- Property: is actor of -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/a/P50071">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/a/" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
  <reg:name xml:lang="en">actorOf</reg:name>
  <rdfs:label xml:lang="en">is actor of</rdfs:label>
  <skos:definition xml:lang="en">Relates a performer contributing to an expression of a work by acting as a cast member or player in a musical or dramatic presentation, etc. to an expression.</skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10002" />
  <rdfs:subPropertyOf rdf:resource="http://rdaregistry.info/Elements/a/P50147" />
  <reg:hasSubproperty rdf:resource="http://rdaregistry.info/Elements/a/P50217" />
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/a/actorOf.en" />
  <reg:hasUnconstrained rdf:resource="http://rdaregistry.info/Elements/u/P60017" />
  <rdfs:label xml:lang="fr">est un acteur de</rdfs:label>
  <skos:definition xml:lang="fr">Met un interprète qui contribue à une expression d’une œuvre en agissant à titre de membre d’une distribution artistique ou de comédien dans une représentation musicale ou dramatique, etc. en relation avec une expression.</skos:definition>
  <rdakit:toolkitLabel xml:lang="fr">acteur de</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="fr">Expression d’une œuvre qui intègre une prestation à titre de membre d’une distribution artistique ou de comédien dans une représentation musicale ou dramatique, etc.</rdakit:toolkitDefinition>
  <rdakit:toolkitLabel xml:lang="en">actor of</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="en">An expression of a work that incorporates a performance of acting as a cast member or player in a musical or dramatic presentation, etc.</rdakit:toolkitDefinition>
  <rdakit:instructionNumber xml:lang="">18.5</rdakit:instructionNumber>
  <reg:hasSubproperty rdf:resource="http://rdaregistry.info/Elements/a/object/P50071" />
  <reg:hasSubproperty rdf:resource="http://rdaregistry.info/Elements/a/datatype/P50071" />
  <reg:name xml:lang="el">ηθοποιόςΤου</reg:name>
  <rdfs:label xml:lang="el">είναι ηθοποιός του</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/a/ηθοποιόςΤου.el" />
  <skos:definition xml:lang="el">Συνδέει έναν εκτελεστή, ο οποίος συνεισφέρει στην έκφραση ενός έργου ενεργώντας ως ηθοποιός ή εκτελεστής σε μια μουσική ή δραματική παράσταση κ.λπ., με μια έκφραση.</skos:definition>
  <rdakit:toolkitLabel xml:lang="el">ηθοποιός του</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="el">Μια έκφραση ενός έργου που ενσωματώνει τη συνεισφορά της υποκριτικής ερμηνείας ως μέλος του επιτελείου των ηθοποιών ή των εκτελεστών, σε μια μουσική ή δραματική παρουσίαση κ.λπ.</rdakit:toolkitDefinition>
  <skos:definition xml:lang="de">Setzt einen Ausführenden, der zu einer Expression eines Werks als Ensemble-Mitglied oder Darsteller in einer musikalischen oder dramatischen Darbietung usw. beiträgt, mit einer Expression in Beziehung.</skos:definition>
  <rdakit:toolkitLabel xml:lang="de">Schauspieler von</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="de">Eine Expression eines Werks, zu der jemand als Ensemble-Mitglied oder Darsteller in einer musikalischen oder dramatischen Darbietung usw. beiträgt.</rdakit:toolkitDefinition>
  <reg:name xml:lang="da">skuespillerI</reg:name>
  <rdfs:label xml:lang="da">er skuespiller i</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/a/skuespillerI.da" />
  <skos:definition xml:lang="da">Relaterer en udøver, der bidrager til et udtryk af et værk ved at være skuespiller eller musiker i en musikalsk eller dramatisk præsentation etc. til et udtryk.</skos:definition>
  <rdakit:toolkitLabel xml:lang="da">skuespiller i</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="da">Udtryk af et værk der inkorporerer en optræden ved at fungere som skuespiller eller musiker i en musikalsk eller dramatisk præsentation etc.</rdakit:toolkitDefinition>
  <rdfs:label xml:lang="ca">és actor de</rdfs:label>
  <skos:definition xml:lang="ca">Relaciona un intèrpret, que contribueix a una expressió d’una obra musical dirigint un grup d’intèrprets en una representació musical o dramàtica, etc., amb una expressió.</skos:definition>
  <rdakit:toolkitLabel xml:lang="ca">actor de</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="ca">Expressió d’una obra que incorpora una interpretació que actua com un membre del repartiment o com un comediant d’una representació musical o dramàtica, etc.</rdakit:toolkitDefinition>
  <reg:name xml:lang="fi">esiintyjanaEsityksessa</reg:name>
  <rdfs:label xml:lang="fi">esiintyjänä esityksessä</rdfs:label>
