<!-- Property: is compiler of -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/a/P50189">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/a/" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
  <reg:name xml:lang="en">compilerOf</reg:name>
  <rdfs:label xml:lang="en">is compiler of</rdfs:label>
  <skos:definition xml:lang="en">Relates an agent responsible for creating a new work such as a bibliography or a directory by selecting, arranging, aggregating, and editing data, information, etc., to a work.</skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10002" />
  <rdfs:subPropertyOf rdf:resource="http://rdaregistry.info/Elements/a/P50204" />
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/a/compilerOf.en" />
  <reg:hasUnconstrained rdf:resource="http://rdaregistry.info/Elements/u/P60657" />
  <rdfs:label xml:lang="fr">est un compilateur de</rdfs:label>
  <skos:definition xml:lang="fr">Met un agent responsable de la création d’une nouvelle œuvre telle qu’une bibliographie ou un répertoire par la sélection, l’organisation, l’agrégation et l’édition de données, d’information, etc. en relation avec une œuvre.</skos:definition>
  <rdakit:toolkitLabel xml:lang="fr">compilateur de</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="fr">Œuvre qui comporte une responsabilité de création consistant à réaliser une nouvelle œuvre telle qu’une bibliographie ou un répertoire en sélectionnant, en organisant, en agrégeant et en éditant des données, de l’information, etc.</rdakit:toolkitDefinition>
  <rdakit:toolkitLabel xml:lang="en">compiler of</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="en">A work that involves a creation responsibility for a new work such as a bibliography or a directory by selecting, arranging, aggregating, and editing data, information, etc.</rdakit:toolkitDefinition>
  <rdakit:seeAlso rdf:resource="http://rdaregistry.info/Elements/a/P50156" />
