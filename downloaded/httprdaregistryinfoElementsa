  <rdakit:instructionNumber xml:lang="">18.5</rdakit:instructionNumber>
  <reg:hasSubproperty rdf:resource="http://rdaregistry.info/Elements/a/object/P50137" />
  <reg:hasSubproperty rdf:resource="http://rdaregistry.info/Elements/a/datatype/P50137" />
  <reg:name xml:lang="el">διασκευαστήςΤηςΜουσικήςΤου</reg:name>
