         </owl:Restriction>
      </rdfs:subClassOf>
      <terms:part rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">EEG</terms:part>
      <DCMIType:StillImage rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Record_(SOSA-SSN).png</DCMIType:StillImage>
      <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2016-08-05T04:09:00</dct:modified>
      <dct:title xml:lang="en">EegRecord</dct:title>
      <rdfs:isDefinedBy rdf:resource="https://w3id.org/BCI-ontology#"/>
      <skos:changeNote xml:lang="en">Status: *STABLE*</skos:changeNote>
