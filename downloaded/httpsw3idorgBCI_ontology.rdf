   <!-- https://w3id.org/BCI-ontology#AccessMethod -->
   <owl:Class rdf:about="https://w3id.org/BCI-ontology#AccessMethod">
      <rdfs:subClassOf>
         <owl:Restriction>
            <owl:onProperty rdf:resource="https://w3id.org/BCI-ontology#hasLocator"/>
            <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
