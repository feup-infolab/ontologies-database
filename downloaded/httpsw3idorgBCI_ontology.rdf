   <!-- https://w3id.org/BCI-ontology#EegRecord -->
   <owl:Class rdf:about="https://w3id.org/BCI-ontology#EegRecord">
      <rdfs:subClassOf rdf:resource="https://w3id.org/BCI-ontology#Record"/>
      <rdfs:subClassOf>
         <owl:Restriction>
            <owl:onProperty rdf:resource="https://w3id.org/BCI-ontology#observedByEegDevice"/>
            <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
