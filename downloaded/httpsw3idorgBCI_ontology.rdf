   <!-- https://w3id.org/BCI-ontology#hasMQTT.Topic -->
   <owl:DatatypeProperty rdf:about="https://w3id.org/BCI-ontology#hasMQTT.Topic">
      <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
      <rdfs:domain rdf:resource="https://w3id.org/BCI-ontology#AccessMethod.MQTT"/>
      <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
