  <reg:hasUnconstrained rdf:resource="http://rdaregistry.info/Elements/u/P60220" />
  <rdakit:toolkitLabel xml:lang="en">prequel</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="en">A work that extends the narrative of an earlier work backwards in time.</rdakit:toolkitDefinition>
  <rdakit:toolkitLabel xml:lang="fr">antépisode</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="fr">Œuvre qui étend le récit d’une œuvre antérieure sur une période plus ancienne dans le temps.</rdakit:toolkitDefinition>
  <rdakit:toolkitLabel xml:lang="es">precuela</rdakit:toolkitLabel>
