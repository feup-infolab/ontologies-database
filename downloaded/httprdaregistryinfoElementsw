<!-- Property: is prequel -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/w/P10122">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/w/" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
  <reg:name xml:lang="en">prequel</reg:name>
  <rdfs:label xml:lang="en">is prequel</rdfs:label>
  <skos:definition xml:lang="en">Relates a work to a work that extends the narrative of an earlier work backwards in time.</skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10001" />
  <rdfs:subPropertyOf rdf:resource="http://rdaregistry.info/Elements/w/P10156" />
