<!-- Properties -->

<!-- Property: is abridged as (work) -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/w/P10167">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/w/" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
  <reg:name xml:lang="en">abridgedAsWork</reg:name>
  <rdfs:label xml:lang="en">is abridged as (work)</rdfs:label>
  <skos:definition xml:lang="en">Relates a work to a work that shortens a source work without changing the general meaning or manner of presentation.</skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10001" />
  <rdfs:subPropertyOf rdf:resource="http://rdaregistry.info/Elements/w/P10148" />
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/w/abridgedAsWork.en" />
  <reg:hasUnconstrained rdf:resource="http://rdaregistry.info/Elements/u/P60275" />
  <rdakit:toolkitLabel xml:lang="en">abridged as (work)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="en">A work that shortens a source work without changing the general meaning or manner of presentation.</rdakit:toolkitDefinition>
  <rdakit:toolkitLabel xml:lang="fr">abrégé comme (œuvre)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="fr">Œuvre qui raccourcit une œuvre source sans en changer le sens général ou le mode de présentation.</rdakit:toolkitDefinition>
  <rdakit:toolkitLabel xml:lang="es">abreviada como (obra)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="es">Una obra que acorta el contenido de la obra fuente sin cambiar el significado general o la manera de presentación.</rdakit:toolkitDefinition>
  <rdakit:instructionNumber xml:lang="">24.5</rdakit:instructionNumber>
  <reg:hasSubproperty rdf:resource="http://rdaregistry.info/Elements/w/object/P10167" />
  <reg:name xml:lang="da">forkortetSomVærk</reg:name>
  <rdfs:label xml:lang="da">er forkortet som (værk)</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/w/forkortetSomVærk.da" />
  <skos:definition xml:lang="da">Relaterer et værk til et værk, der forkorter et oprindeligt værk uden at ændre den overordnede betydning eller præsentationsmåde.</skos:definition>
  <rdakit:toolkitLabel xml:lang="da">forkortet som (værk)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="da">Værk, der forkorter et oprindeligt værk uden at ændre den overordnede betydning eller præsentationsmåde.</rdakit:toolkitDefinition>
  <skos:definition xml:lang="fr">Met une œuvre en relation avec une œuvre qui raccourcit une œuvre source sans en changer le sens général ou le mode de présentation.</skos:definition>
  <reg:name xml:lang="vi">đượcRútGọnNhưLàTácPhẩm</reg:name>
  <rdfs:label xml:lang="vi">được rút gọn như là (tác phẩm)</rdfs:label>
  <skos:definition xml:lang="vi">Liên kết tác phẩm với tác phẩm làm ngắn tác phẩm nguồn mà không thay đổi ý nghĩa hoặc phương cách trình bày.</skos:definition>
  <rdakit:toolkitLabel xml:lang="vi">được rút gọn như là (tác phẩm)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="vi">Tác phẩm làm ngắn tác phẩm nguồn mà không thay đổi ý nghĩa hoặc phương cách trình bày.</rdakit:toolkitDefinition>
  <rdfs:label xml:lang="de">ist gekürzt als (Werk)</rdfs:label>
  <skos:definition xml:lang="de">Setzt ein Werk mit einem Werk, das ein ursprüngliches Werk ohne Änderung der allgemeinen Bedeutung oder Darstellungsweise kürzt, in Beziehung.</skos:definition>
  <rdakit:toolkitLabel xml:lang="de">Gekürzt als (Werk)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="de">Ein Werk, das ein ursprüngliches Werk ohne Änderung der allgemeinen Bedeutung oder Darstellungsweise kürzt.</rdakit:toolkitDefinition>
  <rdfs:label xml:lang="fr">est abrégé comme (œuvre)</rdfs:label>
  <reg:name xml:lang="fi">lyhennelmaTeos</reg:name>
  <rdfs:label xml:lang="fi">lyhennelmä (teos)</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/w/lyhennelmaTeos.fi" />
  <skos:definition xml:lang="fi">Liittää teoksen teokseen, joka on lyhennetty lähdeteoksesta muuttamatta sen merkityssisältöä tai esitystyyliä.</skos:definition>
  <rdakit:toolkitLabel xml:lang="fi">lyhennelmä (teos)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="fi">Teos, joka on lyhennetty lähdeteoksesta muuttamatta sen merkityssisältöä tai esitystyyliä.</rdakit:toolkitDefinition>
  <rdakit:toolkitLabel xml:lang="no">forkortet som (verk)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="no">Et verk som forkorter et kildeverk uten å endre den generelle meningen eller presentasjonsformen.</rdakit:toolkitDefinition>
  <rdfs:label xml:lang="ca">és abreujat com (obra)</rdfs:label>
  <skos:definition xml:lang="ca">Relaciona una obra amb una obra que escurça una obra font sense canviar-ne el significat general o la forma de presentació.</skos:definition>
  <rdakit:toolkitLabel xml:lang="ca">abreujat com (obra)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="ca">Obra que escurça una obra font sense canviar-ne el significat general o la forma de presentació.</rdakit:toolkitDefinition>
  <reg:name xml:lang="fr">abrégéCommeŒuvre</reg:name>
</rdf:Description>

<!-- Property: is abridgement of (work) -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/w/P10125">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/w/" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
  <reg:name xml:lang="en">abridgementOfWork</reg:name>
  <rdfs:label xml:lang="en">is abridgement of (work)</rdfs:label>
  <skos:definition xml:lang="en">Relates a work to a work that has been abridged, i.e., shortened without changing the general meaning or manner of presentation of a source work.</skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10001" />
  <rdfs:subPropertyOf rdf:resource="http://rdaregistry.info/Elements/w/P10190" />
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/w/abridgementOfWork.en" />
  <reg:hasUnconstrained rdf:resource="http://rdaregistry.info/Elements/u/P60223" />
  <rdakit:toolkitLabel xml:lang="en">abridgement of (work)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="en">A work that has been abridged, i.e., shortened without changing the general meaning or manner of presentation of a source work.</rdakit:toolkitDefinition>
  <rdakit:toolkitLabel xml:lang="fr">abrégé de (œuvre)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="fr">Œuvre qui a été abrégée, c’est-à-dire raccourcie sans changer le sens général ou le mode de présentation d’une œuvre source.</rdakit:toolkitDefinition>
  <rdakit:toolkitLabel xml:lang="es">abreviada de (obra)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="es">Una obra que ha sido abreviada, i.e., acortada sin cambiar el significado general o manera de presentación de la obra fuente.</rdakit:toolkitDefinition>
  <rdakit:instructionNumber xml:lang="">24.5</rdakit:instructionNumber>
  <reg:hasSubproperty rdf:resource="http://rdaregistry.info/Elements/w/object/P10125" />
  <reg:name xml:lang="da">forkortelseAfVærk</reg:name>
  <rdfs:label xml:lang="da">er forkortelse af (værk)</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/w/forkortelseAfVærk.da" />
  <skos:definition xml:lang="da">Relaterer et værk til et værk, der er blevet forkortet, dvs. afkortet, uden at der er ændret på et oprindeligt værks overordnede betydning eller præsentationsmåde.</skos:definition>
  <rdakit:toolkitLabel xml:lang="da">forkortelse af (værk)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="da">Værk, der er blevet forkortet, dvs. afkortet, uden at der er ændret på et oprindeligt værks overordnede betydning eller præsentationsmåde.</rdakit:toolkitDefinition>
  <skos:definition xml:lang="fr">Met une œuvre en relation avec une œuvre qui a été abrégée, c’est-à-dire raccourcie sans changer le sens général ou le mode de présentation d’une œuvre source.</skos:definition>
  <reg:name xml:lang="vi">rútGọnCủaTácPhẩm</reg:name>
  <rdfs:label xml:lang="vi">là rút gọn của (tác phẩm)</rdfs:label>
  <skos:definition xml:lang="vi">Liên kết tác phẩm với tác phẩm đã được rút gọn, nghĩa là, làm ngắn nhưng không thay đổi ý nghĩa và cách thức chung của trình bày của tác phẩm nguồn.</skos:definition>
  <rdakit:toolkitLabel xml:lang="vi">rút gọn của (tác phẩm)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="vi">Tác phẩm đã được rút gọn, nghĩa là, làm ngắn nhưng không thay đổi ý nghĩa và cách thức chung của trình bày của tác phẩm nguồn.</rdakit:toolkitDefinition>
  <rdfs:label xml:lang="de">ist Kurzfassung von (Werk)</rdfs:label>
  <skos:definition xml:lang="de">Setzt ein Werk mit einem Werk, das abgekürzt wurde, d. h. gekürzt, ohne die allgemeine Bedeutung oder die Darstellungsweise des ursprünglichen Werks zu verändern, in Beziehung.</skos:definition>
  <rdakit:toolkitLabel xml:lang="de">Kurzfassung von (Werk)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="de">Ein Werk, das abgekürzt wurde, d. h. gekürzt, ohne die allgemeine Bedeutung oder die Darstellungsweise des ursprünglichen Werks zu verändern.</rdakit:toolkitDefinition>
  <rdfs:label xml:lang="fr">est abrégé de (œuvre)</rdfs:label>
  <reg:name xml:lang="fi">lyhennelmanPerustanaTeos</reg:name>
  <rdfs:label xml:lang="fi">lyhennelmän perustana (teos)</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/w/lyhennelmanPerustanaTeos.fi" />
  <skos:definition xml:lang="fi">Liittää teoksen teokseen, jota on lyhennetty muuttamatta sen merkityssisältöä tai esitystapaa.</skos:definition>
  <rdakit:toolkitLabel xml:lang="fi">lyhennelmän perustana (teos)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="fi">Teos, jota on lyhennetty muuttamatta sen merkityssisältöä tai esitystapaa.</rdakit:toolkitDefinition>
  <rdakit:toolkitLabel xml:lang="no">forkortelse av (verk)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="no">Et verk som er forkortet uten å endre den generelle meningen eller presentasjonsmåten til et kildeverk.</rdakit:toolkitDefinition>
  <rdfs:label xml:lang="ca">és abreujament de (obra)</rdfs:label>
  <skos:definition xml:lang="ca">Relaciona una obra amb una obra que ha estat abreujada, és a dir, escurçada sense canviar el significat general o la forma de presentació d’una obra font.</skos:definition>
  <rdakit:toolkitLabel xml:lang="ca">abreujament de (obra)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="ca">Obra que ha estat abreujada, és a dir, escurçada sense canviar el significat general o la forma de presentació d’una obra font.</rdakit:toolkitDefinition>
  <reg:name xml:lang="fr">abrégéDeŒuvre</reg:name>
</rdf:Description>

<!-- Property: is absorbed by (work) -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/w/P10145">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/w/" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
  <reg:name xml:lang="en">absorbedByWork</reg:name>
  <rdfs:label xml:lang="en">is absorbed by (work)</rdfs:label>
  <skos:definition xml:lang="en">Relates a work to a work that incorporates another work.</skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10001" />
  <rdfs:subPropertyOf rdf:resource="http://rdaregistry.info/Elements/w/P10170" />
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/w/absorbedByWork.en" />
  <reg:hasUnconstrained rdf:resource="http://rdaregistry.info/Elements/u/P60247" />
  <rdakit:toolkitLabel xml:lang="en">absorbed by (work)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="en">A work that incorporates another work.</rdakit:toolkitDefinition>
  <rdakit:toolkitLabel xml:lang="fr">absorbé par (œuvre)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="fr">Œuvre qui intègre une autre œuvre.</rdakit:toolkitDefinition>
  <rdakit:toolkitLabel xml:lang="es">absorbida por (obra)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="es">Una obra que incorpora otra obra.</rdakit:toolkitDefinition>
  <rdakit:instructionNumber xml:lang="">24.5</rdakit:instructionNumber>
  <reg:hasSubproperty rdf:resource="http://rdaregistry.info/Elements/w/object/P10145" />
  <reg:name xml:lang="da">absorberetAfVærk</reg:name>
  <rdfs:label xml:lang="da">er absorberet af (værk)</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/w/absorberetAfVærk.da" />
  <skos:definition xml:lang="da">Relaterer et værk til et værk, der inkorporerer et andet værk.</skos:definition>
  <rdakit:toolkitLabel xml:lang="da">absorberet af (værk)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="da">Værk, der inkorporerer et andet værk.</rdakit:toolkitDefinition>
  <skos:definition xml:lang="fr">Met une œuvre en relation avec une œuvre qui intègre une autre œuvre.</skos:definition>
  <reg:name xml:lang="vi">đượcHấpThụBởiTácPhẩm</reg:name>
  <rdfs:label xml:lang="vi">được hấp thụ bởi (tác phẩm)</rdfs:label>
  <skos:definition xml:lang="vi">Liên kết tác phẩm với tác phẩm sáp nhập tác phẩm khác.</skos:definition>
  <rdakit:toolkitLabel xml:lang="vi">được hấp thụ bởi (tác phẩm)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="vi">Tác phẩm sáp nhập tác phẩm khác.</rdakit:toolkitDefinition>
  <rdfs:label xml:lang="de">ist aufgegangen in (Werk)</rdfs:label>
  <skos:definition xml:lang="de">Setzt ein Werk mit einem Werk, in das ein anderes Werk eingeflossen ist, in Beziehung.</skos:definition>
  <rdakit:toolkitLabel xml:lang="de">Aufgegangen in (Werk)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="de">Ein Werk, in das ein anderes Werk eingeflossen ist.</rdakit:toolkitDefinition>
  <rdfs:label xml:lang="fr">est absorbé par (œuvre)</rdfs:label>
  <reg:name xml:lang="fi">sulautunutTeokseen</reg:name>
  <rdfs:label xml:lang="fi">sulautunut teokseen</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/w/sulautunutTeokseen.fi" />
  <skos:definition xml:lang="fi">Liittää teoksen teokseen, joka on sulautunut osaksi toista teosta.</skos:definition>
  <rdakit:toolkitLabel xml:lang="fi">sulautunut teokseen</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="fi">Teos, joka on sulautunut osaksi toista teosta.</rdakit:toolkitDefinition>
  <rdakit:toolkitLabel xml:lang="no">innlemmet i (verk)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="no">Et verk som omfatter et annet verk.</rdakit:toolkitDefinition>
  <rdfs:label xml:lang="ca">és absorbit per (obra)</rdfs:label>
  <skos:definition xml:lang="ca">Relaciona una obra amb una obra que incorpora una altra obra.</skos:definition>
  <rdakit:toolkitLabel xml:lang="ca">absorbit per (obra)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="ca">Obra que incorpora una altra obra.</rdakit:toolkitDefinition>
  <reg:name xml:lang="fr">absorbéParŒuvre</reg:name>
</rdf:Description>

<!-- Property: is absorbed in part by (work) -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/w/P10146">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/w/" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
  <reg:name xml:lang="en">absorbedInPartByWork</reg:name>
  <rdfs:label xml:lang="en">is absorbed in part by (work)</rdfs:label>
  <skos:definition xml:lang="en">Relates a work to a work that incorporates part of the content of another work.</skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10001" />
  <rdfs:subPropertyOf rdf:resource="http://rdaregistry.info/Elements/w/P10170" />
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/w/absorbedInPartByWork.en" />
  <reg:hasUnconstrained rdf:resource="http://rdaregistry.info/Elements/u/P60248" />
  <rdakit:toolkitLabel xml:lang="en">absorbed in part by (work)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="en">A work that incorporates part of the content of another work.</rdakit:toolkitDefinition>
  <rdakit:toolkitLabel xml:lang="fr">absorbé en partie par (œuvre)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="fr">Œuvre qui intègre une partie du contenu d’une autre œuvre.</rdakit:toolkitDefinition>
  <rdakit:toolkitLabel xml:lang="es">absorbida en parte por (obra)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="es">Una obra que incorpora parte del contenido de otra obra.</rdakit:toolkitDefinition>
  <rdakit:instructionNumber xml:lang="">24.5</rdakit:instructionNumber>
  <reg:hasSubproperty rdf:resource="http://rdaregistry.info/Elements/w/object/P10146" />
  <reg:name xml:lang="da">delvisAbsorberetAfVærk</reg:name>
  <rdfs:label xml:lang="da">er delvis absorberet af (værk)</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/w/delvisAbsorberetAfVærk.da" />
  <skos:definition xml:lang="da">Relaterer et værk til et værk, der inkorporerer en del af et andet værks indhold.</skos:definition>
  <rdakit:toolkitLabel xml:lang="da">delvis absorberet af (værk)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="da">Værk, der inkorporerer en del af et andet værks indhold.</rdakit:toolkitDefinition>
  <skos:definition xml:lang="fr">Met une œuvre en relation avec une œuvre qui intègre une partie du contenu d’une autre œuvre.</skos:definition>
  <reg:name xml:lang="vi">đượcHấpThụMộtPhầnBởiTácPhẩm</reg:name>
  <rdfs:label xml:lang="vi">được hấp thụ một phần bởi (tác phẩm)</rdfs:label>
  <skos:definition xml:lang="vi">Liên kết tác phẩm với tác phẩm sáp nhập phần của nội dung của tác phẩm khác.</skos:definition>
  <rdakit:toolkitLabel xml:lang="vi">được hấp thụ một phần bởi t(tác phẩm)</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="vi">Tác phẩm sáp nhập phần của nội dung của tác phẩm khác.</rdakit:toolkitDefinition>
  <rdfs:label xml:lang="de">ist teilweise aufgegangen in (Werk)</rdfs:label>
  <skos:definition xml:lang="de">Setzt ein Werk mit einem Werk, in das ein Teil des Inhalts eines anderen Werks eingeflossen ist, in Beziehung.</skos:definition>
