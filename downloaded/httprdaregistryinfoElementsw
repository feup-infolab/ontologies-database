<!-- Property: has longitude -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/w/P10301">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/w/" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <reg:name xml:lang="en">longitude</reg:name>
  <rdfs:label xml:lang="en">has longitude</rdfs:label>
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/w/longitude.en" />
  <skos:definition xml:lang="en">Relates a work to a distance of a point on a planet or satellite measured east and west from a reference meridian.</skos:definition>
  <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10001" />
  <rdakit:toolkitLabel xml:lang="en">longitude</rdakit:toolkitLabel>
