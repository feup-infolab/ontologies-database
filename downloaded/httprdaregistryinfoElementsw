  <rdakit:toolkitDefinition xml:lang="de">Ein Werk, in das ein Teil des Inhalts eines anderen Werks eingeflossen ist.</rdakit:toolkitDefinition>
  <rdfs:label xml:lang="fr">est absorbé en partie par (œuvre)</rdfs:label>
  <reg:name xml:lang="fi">sulautunutOsittainTeokseen</reg:name>
  <rdfs:label xml:lang="fi">sulautunut osittain teokseen</rdfs:label>
