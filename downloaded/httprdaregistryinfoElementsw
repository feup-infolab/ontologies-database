  <skos:scopeNote xml:lang="en">Revision, content clarification, etc., in an expression of a work is excluded.</skos:scopeNote>
  <rdakit:seeAlso rdf:resource="http://rdaregistry.info/Elements/e/P20048" />
  <rdakit:instructionNumber xml:lang="">18.5</rdakit:instructionNumber>
