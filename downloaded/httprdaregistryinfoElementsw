<!-- Property: has jurisdiction governed -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/w/P10018">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/w/" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
  <reg:name xml:lang="en">jurisdictionGoverned</reg:name>
  <rdfs:label xml:lang="en">has jurisdiction governed</rdfs:label>
  <skos:definition xml:lang="en">Relates a work to a jurisdiction governed by a law, regulation, etc., that was enacted by another jurisdiction.</skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10001" />
  <rdfs:subPropertyOf rdf:resource="http://rdaregistry.info/Elements/w/P10047" />
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/w/jurisdictionGoverned.en" />
  <reg:hasUnconstrained rdf:resource="http://rdaregistry.info/Elements/u/P60097" />
  <rdakit:toolkitLabel xml:lang="en">jurisdiction governed</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="en">A jurisdiction governed by a law, regulation, etc., that was enacted by another jurisdiction.</rdakit:toolkitDefinition>
  <rdakit:toolkitLabel xml:lang="fr">juridiction régie</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="fr">Juridiction régie par une loi, un règlement, etc. qui a été promulgué par une autre juridiction.</rdakit:toolkitDefinition>
  <rdakit:toolkitLabel xml:lang="es">jurisdicción regulada</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="es">Una jurisdicción en la que se aplica una ley, disposición, etc., que fue promulgada por otra jurisdicción.</rdakit:toolkitDefinition>
  <rdakit:instructionNumber xml:lang="">18.5</rdakit:instructionNumber>
  <reg:hasSubproperty rdf:resource="http://rdaregistry.info/Elements/w/object/P10018" />
