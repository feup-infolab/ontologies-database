  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/w/declination.en" />
  <skos:definition xml:lang="en">Relates a work to an angular distance to a body on the celestial sphere measured north or south through 90° from the celestial equator along the hour circle of the body.</skos:definition>
  <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10001" />
  <rdakit:toolkitLabel xml:lang="en">declination</rdakit:toolkitLabel>
