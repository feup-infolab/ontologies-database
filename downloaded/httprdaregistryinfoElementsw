<!-- Property: has identifier for work -->
<rdf:Description rdf:about="http://rdaregistry.info/Elements/w/P10002">
  <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/w/" />
  <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
  <reg:name xml:lang="en">identifierForWork</reg:name>
  <rdfs:label xml:lang="en">has identifier for work</rdfs:label>
  <skos:definition xml:lang="en">Relates a work to a character string uniquely associated with a work, or with a surrogate such as an authority record for a work.</skos:definition>
  <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
  <skos:scopeNote xml:lang="en">The identifier serves to differentiate a work from other works.</skos:scopeNote>
  <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10001" />
  <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/w/identifierForWork.en" />
  <reg:hasUnconstrained rdf:resource="http://rdaregistry.info/Elements/u/P60052" />
  <rdakit:instructionNumber xml:lang="">6.8</rdakit:instructionNumber>
  <rdakit:toolkitLabel xml:lang="en">identifier for work</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="en">A character string uniquely associated with a work, or with a surrogate such as an authority record for a work.</rdakit:toolkitDefinition>
  <skos:scopeNote xml:lang="fr">L’identifiant sert à différencier une œuvre d’autres œuvres.</skos:scopeNote>
  <rdakit:toolkitLabel xml:lang="fr">identifiant d’œuvre</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="fr">Chaîne de caractères associée de manière unique à une œuvre ou à un substitut tel qu’une notice d’autorité pour une œuvre.</rdakit:toolkitDefinition>
  <rdakit:toolkitLabel xml:lang="es">identificador de la obra</rdakit:toolkitLabel>
  <rdakit:toolkitDefinition xml:lang="es">Una cadena de caracteres asociada con una obra, o un substituto de una obra (p.ej., un registro de autoridad).</rdakit:toolkitDefinition>
